            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  index(
    a!flatten(ri!cdt),
    #"_a-0000e70f-2824-8000-9c01-011c48011c48_2594918"(
      cdt: ri!cdt,
      fields: ri!fields,
      values: ri!values
    ),
    {}
  )
)</definition>
        <namedTypedValue>
