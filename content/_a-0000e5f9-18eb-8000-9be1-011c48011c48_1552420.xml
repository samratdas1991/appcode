    <rule>
        <name>AS_CO_UT_isObjectFieldsEmpty</name>
        <uuid>_a-0000e5f9-18eb-8000-9be1-011c48011c48_1552420</uuid>
        <description>Given a single object, returns true if is it blank or all of its fields are blank.  Can pass fields to exclude from the check (typically an ID field).</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>or(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(value: ri!object),
  if(
    #"7fe7c8a5-36b5-43e4-a4b5-12ac903c8462"(object: ri!object),
    error("Object must not be a list type"),
    all(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(value: _),
      #"SYSTEM_SYSRULES_forEach"(
        items: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_165064"(
          array: #"_a-0000e774-a792-8000-9c04-011c48011c48_2821463"(ri!object),
          values: if(
            ri!checkMetaDataFields,
            ri!fieldsToExclude,
            append(
              ri!fieldsToExclude,
              {
                "createdBy",
                "createdDatetime",
                "modifiedBy",
                "modifiedDatetime"
              }
            )
          )
        ),
        expression: ri!object[fv!item]
      )
    )
  )
)</definition>
