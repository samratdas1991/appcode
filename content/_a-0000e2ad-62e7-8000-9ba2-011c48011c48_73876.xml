                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">{3.14159, 1/6, 3.333333}</a:value>
                    <a:nameRef>number</a:nameRef>
