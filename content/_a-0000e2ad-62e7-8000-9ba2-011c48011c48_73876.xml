                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">true</a:value>
                    <a:nameRef>no_commas</a:nameRef>
                    <a:id>3</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
