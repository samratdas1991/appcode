<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e673-a4a7-8000-9bef-011c48011c48_2018801</versionUuid>
    <rule>
        <name>AS_CO_UT_fixed</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73876</uuid>
        <description>Executes fn!fixed() in a null-safe manner.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!number),
  null,
  fixed(ri!number, if(isnull(ri!decimals), 2, ri!decimals), if(isnull(ri!no_commas), false, ri!no_commas))
)</definition>
        <namedTypedValue>
            <name>number</name>
            <type>
                <name>Decimal?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>decimals</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>no_commas</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
