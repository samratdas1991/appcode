    <versionUuid>_a-0000e673-a4a7-8000-9bef-011c48011c48_2008493</versionUuid>
    <rule>
        <name>AS_IO_UT_disableTaskActions</name>
        <uuid>_a-0000e36c-7599-8000-9ba2-011c48011c48_64585</uuid>
        <description>Returns true (implying disabled) if task is not assigned to the logged in user or a group the logged in user belongs to</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>or(
  /*Check if all tasks are blank*/
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!tasks),
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!tasks,
    expression: if(
      /*First check that the task status is assigned*/
      fv!item.taskStatus.taskStatusId &lt;&gt; #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236",
      true,
      if(
        /*Then check if there is a default assignee on the task*/
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(fv!item.groupAssignee),
        /*Return false (not disabled) if EITHER the user is in the default assignee group OR the user is the individual assignee*/
        and(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(
            #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_114114"(
              user: loggedInUser(),
              groups: fv!item.groupAssignee
            )
          ),
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(fv!item.userAssignee = loggedInUser()),
          
        ),
        /*Return false (not disabled) if the user is the individual assignee*/
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(fv!item.userAssignee = loggedInUser())
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>tasks</name>
            <type>
                <name>AS_IO_Task?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
