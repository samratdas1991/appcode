    <rule>
        <name>AS_CO_UT_captureMetadataIfUpdated</name>
        <uuid>_a-0000e460-9d93-8000-9bc1-011c48011c48_619401</uuid>
        <description>Given two objects, performs a soft comparison on them (i.e. no type compare).  If the fields are all equal, does not update metadata, but otherwise does.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!updatedCdt),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73624"(
      value1: cast(
        typeof(ri!updatedCdt),
        ri!originalCdt
      ),
      value2: ri!updatedCdt
    ),
    ri!updatedCdt,
    #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_28158"(
      cdt: ri!updatedCdt,
      modifiedBy: ri!modifiedBy
    )
  )
)</definition>
        <namedTypedValue>
            <name>originalCdt</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>updatedCdt</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>modifiedBy</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
