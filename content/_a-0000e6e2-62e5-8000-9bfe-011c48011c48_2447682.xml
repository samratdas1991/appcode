    <versionUuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_2972384</versionUuid>
    <rule>
        <name>AS_CO_UI_showMoreShowLessLink</name>
        <uuid>_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2447682</uuid>
        <description>Rich text item with a link that will toggle a boolean and display "Show More" or "Show Less" (or alternate labels)</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_richTextItem_v1"(
  text: {
    #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
      i18nData: ri!i18nData,
      bundleKey: if(
        ri!isShowMore,
        a!defaultValue(
          ri!showLessLabel,
          "AS.CO.CommonObjects.txt_ShowLess"
        ),
        a!defaultValue(
          ri!showMoreLabel,
          "AS.CO.CommonObjects.txt_ShowMore"
        )
      )
    ),
    #"SYSTEM_SYSRULES_richTextIcon"(
      icon: ri!icon,
      showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!icon)
    )
  },
  link: #"SYSTEM_SYSRULES_dynamicLink"(
    value: not(ri!isShowMore),
    saveInto: { ri!isShowMore, ri!additionalSave }
  ),
  linkStyle: "STANDALONE",
  showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!isShowMore),
  size: ri!labelSize
)</definition>
        <namedTypedValue>
            <name>i18nData</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>isShowMore</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>showMoreLabel</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>showLessLabel</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>additionalSave</name>
            <type>
                <name>Save</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>icon</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>labelSize</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
