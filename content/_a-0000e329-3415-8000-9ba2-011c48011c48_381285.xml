    <rule>
        <name>AS_CO_UT_checkWebApiRequestForRequiredFields</name>
        <uuid>_a-0000e329-3415-8000-9ba2-011c48011c48_381285</uuid>
        <description>Checks for list of required fields, returns fields that are missing. </description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  
  /*Return a list of missing fields from the inputs*/
  local!missingFields: touniformstring({
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!qpRequiredFields,
      expression: if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(index(ri!queryParameters, fv!item, null)),
        #"_a-0000e329-3415-8000-9ba2-011c48011c48_386641"(text: fv!item, quote: "'"),
        {}
      )
    ),
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!bodyRequiredFields,
      expression: if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(cdt: ri!body, field: fv!item)),
        #"_a-0000e329-3415-8000-9ba2-011c48011c48_386641"(text: fv!item, quote: "'"),
        {}
      )
    )
  }),
  
  /*Return an error if any fields are missing*/
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(local!missingFields), 
    #"_a-0000e329-3415-8000-9ba2-011c48011c48_381240"(missingFields: local!missingFields),
    {}
  )
)</definition>
