    <rule>
        <name>AS_CO_UT_sortArray</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_87660</uuid>
        <description>Sorts an array of primitive types (e.g. text, integers, dates)</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!array),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!array),
    ri!array,
    todatasubset(
      #"SYSTEM_SYSRULES_forEach"(
        items: ri!array,
        expression: { item: fv!item }
      ),
      #"SYSTEM_SYSRULES_pagingInfo"(
        startIndex: 1,
        batchSize: - 1,
        sort: #"SYSTEM_SYSRULES_sortInfo"(
          field: "item",
          ascending: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
            nullableValue: ri!ascending,
            replacementValue: true
          )
        )
      )
    ).data.item
  )
)</definition>
        <namedTypedValue>
            <name>array</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>ascending</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
