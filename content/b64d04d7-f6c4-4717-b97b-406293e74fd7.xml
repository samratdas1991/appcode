                    <a:value xmlns:n1="urn:com:appian:types:AS:IO" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nil="true" xsi:type="n1:AS_IO_Task"/>
                    <a:nameRef>currentTask</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
