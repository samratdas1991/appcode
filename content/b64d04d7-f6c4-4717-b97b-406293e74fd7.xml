<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_823149</versionUuid>
    <rule>
        <name>AS_IO_BL_checkIfDueDateFallsAfterPrecedentsDueDate</name>
        <uuid>b64d04d7-f6c4-4717-b97b-406293e74fd7</uuid>
        <description>Returns true if the tasks due date is less than the precedent tasks due date</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
    ri!currentTask.taskPrecedents
  ),
  false,
  a!localVariables(
    local!precedentTasks: index(
      ri!taskForRequest,
      ri!currentTask.taskPrecedents.taskId_precedent,
      null
    ),
    or(
      #"SYSTEM_SYSRULES_forEach"(
        items: local!precedentTasks,
        expression: fv!item.dueDate &gt; ri!currentTask.dueDate
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>taskForRequest</name>
            <type>
                <name>AS_IO_Task?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>currentTask</name>
            <type>
                <name>AS_IO_Task</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
