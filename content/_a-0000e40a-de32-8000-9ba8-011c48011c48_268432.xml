            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  if(
    and(
      /*Have to handle differently for a dictionary/map list, or if the underlying field is an array*/
      or(
        #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
          value1: ri!cdt,
          value2: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_101176"()
        ),
        #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
          value1: ri!cdt,
          value2: {a!map()}
        ),
        and(
          /*Both the CDT to update and the field within the CDT are list types*/
          #"_a-0000e415-3035-8000-9bb2-011c48011c48_291952"(
            typeOf: ri!cdt
          ),
          #"_a-0000e415-3035-8000-9bb2-011c48011c48_291952"(
            typeOf: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75842"(
              typeOf: ri!cdt
            )[ri!field]
          )
        )
      ),
      #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
        value1: ri!field,
        /*Actual field type, not index*/
        value2: ""
      )
    ),
    apply(
      #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268359"(
        dicionary: ri!cdt,
        indices: a!defaultValue(
          value: ri!indices,
          default: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268381"(input: ri!cdt)
        ),
        field: ri!field,
        newValue: ri!newValue,
        index: _
      ),
      #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268381"(input: ri!cdt)
    ),
    #"_a-0000e416-3948-8000-9bb5-011c48011c48_292774"(
      cdt: ri!cdt,
      field: ri!field,
      indices: ri!indices,
      newValue: ri!newValue
    )
  )
)</definition>
