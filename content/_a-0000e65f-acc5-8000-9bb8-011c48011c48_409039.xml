    <versionUuid>_a-0000e7c7-bc72-8000-9c0e-011c48011c48_3102414</versionUuid>
    <rule>
        <name>AS_FRM_parseColorSchemeBrandingConfig</name>
        <uuid>_a-0000e65f-acc5-8000-9bb8-011c48011c48_409039</uuid>
        <description>Pass a branding config of type `COLOR_SCHEME` to this rule and it will return the value used by charts in the `colorScheme` parameter</description>
        <parentUuid>_a-0000e5d4-2024-8000-9baf-011c48011c48_320771</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    isnull(ri!colorSchemeValue),
    contains(#"_a-0000e65f-acc5-8000-9bb8-011c48011c48_409027"().key, ri!colorSchemeValue)
  ),
  
  /*Is a standard color scheme (e.g. MIDNIGHT)*/
  if(
    ri!returnRawHexCodes,
    a!flatten(displayvalue(
      ri!colorSchemeValue,
      #"_a-0000e65f-acc5-8000-9bb8-011c48011c48_409027"().key,
      #"_a-0000e65f-acc5-8000-9bb8-011c48011c48_409027"().hexCodes,
      {}
    )),
    ri!colorSchemeValue
  ),
  
  /*Is a custom color scheme*/
  if(
    ri!returnRawHexCodes,
    split(ri!colorSchemeValue, ","),
    #"SYSTEM_SYSRULES_colorSchemeCustom"(colors: split(ri!colorSchemeValue, ","))
  )
)</definition>
        <namedTypedValue>
            <name>colorSchemeValue</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>returnRawHexCodes</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
