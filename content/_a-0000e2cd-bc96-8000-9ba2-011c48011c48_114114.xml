<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e673-a4a7-8000-9bef-011c48011c48_2010924</versionUuid>
    <rule>
        <name>AS_CO_UT_isUserMemberOfGroups</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_114114</uuid>
        <description>Returns true if the user is a member of any of the groups (or condition), uses `loggedInUser()` if `user` input is null</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Boolean',
  with(
    local!userToUse: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
      nullableValue: ri!user,
      replacementValue: loggedInUser()
    ),
    if(
      or(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!groups),
        not(
          #"_a-0000e447-933d-8000-9baa-011c48011c48_43946"(user: local!userToUse)
        )
      ),
      false,
      #"urn:appian:function:v1:a:isusermemberofgroup?okey=a!isUserMemberOfGroup"(username: local!userToUse, groups: ri!groups)
    )
  )
)</definition>
        <namedTypedValue>
            <name>groups</name>
            <type>
                <name>Group?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>user</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
