    <rule>
        <name>AS_FS_IDP_QPA_isProcessActiveForProcessModelsAndChannelId</name>
        <uuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1284250</uuid>
        <description>QPA for checking Process Active For ProcessModels And ChannelId</description>
        <parentUuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1261631</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!queryProcessAnalytics(
  report: #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1272804",
  contextProcessModels: ri!processModels,
  query: #"SYSTEM_SYSRULES_query"(
    logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "AND",
      filters: {
        /*Exclues 1 - Completed and 3 - Canceled process statuses*/
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "c1",
          operator: "not in",
          value: { 1, 3 }
        ),
        /*Must match given channel ID*/
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "c3",
          operator: "=",
          value: ri!channelId
        )
      },
      ignoreFiltersWithEmptyValues: true
    ),
    pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(startIndex: 1, batchSize: - 1)
  )
).totalCount &gt; 0</definition>
        <namedTypedValue>
            <name>processModels</name>
            <type>
                <name>ProcessModel?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>channelId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
