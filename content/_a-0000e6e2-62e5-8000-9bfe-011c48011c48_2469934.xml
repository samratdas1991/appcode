            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!index: choose(
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_280802"(
      value: ri!displayType,
      inArray: {
        #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2469960",
        #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2469966"
      }
    ),
    /*Default to plaintext*/
    "relevantUniqueRecordIdentifiersForTextDisplay",
    "relevantUniqueRecordIdentifiersForTextDisplay",
    "relevantRecordLinks"
  ),
  local!castType: choose(
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_280802"(
      value: ri!displayType,
      inArray: {
        #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2469960",
        #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2469966"
      }
    ),
    /*Default to plaintext*/
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_282990"(),
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_282990"(),
    typeof({ #"SYSTEM_SYSRULES_dynamicLink"() })
  ),
  local!fieldUpdateMetadata: if(
    ri!applyFieldFilter,
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
      cdt: ri!pendingObjectDisplay.pendingFieldUpdateDisplayMetadata,
      field: "field",
      value: ri!field
    ),
    ri!pendingObjectDisplay.pendingFieldUpdateDisplayMetadata
  ),
  cast(
    local!castType,
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73758"(
      value: a!flatten(
        apply(
          index(_, local!index, {}),
          local!fieldUpdateMetadata          
        )
      )
    )
  )
)</definition>
