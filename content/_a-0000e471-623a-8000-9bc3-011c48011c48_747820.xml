    <rule>
        <name>AS_IO_VLD_processSetupTasksDueDate</name>
        <uuid>_a-0000e471-623a-8000-9bc3-011c48011c48_747820</uuid>
        <description>Validation for the due dates of the process setup tasks.</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>{
  if(
    ri!currentTask.dueDate &lt; today(),
    #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
      i18nData: ri!i18nData,
      bundleKey: "AS.CO.AllBundles.vld_PastDueDates"
    ),
    ""
  )
}</definition>
