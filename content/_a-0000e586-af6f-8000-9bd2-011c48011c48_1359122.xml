        <name>AS_IO_UT_taskGroupAssigneeSuggestFunction</name>
        <uuid>_a-0000e586-af6f-8000-9bd2-011c48011c48_1359122</uuid>
        <description>Group list for a custom picker</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!groupList: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_165064"(
    array: #"_a-0000e586-af6f-8000-9bd2-011c48011c48_1349340"(customerGroupId: ri!customerGroupId),
    values: #"_a-0000e6ba-d6dd-8000-9bf5-011c48011c48_2180656"()
  ),
  local!groupData: #"SYSTEM_SYSRULES_forEach"(
    items: tointeger(local!groupList),
    expression: a!map(
      groupId: fv!item,
      groupName: group(fv!item, "groupName"),
      groupDesc: group(fv!item, "description")
    )
  ),
  local!searchData: #"SYSTEM_SYSRULES_forEach"(
    items: local!groupData,
    expression: if(
      or(
        search(ri!searchText, fv!item.groupName) &gt; 0,
        search(ri!searchText, fv!item.groupDesc) &gt; 0
      ),
      fv!item,
      {}
    )
  ),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!searchData),
    #"SYSTEM_SYSRULES_dataSubset"(data: null, identifiers: null),
    #"SYSTEM_SYSRULES_dataSubset"(
      data: local!searchData.groupName,
      identifiers: local!searchData.groupId
    )
  )
)</definition>
