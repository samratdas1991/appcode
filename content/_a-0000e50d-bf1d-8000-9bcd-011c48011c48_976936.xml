        <definition>a!localVariables(
  local!dbCountriesData: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157870"(
    entity: #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_977458",
    entityType: 'type!{urn:com:appian:types:AS:FS}AS_FS_R_Country',
    filters: {
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "countryCode",
        operator: "in",
        value: ri!countryCodes
      ),
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "regionCode",
        operator: "in",
        value: ri!regionCodes
      ),
      #"_a-0000e5a0-53e9-8000-9bd8-011c48011c48_1449750"(
        isActive: ri!isActive
      )
    },
    aggregation: ri!aggregation,
    executeWhen: ri!executeWhen,
    returnType: ri!returnType
  ),
  local!countries: #"SYSTEM_SYSRULES_forEach"(
    items: local!dbCountriesData,
    expression: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268479"(
      cdt: fv!item,
      fields: "countryName",
      newValues: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: fv!item.countryName
      )
    )
  ),
  local!countriesSortedAlphabetically: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!aggregation),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75720"(
      cdt: local!countries,
      field: "countryName",
      ascending: true()
    ),
    local!countries
  ),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!countryNames),
    local!countriesSortedAlphabetically,
    #"SYSTEM_SYSRULES_forEach"(
      items: local!countriesSortedAlphabetically,
      expression: if(
        search(ri!countryNames, fv!item.countryName) &gt; 0,
        fv!item,
        {}
      )
    )
  )
)</definition>
