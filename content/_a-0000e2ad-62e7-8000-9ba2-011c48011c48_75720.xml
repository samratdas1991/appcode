    <rule>
        <name>AS_CO_UT_sortCdt</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75720</uuid>
        <description>Sorts a CDT, dictionary, or complex type.  Pass nullsFirst to explicitly sort nulls first or last.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!field),
    ri!cdt,
    with(
      local!dataSortedByField: cast(
        typeof(ri!cdt),
        todatasubset(
          cast(#"_a-0000e607-97fd-8000-9be9-011c48011c48_1594268"(), ri!cdt),
          #"SYSTEM_SYSRULES_pagingInfo"(
            startIndex: 1,
            batchSize: - 1,
            sort: #"SYSTEM_SYSRULES_sortInfo"(
              field: ri!field,
              ascending: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
                nullableValue: ri!ascending,
                replacementValue: true
              )
            )
          )
        ).data
      ),
      if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!nullsFirst),
        local!dataSortedByField,
        with(
          local!cdtsWithNullFieldValue: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
            cdt: local!dataSortedByField,
            field: ri!field,
            value: null
          ),
          local!cdtsWithNonNullFieldValue: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
            cdt: local!dataSortedByField,
            field: ri!field,
            value: null
          ),
          if(
            ri!nullsFirst,
            append(
              local!cdtsWithNullFieldValue,
              local!cdtsWithNonNullFieldValue
            ),
            append(
              local!cdtsWithNonNullFieldValue,
              local!cdtsWithNullFieldValue
            )
          )
        )
      )
    )
  )
)</definition>
