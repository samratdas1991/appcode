        </visibility>
        <definition>if(
  ri!readOnly,
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74131"(
    label: ri!label,
    instructions: ri!instructions,
    value: ri!value,
    labelPosition: ri!labelPosition,
    helpTooltip: ri!helpTooltip,
    showWhen: ri!showWhen,
    accessibilityText: ri!accessibilityText,
    align: null,
    showHyphenIfValueNull: ri!showHyphenIfValueNull
  ),
  #"SYSTEM_SYSRULES_paragraphField"(
    label: ri!label,
    labelPosition: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73568"(
      label: ri!label,
      labelPosition: ri!labelPosition,
      readOnly: false
    ),
    height: ri!height,
    instructions: ri!instructions,
    helpTooltip: ri!helpTooltip,
    placeholder: ri!placeholder,
    value: ri!value,
    /* We are using nested saveIntos to automatically perform a trim on any value */
    /* before executing the actual saves */
    /* Saves can be nested in this format to allow for better reusability */
    saveInto: a!save(ri!saveInto, trim(save!value)),
    showWhen: ri!showWhen,
    required: if(ri!readOnly, false, ri!required),
    requiredMessage: ri!requiredMessage,
    readOnly: ri!readOnly,
    disabled: ri!disabled,
    validations: if(
      ri!readOnly,
      {},
      a!flatten({ ri!validations })
    ),
    validationGroup: ri!validationGroup,
    accessibilitytext: ri!accessibilityText,
    refreshAfter: ri!refreshAfter,
    characterLimit: ri!maxLength,
    /*By default show character count unless showCharacterCount is set to false*/
    /*By default show character count on validation even if showCharCount is false*/
    /*unless showCharacterCountOnValidation is set to false*/
    showCharacterCount: if(
      len(ri!value) &gt; ri!maxLength,
      ri!showCharacterCountOnValidation,
      ri!showCharacterCount
    )
  )
)</definition>
