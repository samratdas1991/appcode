            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Integer?list',
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!input),
    {},
    enumerate(count(a!flatten(ri!input))) + 1
  )
)</definition>
