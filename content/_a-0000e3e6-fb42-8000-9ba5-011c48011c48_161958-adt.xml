    <rule>
        <name>AS_ADT_UT_determineAuditActionCode</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_161958-adt</uuid>
        <description>Given a newObject, an oldObject, and the auditConfig, determines the auditActionCode</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-adt</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>choose(
  where(
    {
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(index(ri!originalObject, ri!auditConfig.idField, null)),
      or(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(index(ri!newObject, ri!auditConfig.idField, null)),
        toboolean(index(ri!newObject, "isDeleted", false))
      ),
      toboolean(index(ri!originalObject, "isActive", 1)) = toboolean(index(ri!newObject, "isActive", 1)),
      toboolean(index(ri!originalObject, "isActive", false)),
      toboolean(index(ri!newObject, "isActive", true))
    }
  )[1],
  #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_181584-adt",
  #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_181590-adt",
  #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_181596-adt",
  #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_181602-adt",
  #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_181608-adt"
)</definition>
        <namedTypedValue>
            <name>originalObject</name>
            <type>
                <name>Variant</name>
