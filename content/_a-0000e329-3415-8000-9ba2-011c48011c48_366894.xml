    <rule>
        <name>AS_CO_UT_getWebApiVersionForChoose</name>
        <uuid>_a-0000e329-3415-8000-9ba2-011c48011c48_366894</uuid>
        <description>Returns one number higher than the Web API version number from the path, returns 1 if the version is invalid</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!versionNumber: cast(
    typeof(1),
    index(ri!request.pathSegments, 1, null)
  ),
  
  if(
    contains(ri!validVersions, local!versionNumber),
    local!versionNumber + 1,
    1
  )
)</definition>
        <namedTypedValue>
            <name>request</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>validVersions</name>
            <type>
                <name>Integer?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
