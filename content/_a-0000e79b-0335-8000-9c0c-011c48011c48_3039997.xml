    <rule>
        <name>AS_IO_BL_constructTasksForRequiredDocuments</name>
        <uuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_3039997</uuid>
        <description>To construct CDT for task for a required document</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!onboardingRequest: #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2501604"(
    castToCdtType: 'type!{urn:com:appian:types:AS:IO}AS_IO_OnboardingRequest',
    returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
    requestId: ri!requestId
  ),
  local!dueDateCalculationUnit: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: local!onboardingRequest.slaDayType,
    replacementValue: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620378"
  ),
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!requiredDocuments,
    expression: if(
      or(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
          value: fv!item.onboardingDocument.customerDocument.appianDocId
        ),
        ri!isReuploadTask
      ),
      a!localVariables(
        local!dueDate: if(
          local!dueDateCalculationUnit = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620378",
          workday(
            today(),
            #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3039343"
          ),
          /*default is calendar days*/
          today() + 7
        ),
        /*While constructing upload tasks for rejected required doc types, the review task's id is stored as precedent to the reupload task*/
        local!precedentId: if(
          ri!isReuploadTask,
          index(
            ri!precedentTasks,
            wherecontains(
              tointeger(fv!item.requiredOnboardingDocumentId),
              tointeger(
                ri!precedentTasks.requiredDocument.requiredOnboardingDocumentId
              )
            ),
            null
          ).taskId,
          null
        ),
        'type!{urn:com:appian:types:AS:IO}AS_IO_Task'(
          taskId: null,
          taskPrecedents: if(
            ri!isReuploadTask = true(),
            'type!{urn:com:appian:types:AS:IO}AS_IO_Task_Precedent'(taskId_precedent: local!precedentId),
            null
          ),
          request: local!onboardingRequest,
          taskRefId: #"_a-0000e7fa-f829-8000-9c17-011c48011c48_3480080",
          taskName: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: ri!i18nData,
            bundleKey: "AS.CO.AllBundles.lbl_AttachRequiredDocument",
            arguments: {#"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
              i18nData: ri!i18nData,
              bundleKey: fv!item.requirementName
            ), " "}
          ),
          taskBehaviorType: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_108080"(
            behaviorSubtypeCode: #"_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_929082",
            configurationLevelCode: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_99892"
          ),
          taskCategory: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_100474"(
            returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
            taskCategoryId: #"_a-0000e7d2-6286-8000-9c11-011c48011c48_3119705",
            includeNotActive: true
          ),
          /*avoid due date in the past*/
          dueDate: local!dueDate,
          slaDays: #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3039343",
          slaRule: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620337",
          groupAssignee: if(
            ri!isUploadLater,
            null,
            #"_a-0000e438-fd8a-8000-9bbe-011c48011c48_411694"
          ),
          requiredDocument: fv!item,
          taskStatus: #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3006672"(
            returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
            taskStatusIds: ri!taskStatusId
          )
        )
      ),
      {}
    )
  )
)</definition>
