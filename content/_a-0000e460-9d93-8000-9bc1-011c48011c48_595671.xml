    <rule>
        <name>AS_QNM_UT_validate</name>
        <uuid>_a-0000e460-9d93-8000-9bc1-011c48011c48_595671</uuid>
        <description>Validates a Question based upon its responses and Question Type</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!fieldTypes: a!refreshVariable(
    value: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_595621"(
      questionTypeCode: ri!question.questionTypeCode
    ),
    refreshAlways: true
  ),
  if(
    contains(
      local!fieldTypes.fieldKey,
      ri!fieldKey
    ),
    a!localVariables(
      local!validate: a!refreshVariable(
        value: local!fieldTypes.validate[wherecontains(
          ri!fieldKey, local!fieldTypes.fieldKey
        )[1]],
        refreshAlways: true
      ),
      local!validate(
        question: ri!question,
        fieldKey: ri!fieldKey
      )
    ),
    error(
      concat(
        "Question (ID:",
        ri!question.questionId,
        ") does not have a field with fieldKey ",
        ri!fieldKey,
        "."
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>question</name>
            <type>
                <name>AS_QNM_Question</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fieldKey</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
