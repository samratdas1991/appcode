    <versionUuid>_a-0000e811-1167-8000-9c18-011c48011c48_3521489</versionUuid>
    <rule>
        <name>AS_IO_CDT_cloneTemplate_Tasks</name>
        <uuid>_a-0000e7e5-dad4-8000-9c14-011c48011c48_3255952</uuid>
        <description>Rule to clone the tasks in a template</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!clonedTaskMaps: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!tasksToClone),
    {},
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!tasksToClone,
      expression: a!map(
        originalTask: fv!item,
        newTask: 'type!{urn:com:appian:types:AS:IO}AS_IO_R_TemplateTask'(
          templateTaskId: null,
          templateId: null,
          taskRef: fv!item.taskRef,
          taskName: fv!item.taskRef.taskName,
          orderNumber: fv!item.orderNumber,
          groupAssignee: fv!item.groupAssignee,
          taskDesc: fv!item.taskDesc,
          slaDays: fv!item.slaDays,
          slaRule: fv!item.slaRule,
          /*  Find the new required document UUID  */
          requiredDocumentTemplateUUID: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
            cdt: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
              cdt: ri!clonedRequiredDocumentMaps,
              field: "originalRequiredDocument.requiredDocumentTemplateUUID",
              value: fv!item.requiredDocumentTemplateUUID
            ),
            field: "[1]newRequiredDocument.requiredDocumentTemplateUUID"
          ),
          templateTaskUUID: #"_a-0000e7e5-dad4-8000-9c14-011c48011c48_3308181"(),
          templateTaskPrecedents: fv!item.templateTaskPrecedents
        )
      )
    )
  ),
  #"SYSTEM_SYSRULES_forEach"(
    items: local!clonedTaskMaps.newTask,
    expression: #"urn:appian:function:v1:a:update"(
      data: fv!item,
      index: "templateTaskPrecedents",
      value: #"_a-0000e7e5-dad4-8000-9c14-011c48011c48_3256281"(
        taskPrecedentsToClone: fv!item.templateTaskPrecedents,
        clonedTaskMaps: local!clonedTaskMaps
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>tasksToClone</name>
            <type>
                <name>AS_IO_R_TemplateTask?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>clonedRequiredDocumentMaps</name>
            <type>
                <name>Map?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
