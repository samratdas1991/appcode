    <versionUuid>_a-0000e673-a4a7-8000-9bef-011c48011c48_2016930</versionUuid>
    <rule>
        <name>AS_FRM_solutionVersionConstantParser</name>
        <uuid>ca3dc866-cbf6-4df4-95a9-60a67847a13c</uuid>
        <description>Parses the standard format for solution's version naming for display on front-end pages</description>
        <parentUuid>_a-0000e5d4-2024-8000-9baf-011c48011c48_320771</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!split: tointeger(split(ri!versionConstant, ".")),
  a!map(
    platformMajor: local!split[1],
    platformMinor: local!split[2],
    solutionMajor: local!split[3],
    solutionMinor: local!split[4],
    solutionHotfix: local!split[5]
  )
)
</definition>
        <namedTypedValue>
            <name>versionConstant</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
