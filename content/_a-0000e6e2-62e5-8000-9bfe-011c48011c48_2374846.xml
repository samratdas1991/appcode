        <definition>if(
  or(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!list),
    length(ri!list) = 1
  ),
  ri!list,
  with(
    local!allPositions: enumerate(length(ri!list)) + 1,
    local!firstHalfPositions: enumerate(ceiling(length(ri!list) / 2)) + 1,
    local!secondHalfPositions: difference(
      local!allPositions,
      local!firstHalfPositions
    ),
    local!firstHalfSorted: #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2374846"(
      list: ri!list[local!firstHalfPositions],
      fieldToCompare: ri!fieldToCompare,
      ascending: ri!ascending
    ),
    local!secondHalfSorted: #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2374846"(
      list: ri!list[local!secondHalfPositions],
      fieldToCompare: ri!fieldToCompare,
      ascending: ri!ascending
    ),
    #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2374822"(
      listOne: local!firstHalfSorted,
      listTwo: local!secondHalfSorted,
      fieldToCompare: ri!fieldToCompare,
      ascending: ri!ascending
    )
  )
)</definition>
