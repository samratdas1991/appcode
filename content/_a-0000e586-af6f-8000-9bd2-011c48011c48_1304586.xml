  ),
  #"SYSTEM_SYSRULES_ButtonArrayLayout"(
    showWhen: ri!showWhen,
    align: "START",
    buttons: #"SYSTEM_SYSRULES_ButtonWidget"(
      icon: "REFRESH",
      style: ri!style,
      disabled: local!disable,
      tooltip: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: if(
          len(ri!customer.duns) &lt;&gt; 9,
          "AS.EI.DNB.Integrations.lbl_ProvideADunsNumberToExecuteUBORefresh",
          if(
