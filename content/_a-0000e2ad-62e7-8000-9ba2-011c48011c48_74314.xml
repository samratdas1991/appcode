        <name>AS_CO_CP_floatingPointField</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74314</uuid>
        <description>Generic floating point field to handle read-only logic dynamically. </description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  ri!readOnly,
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74131"(
    label: ri!label,
    instructions: ri!instructions,
    value: if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!formattedInput),
      if(
        ri!noFormat,
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73876"(
          number: ri!value,
          decimals: if(
            isnull(ri!numberOfDecimals),
            10,
            ri!numberOfDecimals
          ),
          no_commas: true
        ),
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73876"(
          number: ri!value,
          decimals: if(
            isnull(ri!numberOfDecimals),
            2,
            ri!numberOfDecimals
          ),
          no_commas: false
        )
      ),
      ri!formattedInput
    ) &amp; if(isnull(ri!value), "", ri!textToAppend),
    labelPosition: ri!labelPosition,
    helpTooltip: ri!helpTooltip,
    showWhen: ri!showWhen,
    accessibilityText: ri!accessibilityText
  ),
  #"SYSTEM_SYSRULES_floatingPointField"(
    label: ri!label &amp; ri!textToAppend,
    labelPosition: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73568"(
      label: ri!label,
      labelPosition: ri!labelPosition,
      readOnly: ri!readOnly
    ),
    instructions: ri!instructions,
    helpTooltip: ri!helpTooltip,
    placeholder: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74260"(placeholder: ri!placeholder),
    value: ri!value,
    saveInto: ri!saveInto,
    refreshAfter: ri!refreshAfter,
    showWhen: if(isnull(ri!showWhen), true, ri!showWhen),
    required: if(ri!readOnly, false, ri!required),
    requiredMessage: ri!requiredMessage,
    readOnly: ri!readOnly,
    disabled: ri!disabled,
    validations: if(
      ri!readOnly,
      {},
      {
        ri!validations,
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73956"(
          value: ri!value,
          minSize: ri!minSize,
          /*Null values not yet parameterized in this rule, but could certainly be set up*/
          minSizeMessage: null,
          maxSize: ri!maxSize,
          maxSizeMessage: null,
          required: ri!required,
          requiredMessage: ri!requiredMessage,
          infiniteMessage: null
        )
      }
    ),
    validationGroup: ri!validationGroup,
    align: ri!align,
    accessibilityText: ri!accessibilityText
  )
)</definition>
