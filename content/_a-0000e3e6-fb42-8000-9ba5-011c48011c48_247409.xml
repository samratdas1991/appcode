    <interface>
        <name>AS_CO_DSP_zINTERNAL_styledBanner</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_247409</uuid>
        <description>Card to display a variety of styled messages.  Based off of the predefined Appian alert banner designs found in the patterns tab of the interface design mode palette.  Should be wrapped in other rules for front-end display (e.g. AS_CO_DSP_warningBanner) to set consistent style patterns throughout the application.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_cardLayout"(
  contents: {
    #"SYSTEM_SYSRULES_sideBySideLayout"(
      items: {
        #"SYSTEM_SYSRULES_sideBySideItem"(
          item: #"SYSTEM_SYSRULES_richTextDisplayField"(
            labelPosition: "COLLAPSED",
            value: {
              #"SYSTEM_SYSRULES_richTextIcon"(
                icon: ri!icon,
                color: ri!iconColor,
                size: "MEDIUM"
              )
            }
          ),
          width: "MINIMIZE"
        ),
        #"SYSTEM_SYSRULES_sideBySideItem"(
          item: #"SYSTEM_SYSRULES_richTextDisplayField"(
            labelPosition: "COLLAPSED",
            value: {
              #"SYSTEM_SYSRULES_richTextItem_v1"(
                text: ri!label,
                style: "STRONG"
              ),
              " ",
              #"SYSTEM_SYSRULES_richTextItem_v1"(
                text: ri!secondaryLabel,
                style: "PLAIN",
                showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
                  ri!secondaryLabel
                )
              ),
              if(#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!additionalRichText),
              ri!additionalRichText,
              "")
              /* Could support a dynamic informational link, as is done in the Appian pallette alerts pattern */
              /*a!richTextItem(*/
                /*text: "Download error logs",*/
                /*link: a!dynamicLink()*/
              /*)*/
            }
          )
        ),
        #"SYSTEM_SYSRULES_sideBySideItem"(
          item: #"SYSTEM_SYSRULES_richTextDisplayField"(
            labelPosition: "COLLAPSED",
            value: #"SYSTEM_SYSRULES_richTextIcon"(
              icon: "times",
              link: #"SYSTEM_SYSRULES_dynamicLink"(
                saveInto: ri!acknowledgementSaveInto
              ),
              linkStyle: "STANDALONE",
              size: "MEDIUM",
              showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
                ri!acknowledgementSaveInto
              )
            ),
            align: "RIGHT"
          ),
          width: "MINIMIZE"
        )
        /* Could also support a button option here, as is done in the Appian pallette alerts pattern */
      },
      alignVertical: "MIDDLE",
      spacing: "STANDARD"
    )
  },
  showWhen: ri!showWhen,
  style: ri!style,
  marginBelow: "STANDARD",
  accessibilityText: ri!accessibilityText
)</definition>
