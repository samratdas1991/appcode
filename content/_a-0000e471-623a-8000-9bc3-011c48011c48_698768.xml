<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_816804</versionUuid>
    <rule>
        <name>AS_IO_BL_isEstFundingDateAtRiskByTask</name>
        <uuid>_a-0000e471-623a-8000-9bc3-011c48011c48_698768</uuid>
        <description>determines if a given task puts est funding date at risk by either being before today or after the est funding date if the task needs to be completed before funding</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  /*In case request is not passed*/
  local!estFundingDate: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!request.fundingDetails.estFundingDate,
    replacementValue: ri!task.request.fundingDetails.estFundingDate
  ),
  or(
    and(
      ri!task.dueDate&gt;local!estFundingDate, 
      ri!task.slaRule = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620337"
    ),
    ri!task.dueDate&lt;today()
  )   
)</definition>
        <namedTypedValue>
            <name>request</name>
            <type>
                <name>AS_IO_OnboardingRequest</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>task</name>
            <type>
                <name>AS_IO_Task</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="administrators">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyReaders">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAuthors">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAdministrators">
            <users/>
            <groups/>
        </role>
    </roleMap>
    <typedValue>
        <type>
            <name>RuleTestConfig?list</name>
            <namespace>http://www.appian.com/ae/types/2009</namespace>
        </type>
        <value>
            <el>
                <a:name>task slaRule is before funding, but due date is after est funding date</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">'type!{urn:com:appian:types:AS:IO}AS_IO_OnboardingRequest'(
  fundingDetails: 
  {estFundingDate: today()}
)</a:value>
                    <a:nameRef>request</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">'type!{urn:com:appian:types:AS:IO}AS_IO_Task'(
  slaRule: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620337",
  dueDate: today()+1
)</a:value>
                    <a:nameRef>task</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
                    <a:expectedOutput>
                        <a:type a:uuid="{http://www.appian.com/ae/types/2009}Boolean"/>
                        <a:runtimeType a:uuid="{http://www.appian.com/ae/types/2009}Boolean"/>
                        <a:value xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xsd:boolean">true</a:value>
                    </a:expectedOutput>
                </a:assertions>
            </el>
            <el>
                <a:name>Task slaRule is before funding and due date is not after est funding date</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">'type!{urn:com:appian:types:AS:IO}AS_IO_OnboardingRequest'(
  fundingDetails: 
  {estFundingDate: today()}
)</a:value>
                    <a:nameRef>request</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">'type!{urn:com:appian:types:AS:IO}AS_IO_Task'(
  slaRule: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620337",
  dueDate: today()
)</a:value>
                    <a:nameRef>task</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
                    <a:expectedOutput>
                        <a:type a:uuid="{http://www.appian.com/ae/types/2009}Boolean"/>
                        <a:runtimeType a:uuid="{http://www.appian.com/ae/types/2009}Boolean"/>
                        <a:value xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xsd:boolean">false</a:value>
                    </a:expectedOutput>
                </a:assertions>
            </el>
            <el>
                <a:name>Task slaRule is not before funding but due date is in the past</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">'type!{urn:com:appian:types:AS:IO}AS_IO_OnboardingRequest'(
  fundingDetails: 
  {estFundingDate: today()}
)</a:value>
                    <a:nameRef>request</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">'type!{urn:com:appian:types:AS:IO}AS_IO_Task'(
  dueDate: today()-1
)</a:value>
                    <a:nameRef>task</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
                    <a:expectedOutput>
                        <a:type a:uuid="{http://www.appian.com/ae/types/2009}Boolean"/>
                        <a:runtimeType a:uuid="{http://www.appian.com/ae/types/2009}Boolean"/>
                        <a:value xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xsd:boolean">true</a:value>
                    </a:expectedOutput>
                </a:assertions>
            </el>
            <el>
                <a:name>slaRule is not before est funding date and due date is not in the past</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">'type!{urn:com:appian:types:AS:IO}AS_IO_OnboardingRequest'(
  fundingDetails: 
  {estFundingDate: today()+1}
)</a:value>
                    <a:nameRef>request</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">'type!{urn:com:appian:types:AS:IO}AS_IO_Task'(
  dueDate: today()
)</a:value>
                    <a:nameRef>task</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
                    <a:expectedOutput>
                        <a:type a:uuid="{http://www.appian.com/ae/types/2009}Boolean"/>
                        <a:runtimeType a:uuid="{http://www.appian.com/ae/types/2009}Boolean"/>
                        <a:value xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xsd:boolean">false</a:value>
                    </a:expectedOutput>
                </a:assertions>
            </el>
        </value>
    </typedValue>
