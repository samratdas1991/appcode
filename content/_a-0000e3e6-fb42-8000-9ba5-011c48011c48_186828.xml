    <rule>
        <name>AS_IO_BL_initiateTaskVisibility</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_186828</uuid>
        <description>return true if user can initiate queued tasks</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>and(
  /*security*/
  #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_114114"(
    user: loggedInUser(),
    groups: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_186227"
  ),
  /*onboarding is active*/
  contains(
    #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1209491"(),
    ri!onboardingStatus
  ),
  /*task is queued or assigned*/
  or(
    ri!taskStatus = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603230",
    ri!taskStatus = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236"
  ),
  /*task is not related to required documents*/
  ri!taskCategory &lt;&gt; #"_a-0000e7d2-6286-8000-9c11-011c48011c48_3119705",
  /*task is not already available*/
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!availableDatetime)
)</definition>
        <namedTypedValue>
            <name>onboardingStatus</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>taskStatus</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>taskCategory</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>availableDatetime</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
