    <rule>
        <name>AS_CO_I18N_UT_loadBundleFromFolder</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81720</uuid>
        <description>Loads a property bundle file by name from a folder, returns a dictionary of the key-value pairs</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Dictionary',
  a!localVariables(
    local!locale: if(
      ri!localeAgnostic,
      null,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!localeOverride,
        replacementValue: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76004"(user: loggedInUser())
      )
    ),
    if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!bundleFolder),
      null,
      a!localVariables(
        /*Wrapping this in a local variable to ensure caching -- it will only re-execute if inputs change*/
        local!bundleData: a!refreshVariable(
          value: loadbundlepropertiesfile(
            ri!bundleFolder,
            local!locale,
            a!flatten(ri!bundleNames)
          ),
          refreshOnVarChange: { ri!triggerRefresh }
        ),
        local!bundleData
      )
    )
  )
)</definition>
