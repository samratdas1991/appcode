    entityType: 'type!{urn:com:appian:types:AS:IO}AS_IO_Task',
    returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290175",
    triggerRefresh: ri!triggerRefresh,
    filters: {
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "request.requestId",
        operator: "in",
        value: ri!requestId
      ),
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "taskStatus.taskStatusId",
        operator: "=",
        value: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603248"
      ),
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "taskCategory.categoryName",
        operator: "in",
        value: ri!taskCategory,
        applyWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
          ri!taskCategory
        )
      ),
      
    }
  ),
  {
    totalRequiredTasks: local!totalRequiredTasks,
    totalCompletedTasks: local!totalCompletedTasks
  }
)</definition>
