    returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290175",
    triggerRefresh: ri!triggerRefresh,
    filters: {
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "request.requestId",
        operator: "in",
        value: ri!requestId
      ),
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "taskCategory.categoryName",
