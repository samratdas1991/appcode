        applyWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
          ri!taskCategory
        )
      ),
      /*don't count not needed or cancelled tasks*/
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "taskStatus.taskStatusId",
        operator: "in",
        value: {
