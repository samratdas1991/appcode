    <versionUuid>_a-0000e471-623a-8000-9bc3-011c48011c48_663417</versionUuid>
    <rule>
        <name>AS_QNM_UT_calculateCompletionPercentage</name>
        <uuid>_a-0000e460-9d93-8000-9bc1-011c48011c48_624568</uuid>
        <description>Given a set of question metadata, calculates the questionnaire completion percentage</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e447-933d-8000-9baa-011c48011c48_62810"(
  numerator: length(
    where(
      ri!calculatedQuestionMetadata.fulfilled
    )
  ),
  denominator: length(
    where(
      ri!calculatedQuestionMetadata.required
    )
  ),
  default: 1
) * 100</definition>
        <namedTypedValue>
            <name>calculatedQuestionMetadata</name>
            <type>
                <name>AS_QNM_QuestionMetadata?list</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
