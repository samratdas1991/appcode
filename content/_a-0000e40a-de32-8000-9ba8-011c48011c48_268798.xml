    <rule>
        <name>AS_CO_UT_zINTERNAL_updateCdtFieldsByPath_handler</name>
        <uuid>_a-0000e40a-de32-8000-9ba8-011c48011c48_268798</uuid>
        <description>Used by rule `AS_CO_UT_updateCdtFieldsByPath`</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  if(

    and(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(cleanwith(ri!field, ".[]")),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(stripwith(ri!field, "1234567890")),
      not(
        /*Cannot do a "simple" update if its a dictionary list, map list, or the field itself is an array*/
        or(
          #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
            value1: ri!cdt,
            value2: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_101176"()
          ),
          #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
            value1: ri!cdt,
            value2: {a!map()}
          ),
          and(
            /*Both the CDT to update and the field within the CDT are list types*/
            #"_a-0000e415-3035-8000-9bb2-011c48011c48_291952"(
              typeOf: ri!cdt
            ),
            #"_a-0000e415-3035-8000-9bb2-011c48011c48_291952"(
              typeOf: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75842"(
                typeOf: ri!cdt
              )[ri!field]
            )
          )
        )
      )
    ),

    /*Early exit for a simple field-update*/
    #"_a-0000e416-3948-8000-9bb5-011c48011c48_292774"(
      cdt: ri!cdt,
      field: ri!field,
      indices: ri!indices,
      newValue: ri!newValue
    ),

    with(
      local!fieldsAndIndices: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75913"(field: ri!field),

      reduce(
        #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268524"(
          previous: _,
          cdt: ri!cdt,
          fieldsAndIndices: local!fieldsAndIndices,
          /*Indices is only used when passing through from AS_CO_UT_updateCdtFields, never AS_CO_UT_updateCdtFieldsByPath*/
          indices: ri!indices,
          index: _
        ),
        ri!newValue,
        reverse(#"_a-0000e40a-de32-8000-9ba8-011c48011c48_268381"(input: local!fieldsAndIndices))
      )
    )
  )
)</definition>
        <namedTypedValue>
