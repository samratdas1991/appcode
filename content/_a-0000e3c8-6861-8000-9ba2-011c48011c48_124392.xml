    <rule>
        <name>AS_IO_UT_isDuplicateTaskReferenceName</name>
        <uuid>_a-0000e3c8-6861-8000-9ba2-011c48011c48_124392</uuid>
        <description>Returns a list of booleans that is the same length of the list of reference tasks provided.  If a reference task in ri!taskRefs has the same name as a different reference task (i.e. one without the same taskRefId) found in the database, returns true at its position in the list, otherwise false.</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!taskRefs),
  toboolean({}),
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!taskRefs.taskName),
  repeat(length(ri!taskRefs), false),
  a!localVariables(
    local!existingRefTasksWithTheseNames: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_113793"(
      returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290163",
      taskName: ri!taskRefs.taskName
    ),
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!taskRefs,
      expression: length(
        #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
          cdt: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
            cdt: local!existingRefTasksWithTheseNames,
            field: "taskName",
            value: fv!item.taskName
          ),
          field: "taskRefId",
          value: fv!item.taskRefId
        )
      ) &gt; 0
    )
  )
)</definition>
