    <versionUuid>_a-0000e430-f270-8000-9bbc-011c48011c48_367678</versionUuid>
    <rule>
        <name>AS_IO_UT_returnConfigurationLevelCodeSet</name>
        <uuid>_a-0000e3c8-6861-8000-9ba2-011c48011c48_117094</uuid>
        <description>Given a configuration level code, returns all configuration level codes in the set of codes that are visible for the given level</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  contains(
    #"_a-0000e3c8-6861-8000-9ba2-011c48011c48_117082"(),
    ri!configurationLevelCode
  ),
  ldrop(
    #"_a-0000e3c8-6861-8000-9ba2-011c48011c48_117082"(),
    wherecontains(
      ri!configurationLevelCode,
      #"_a-0000e3c8-6861-8000-9ba2-011c48011c48_117082"()
    )-1
  ),
  /* Invalid/null configuration level returns null for query filters */
  touniformstring({})
)</definition>
        <namedTypedValue>
            <name>configurationLevelCode</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
