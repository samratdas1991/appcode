    <rule>
        <name>AS_CO_UT_returnTypeNumber</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157503</uuid>
        <description>Returns the typeNumber of either `type` or `typeOf`</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Integer',
  if(
    isnull(ri!type),
    typeof(ri!typeOf),
    typeof(cast(ri!type, null))
  )
)</definition>
        <namedTypedValue>
            <name>type</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
