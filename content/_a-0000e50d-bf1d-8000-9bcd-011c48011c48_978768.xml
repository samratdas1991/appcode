        </visibility>
        <definition>ri!address.streetAddress &amp; #"_a-0000e447-933d-8000-9baa-011c48011c48_98617"(
  textReturn: ri!address.addressLine2,
  input: ri!address.addressLine2
) &amp; #"_a-0000e447-933d-8000-9baa-011c48011c48_98617"(
  newLine: true,
  input: ri!address.city,
  textReturn: ri!address.city
) &amp; #"_a-0000e447-933d-8000-9baa-011c48011c48_98617"(
  newLine: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
    ri!address.city
  ),
  input: ri!address.stateRegion,
  separator: if(
    isnull(
      ri!address.city
    ),
    "",
    ","
  ),
  textReturn: ri!address.stateRegion
) &amp; #"_a-0000e447-933d-8000-9baa-011c48011c48_98617"(
  input: ri!address.countryCode,
  separator: if(
    isnull(
      ri!address.city
    ),
    "",
    ","
  ),
  textReturn: ri!address.countryCode
) &amp; #"_a-0000e447-933d-8000-9baa-011c48011c48_98617"(
  input: ri!address.stateRegion &amp; ri!address.postalCode,
  textReturn: ri!address.postalCode
)</definition>
