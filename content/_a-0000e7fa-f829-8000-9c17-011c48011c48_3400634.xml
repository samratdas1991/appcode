        <definition>a!localVariables(
  local!selectedValue,
  local!currentUploadTask,
  local!currentReviewTask,
  {
    #"_a-0000e7e5-dad4-8000-9c14-011c48011c48_3312448"(
      "AS_IO_CPS_OnboardingRequestRequiredDocuments"
    ),
    #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3041611"(
      i18nData: ri!i18nData,
      isAddDocRequirement: ri!isAddDocRequirement,
      selectedDocRequirements: ri!selectedDocRequirements,
      removeSaveIntos: {
        a!save(
          ri!allDocumentRequirements,
          remove(
            ri!allDocumentRequirements,
            wherecontains(
              ri!selectedDocRequirements.requirementName,
              ri!allDocumentRequirements.requirementName
            )
          )
        ),
        #"_a-0000e811-1167-8000-9c18-011c48011c48_3518361"(
          tasksForRequest: ri!tasksForRequest,
          taskIndexesToRemove: #"_a-0000e7fa-f829-8000-9c17-011c48011c48_3431009"(
            requiredDocuments: ri!allDocumentRequirements,
            tasks: ri!tasksForRequest
          )
        ),
        #"_a-0000e3c8-6861-8000-9ba2-011c48011c48_124304"(ri!selectedDocRequirements),
        
      }
    ),
    #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3036763"(
      i18nData: ri!i18nData,
      requestId: ri!requestId,
      isAddDocRequirement: ri!isAddDocRequirement,
      documentTypes: ri!documentTypes,
      documentRequirements: ri!allDocumentRequirements,
      docUploadTasks: ri!docUploadTasks,
      reviewTasks: ri!reviewTasks,
      currentReviewTask: local!currentReviewTask,
      currentUploadTask: local!currentUploadTask,
      addDocumentSaves: {
        a!save(
          ri!tasksForRequest,
          append(
            ri!tasksForRequest,
            /*Updating default assignee*/
            #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
              cdt: local!currentUploadTask,
              fields: "groupAssignee",
              newValues: #"_a-0000e438-fd8a-8000-9bbe-011c48011c48_411694"
            ),
            #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
              cdt: local!currentReviewTask,
              fields: {
                "taskPrecedents.taskId_precedent",
                "groupAssignee"
              },
              newValues: {
                length({ ri!tasksForRequest }) + 1,
                #"_a-0000e438-fd8a-8000-9bbe-011c48011c48_411694"
              }
            )
          )
        )
      }
    ),
    #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3033039"(
      selectedValue: local!selectedValue,
      i18nData: ri!i18nData,
      documentRequirements: ri!allDocumentRequirements,
      documentTypes: ri!documentTypes,
      selectedDocRequirement: ri!selectedDocRequirements
    )
  }
)</definition>
