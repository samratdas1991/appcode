    <rule>
        <name>AS_QNM_UT_calculateQuestionShowWhenMetadata</name>
        <uuid>_a-0000e471-623a-8000-9bc3-011c48011c48_666366</uuid>
        <description>Rule used to calculate the showWhen metadata for a question and its fields</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
