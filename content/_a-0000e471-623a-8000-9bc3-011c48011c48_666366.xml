            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>append(
  ri!applicableQuestionsMetadata,
  with(
    local!fields: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_595621"(
      questionTypeCode: ri!question.questionTypeCode
    ).fieldKey,
    local!questionMetadata: 'type!{urn:com:appian:types:AS:QNM}AS_QNM_QuestionMetadata'(
      questionId: ri!question.questionId,
      showWhen: #"_a-0000e447-933d-8000-9baa-011c48011c48_31049"(
        question: ri!question,
        applicableQuestions: ri!applicableQuestions,
        applicableQuestionsMetadata: ri!applicableQuestionsMetadata,
        fieldKey: null
      )
    ),
    reduce(
      #"_a-0000e471-623a-8000-9bc3-011c48011c48_666372"(
        questionMetadata: _,
        fieldKey: _,
        question: ri!question,
        applicableQuestions: ri!applicableQuestions,
        applicableQuestionsMetadata: ri!applicableQuestionsMetadata
      ),
      local!questionMetadata,
      local!fields
    )
  )
)</definition>
