    <versionUuid>_a-0000e7c7-bc72-8000-9c0e-011c48011c48_3090847</versionUuid>
    <rule>
        <name>AS_IO_BL_updateRequiredDocListAfterRemoveAction</name>
        <uuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_3042466</uuid>
        <description>This rule is for updating required document list after remove action</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!requiredDocsToBeRemoved: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
    cdt: ri!selectedRequiredDocList,
    field: "requiredOnboardingDocumentId",
    value: null()
  ),
  local!requiredDocsToBeUpdated: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_111264"(
    cdt: ri!selectedRequiredDocList,
    fields: "requiredOnboardingDocumentId"
  ),
  local!requiredDocsToBeKept: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
    cdt: ri!requiredDocuments,
    field: "requirementName",
    value: index(
      local!requiredDocsToBeRemoved,
      "requirementName",
      null
    )
  ),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!requiredDocsToBeUpdated),
    /*if there are no saved requirements selected, just remove the unsaved*/
    local!requiredDocsToBeKept,
    /*else update the saved requirements to be deleted*/
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
      cdt: local!requiredDocsToBeKept,
      fields: { "isDeleted" },
      indices: wherecontains(
        touniformstring(
          local!requiredDocsToBeUpdated.requirementName
        ),
        touniformstring(ri!requiredDocuments.requirementName),
        
      ),
      newValues: { true() }
    )
  )
)</definition>
        <namedTypedValue>
            <name>selectedRequiredDocList</name>
            <type>
                <name>AS_IO_RequiredOnboardingDocument?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>requiredDocuments</name>
            <type>
                <name>AS_IO_RequiredOnboardingDocument?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
