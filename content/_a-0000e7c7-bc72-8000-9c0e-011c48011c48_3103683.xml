            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>and(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(
    contains(
      #"_a-0000e79b-0335-8000-9c0c-011c48011c48_2919021"(),
      tostring(ri!onboardingStatus)
    )
  ),
  ri!taskStatus = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236",
  ri!docStatus = #"_a-0000e70f-2824-8000-9c01-011c48011c48_2774613",
  /* Removed the below from record level security because we want to show all required documents in customer portal irrespective of the assignee */
  or(
    #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_114114"(
      groups: ri!groupAssignee,
      user: loggedInUser()
    ),
    ri!userAssignee = loggedInUser()
  )
)</definition>
