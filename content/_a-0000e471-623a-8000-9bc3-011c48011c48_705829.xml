    <rule>
        <name>AS_CO_UT_recordLink</name>
        <uuid>_a-0000e471-623a-8000-9bc3-011c48011c48_705829</uuid>
        <description>Generic link to a record.  Depending upon if the link should open in a new tab, either a safeLink or a recordLink is used.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_richTextItem_v1"(
  text: {
    #"SYSTEM_SYSRULES_richTextItem_v1"(text: ri!label),
    #"SYSTEM_SYSRULES_richTextItem_v1"(
      text: {
        " ",
        #"SYSTEM_SYSRULES_richTextIcon"(
          icon: "external-link",
          altText: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: ri!i18nData,
            bundleKey: "AS.CO.CommonObjects.txt_OpensANewWindow"
          )
        )
      },
      showWhen: #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(ri!includeRichTextIcon)
    )
  },
  style: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!style,
    replacementValue: "STRONG"
  ),
  link: #"SYSTEM_SYSRULES_recordLink"(
    label: ri!label,
    recordType: ri!recordType,
    identifier: ri!recordId,
    dashboard: ri!dashboard,
    openLinkIn: if(
      #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(ri!newTab),
      "NEW_TAB",
      "SAME_TAB"
    )
  ),
  linkStyle: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!linkStyle,
    replacementValue: if(
      #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(ri!newTab),
      "STANDALONE",
      "INLINE"
    )
  ),
  color: ri!color,
  showWhen: ri!showWhen,
  size: ri!size
)</definition>
