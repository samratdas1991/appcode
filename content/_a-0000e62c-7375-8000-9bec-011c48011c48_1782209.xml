  local!stateName: #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_978878"(
    i18nData: local!i18nData,
    stateCodes: ri!stateCode,
    returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157"
  ).stateName,
  if(
    ri!displayPlainText,
    a!defaultValue(
      value: local!stateName,
      default: "-"
    ),
    #"SYSTEM_SYSRULES_richTextItem_v1"(
      text: a!defaultValue(
        value: local!stateName,
        default: "-"
      ),
      style: ri!style,
      size: ri!size
    )
  )
)</definition>
