    <interface>
        <name>AS_CO_SBS_StampHeader</name>
        <uuid>_a-0000e528-bc07-8000-9bcf-011c48011c48_1037497</uuid>
        <description>Header format using a stamp and header text</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!stampSizes: { "TINY", "SMALL", "MEDIUM" },
  #"SYSTEM_SYSRULES_sideBySideLayout"(
    alignVertical: "MIDDLE",
    items: {
      #"SYSTEM_SYSRULES_sideBySideItem"(
        width: "MINIMIZE",
        item: #"SYSTEM_SYSRULES_stampField"(
          icon: ri!icon,
          size: index(
            local!stampSizes,
            wherecontains(ri!size, local!stampSizes) - 1,
            ""
          )
        )
      ),
      #"SYSTEM_SYSRULES_sideBySideItem"(
        width: "MINIMIZE",
        item: #"SYSTEM_SYSRULES_richTextDisplayField"(
          labelPosition: "COLLAPSED",
          value: #"SYSTEM_SYSRULES_richTextHeader"(text: ri!label, size: ri!size)
        )
      )
    },
    showWhen: ri!showWhen
  )
)</definition>
        <namedTypedValue>
            <name>icon</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>label</name>
