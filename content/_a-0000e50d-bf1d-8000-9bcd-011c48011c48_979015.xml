    <versionUuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2702286</versionUuid>
    <rule>
        <name>AS_FS_BL_displayRiskScore</name>
        <uuid>_a-0000e50d-bf1d-8000-9bcd-011c48011c48_979015</uuid>
        <description>Given the risk score as a number from 0-100, displays the risk </description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!riskScoreConfiguration: #"699466f4-8db0-4634-b8e1-cfda69948cd4"(),
  local!mediumRiskScoreThreshold: tointeger(
    local!riskScoreConfiguration.mediumRiskScoreThreshold
  ),
  local!highRiskScoreThreshold: tointeger(
    local!riskScoreConfiguration.highRiskScoreThreshold
  ),
  local!riskScoreStyle: local!riskScoreConfiguration.riskScoreStyle,
  if(
    isnull(ri!riskScore),
    a!map(text: "-", color: "SECONDARY"),
    ri!riskScore &lt; local!mediumRiskScoreThreshold,
    /*Low*/
    a!map(
      text: if(
        local!riskScoreStyle = #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2479036",
        ri!riskScore,
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.FS.AllBundles.lbl_Low"
        )
      ),
      scoreLabel: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: "AS.FS.AllBundles.lbl_Low"
      ),
      color: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(brandingKey: "RiskScoreColorLow"),
      code: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2594788"
    ),
    ri!riskScore &lt; local!highRiskScoreThreshold,
    /*Medium*/
    a!map(
      text: if(
        local!riskScoreStyle = #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2479036",
        ri!riskScore,
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.FS.AllBundles.lbl_Medium"
        )
      ),
      scoreLabel: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: "AS.FS.AllBundles.lbl_Medium"
      ),
      color: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(brandingKey: "RiskScoreColorMedium"),
      code: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2594794"
    ),
    /*High*/
    a!map(
      text: if(
        local!riskScoreStyle = #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2479036",
        ri!riskScore,
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.FS.AllBundles.lbl_High"
        )
      ),
      scoreLabel: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: "AS.FS.AllBundles.lbl_High"
      ),
      color: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(brandingKey: "RiskScoreColorHigh"),
      code: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2594801"
    )
  )
)</definition>
