                    </a:expectedOutput>
                </a:assertions>
            </el>
            <el>
                <a:name>Do Not Remove Values</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">{"a", "b", "c"}</a:value>
                    <a:nameRef>array</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
