            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!onboardingStatus: #"_a-0000e41f-788a-8000-9ba9-011c48011c48_54484"(
    refData: ri!refData,
    refDataCodes: ri!onboarding.status
  ),
  {
    icon: local!onboardingStatus.icon,
    color: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(
      brandingKey: "OnboardingRequestStatusIcon"
    ),
    label: if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!onboardingStatus.code),
      "",
      contains(
        #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_1017465"(),
        local!onboardingStatus.code
      ),
      concat(
        proper(local!onboardingStatus.label),
        if(
          contains(
            {
              #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_49494",
              #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_49488"
            },
            local!onboardingStatus.code
          ),
          concat(
            " ",
            #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
              i18nData: ri!i18nData,
              bundleKey: "AS.CO.AllBundles.ins_OnDateTime",
              arguments: {
                #"_a-0000e447-933d-8000-9baa-011c48011c48_31679"(date: ri!onboarding.modifiedDatetime),
                
              }
            )
          ),
          ""
        )
      ),
      error(
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.ins_StatusNotFound",
          arguments: local!onboardingStatus.code
        )
      )
    )
  }
)</definition>
