<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e62c-7375-8000-9bec-011c48011c48_1866358</versionUuid>
    <rule>
        <name>AS_CO_UT_shortenTextWithEllipsis</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_147294</uuid>
        <description>Takes in text and returns the start of the text and appends ... or `ellipsisOverride` -- Will shorten to the length of 'shortenToLen' if the text is longer than 'shortenIfLongerThanLen' -- If 'shortenIfLongerThanLen' is null it will use 'shortenToLen' as the limit</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Text',
  if(
    or(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!shortenToLen),
      ri!shortenToLen = 0
    ),
    ri!text,
    with(
      local!shortenIfLongerThanLen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!shortenIfLongerThanLen,
        replacementValue: ri!shortenToLen
      ),
      if(
        len(ri!text) &lt;= local!shortenIfLongerThanLen,
        ri!text,
        concat(
          trim(
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75618"(text: ri!text, numChars: ri!shortenToLen)
          ),
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
            nullableValue: ri!ellipsisOverride,
            replacementValue: "..."
          )
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>text</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>shortenToLen</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>shortenIfLongerThanLen</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>ellipsisOverride</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
