        <definition>choose(
  #"_a-0000e40a-de32-8000-9ba8-011c48011c48_280802"(
    value: ri!fieldName,
    inArray: {
      "templateId",
      "taskDesc",
      "groupAssignee",
      "userAssignee"
    }
  ),
  /*Default*/
  "",
  /*Display a template name from ID*/
  #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_155202"(
    templateId: ri!value,
    returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157"
  ).templateName,
  /*Display the task description directly*/
  ri!value,
  /*Display group assignee*/
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73726"(
    groupOrUser: togroup(
      ri!value
    )
  ),
  /*Display the user assignee*/
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73660"(
    user: ri!value
  )
)</definition>
