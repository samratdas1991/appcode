    <rule>
        <name>AS_CO_UT_rejectCdtWhereNullFields</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_111264</uuid>
        <description>Removes CDT indices where any of the passed fields are null (acts as an AND filter)</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271216"(
    cdt: ri!cdt,
    fields: ri!fields,
    values: repeat(count(ri!fields), null)
  )
)</definition>
        <namedTypedValue>
            <name>cdt</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fields</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
