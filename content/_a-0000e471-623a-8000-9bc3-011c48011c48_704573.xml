      ),
      cancelLabel: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: local!i18nData,
        bundleKey: "AS.CO.AllBundles.btn_Cancel"
      ),
      confirmSaveInto: {
        a!save(
          ri!userAction,
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73305"
        ),
        if(
          local!initialFundingDate &lt;&gt; ri!request.fundingDetails.actualFundingDate,
          {
            a!save(
              ri!isUpdateTaskDueDates,
              local!isUpdateTaskDueDates
            )
          },
          {
            a!save(ri!isUpdateTaskDueDates, false),
            a!save(ri!comment, null)
          }
        )
      },
      cancelSaveInto: a!save(
        ri!userAction,
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73173"
      ),
      confirmSubmit: true,
      cancelSubmit: true,
      confirmStyle: "PRIMARY",
      cancelStyle: "LINK"
    )
  )
)</definition>
