    <rule>
        <name>AS_IO_UT_determineRequiredDocumentTaskIndexes</name>
        <uuid>_a-0000e7fa-f829-8000-9c17-011c48011c48_3431009</uuid>
        <description>removes deleted required document tasks from a list given a list of tasks and a list of required documents </description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!uniqueRequiredDocuments: union(
    index(
      ri!requiredDocuments,
      "requirementName",
      {}
    ),
    index(
      ri!requiredDocuments,
      "requirementName",
      {}
    )
  ),
  where(
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!tasks,
      expression: a!localVariables(
        local!task: fv!item,
        and(
          local!task.taskCategory.taskCategoryId = #"_a-0000e7d2-6286-8000-9c11-011c48011c48_3119705",
          #"SYSTEM_SYSRULES_forEach"(
            items: local!uniqueRequiredDocuments,
            expression: find(fv!item, local!task.taskName),
            
          ) = 0
        )
      )
    )
  )
),</definition>
        <namedTypedValue>
            <name>requiredDocuments</name>
            <type>
                <name>AS_IO_RequiredOnboardingDocument?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>tasks</name>
            <type>
                <name>AS_IO_Task?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
