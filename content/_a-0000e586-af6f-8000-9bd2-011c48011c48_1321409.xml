        <definition>#"SYSTEM_SYSRULES_entityData"(
  entity: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_100868",
  data: a!localVariables(
    local!onboardingDocument: #"_a-0000e41f-788a-8000-9ba9-011c48011c48_59560"(
      customerDocumentId: ri!customerDocument.customerDocumentId,
      returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157"
    ),
    if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(value: local!onboardingDocument),
      /* If the document has been deleted, for example,
      no reconciliation task will be created for the onboarding request */
      null,
      a!localVariables(
        local!documentReconciliationTask: #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1273099"(
          docUnderstanding: ri!docUnderstanding,
          customerDocModifiedBy: ri!customerDocument.modifiedBy,
          requestId: local!onboardingDocument.requestId,
          onboardingDocumentId: local!onboardingDocument.onboardingDocumentId
        ),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_28158"(
          cdt: local!documentReconciliationTask,
          modifiedBy: loggedInUser()
        )
      )
    )
  )
)</definition>
