    <rule>
        <name>AS_IO_UT_removeTaskFromTemplateSaves</name>
        <uuid>_a-0000e3d1-f4dc-8000-9ba4-011c48011c48_143273</uuid>
        <description>Removes a task from a template, and removes that task where it is designated as a precedent</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>{
  /*remove task as precedent for other tasks*/
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!selectedTemplateTasks,
    expression: a!save(
      fv!item.templateTaskPrecedents,
      #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
        cdt: fv!item.templateTaskPrecedents,
        field: "templateTaskUUIDPrecedent",
        value: ri!taskUUIDsToRemove
      )
    )
  ),
  /*remove task from template*/
  a!save(
    ri!selectedTemplateTasks,
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
      cdt: ri!selectedTemplateTasks,
      field: "templateTaskUUID",
      value: ri!taskUUIDsToRemove
    )
  )
}</definition>
        <namedTypedValue>
            <name>selectedTemplateTasks</name>
            <type>
                <name>AS_IO_R_TemplateTask?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>taskUUIDsToRemove</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
