    <rule>
        <name>AS_IO_BL_mapRequiredDocsToTasks</name>
        <uuid>_a-0000e7fa-f829-8000-9c17-011c48011c48_3462434</uuid>
        <description>maps required documents to task</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_forEach"(
  items: ri!tasks,
  expression: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
      cdt: fv!item,
      field: "taskCategory.taskCategoryId",
      indexDefault: 0
    ) = #"_a-0000e7d2-6286-8000-9c11-011c48011c48_3119705",
    a!localVariables(
      local!task: fv!item,
      #"SYSTEM_SYSRULES_forEach"(
        items: ri!requiredDocuments,
        expression: if(
          find(
            index(fv!item, "requirementName", {}),
            index(local!task, "taskName", {})
          ) &gt; 0,
          #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
            cdt: local!task,
            fields: "requiredDocument.requirementName",
            newValues: index(fv!item, "requirementName", {})
          ),
          {}
        )
      ),
      
    ),
    fv!item
  )
)</definition>
        <namedTypedValue>
            <name>requiredDocuments</name>
            <type>
                <name>AS_IO_RequiredOnboardingDocument?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>tasks</name>
            <type>
                <name>AS_IO_Task?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
