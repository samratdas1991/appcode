        <definition>a!localVariables(
  local!showAll: false,
  #"SYSTEM_SYSRULES_richTextDisplayField"(
    value: #"SYSTEM_SYSRULES_richTextItem_v1"(
      text: if(
        local!showAll,
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.CommonObjects.txt_ShowLess"
        ),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.CommonObjects.txt_ShowAll"
        )
      ),
      linkStyle: "STANDALONE",
      link: #"SYSTEM_SYSRULES_dynamicLink"(
        saveInto: {
          a!save(
            local!showAll,
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(local!showAll)
          ),
          a!save(
            ri!batchSize,
            if(local!showAll, - 1, ri!collapsedBatchSize)
          )
        }
      ),
      showWhen: ri!numItems &gt;= ri!collapsedBatchSize,
      size: "SMALL"
    ),
    align: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
      nullableValue: ri!align,
      replacementValue: "RIGHT"
    )
  )
)</definition>
