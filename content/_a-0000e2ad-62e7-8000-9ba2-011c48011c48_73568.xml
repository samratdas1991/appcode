        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Text',
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!labelPosition,
    replacementValue: if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!label),
      "COLLAPSED",
      if(ri!readOnly, "ADJACENT", "ABOVE")
    )
  )
)</definition>
        <namedTypedValue>
            <name>label</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
