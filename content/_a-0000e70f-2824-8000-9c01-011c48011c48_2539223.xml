    <rule>
        <name>AS_CO_UT_updateJsonField</name>
        <uuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2539223</uuid>
        <description>Given a JSON object and a field to update, returns a JSON object with the field updated to the new value</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_toJson_v1"(
  #"_a-0000e40a-de32-8000-9ba8-011c48011c48_269170"(
    cdt: if(
      isnull(ri!json),
      a!map(),
      #"SYSTEM_SYSRULES_fromJson_v1"(ri!json)
    ),
    fieldValuePairs: a!map(
      field: ri!field,
      value: if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!delimiter),
        ri!value,
        apply(
          #"_a-0000e70f-2824-8000-9c01-011c48011c48_2605847"(text: _, delimiter: ri!delimiter),
          a!flatten(ri!value)
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>json</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>field</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>value</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>delimiter</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
