        <definition>#"SYSTEM_SYSRULES_richTextDisplayField"(
  labelPosition: "COLLAPSED",
  value: {
    #"SYSTEM_SYSRULES_richTextItem_v1"(
      text: {
        #"SYSTEM_SYSRULES_richTextIcon"(icon: ri!icon),
        " " &amp; upper(ri!header)
      },
      color: "SECONDARY",
      size: "SMALL"
    ),
    char(10),
    #"SYSTEM_SYSRULES_richTextItem_v1"(
      text: { ri!value },
      color: "STANDARD",
      size: "MEDIUM",
      style: { "STRONG" }
    ),
    #"SYSTEM_SYSRULES_richTextItem_v1"(
      showWhen: if(
        ri!showAdditionalContext,
        or(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(input: ri!additionalIcon),
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(input: ri!additionalText)
        ),
        false
      ),
      text: {
        #"SYSTEM_SYSRULES_richTextItem_v1"(text: { "  " }, style: { "STRONG" }),
        #"SYSTEM_SYSRULES_richTextIcon"(
          icon: ri!additionalIcon,
          color: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
            nullableValue: ri!additionalIconColor,
            replacementValue: "STANDARD"
          )
        ),
        " " &amp; ri!additionalText
      },
      color: "SECONDARY",
      size: "SMALL"
    )
  },
  align: "LEFT"
)</definition>
