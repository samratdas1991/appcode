    <versionUuid>_a-0000e7fa-f829-8000-9c17-011c48011c48_3427278</versionUuid>
    <rule>
        <name>AS_CO_UT_returnGroupsUserIsAMemberOf</name>
        <uuid>_a-0000e36c-7599-8000-9ba2-011c48011c48_45439</uuid>
        <description>Returns all groups from a parent group that the user is a member of, uses `loggedInUser()` if `user` input is null</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Group?list',
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!parentGroup),
    {},
    with(
      local!parentGroupSubGroups: a!groupMembers(
        group: ri!parentGroup,
        direct: ri!onlyCheckDirectMemberGroupsOfParent,
        memberType: "GROUP",
        pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(
          startIndex: 1,
          batchSize: #"_a-0000e447-933d-8000-9baa-011c48011c48_43371",
          sort: ri!sort
        )
      ).data,
      local!groupsForUser: a!groupsForUser(username: ri!user),
      intersection(
        togroup(local!parentGroupSubGroups),
        togroup(local!groupsForUser)
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>parentGroup</name>
            <type>
                <name>Group</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>onlyCheckDirectMemberGroupsOfParent</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>user</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>sort</name>
            <type>
                <name>SortInfo</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
