    <rule>
        <name>AS_QNM_UT_updateQuestionFieldResponsesForFreeFormQuestion</name>
        <uuid>_a-0000e447-91f4-8000-9bc0-011c48011c48_573573</uuid>
        <description>Used to update the responses of a field based question (Text, Paragraph etc) for a given fieldKey</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e447-933d-8000-9baa-011c48011c48_39018"(
  question: ri!question,
  fieldKey: ri!fieldKey,
  currentFieldResponses: if(
    /* Necssary to avoid creating a null value response, which is not valid for a freeform question */
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
      ri!valueToSave
    ),
    {},
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!valueToSave,
      expression: #"_a-0000e447-933d-8000-9baa-011c48011c48_44150"(
        fieldKey: ri!fieldKey,
        labelValuePairs: {
          value: fv!item
        }
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>question</name>
            <type>
                <name>AS_QNM_Question</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
