<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1161633</versionUuid>
    <rule>
        <name>AS_CO_UT_retrieveSubFolderByName</name>
        <uuid>_a-0000e447-933d-8000-9baa-011c48011c48_98630</uuid>
        <description>Returns direct sub folder of given name</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/*
Note: this rules does not safely check whether the rootFolder exists or the user
has permission to access it.
*/
a!localVariables(
  local!folders: if(
    isnull(
      ri!rootFolder
    ),
    "",
    folder(
      ri!rootFolder,
      "folderChildren"
    )
  ),
  local!folderNames: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
      local!folders
    ),
    "",
    #"SYSTEM_SYSRULES_forEach"(
      items: local!folders,
      expression: folder(
        fv!item,
        "name"
      )
    )
  ),
  cast(
    typeof(
      ri!rootFolder
    ),
    index(
      index(
        local!folders,
        wherecontains(
          ri!subFolderName,
          local!folderNames
        ),
        {}
      ),
      1,
      {}
    )
  )
)</definition>
        <namedTypedValue>
            <name>rootFolder</name>
            <type>
                <name>CollaborationFolder</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>subFolderName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
