    <rule>
        <name>AS_QNM_UT_replaceTemplateQuestionInList</name>
        <uuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_882558</uuid>
        <description>Given a template question and a list of template questions, replaces a question in the list with the provided question by ID.  If there is no match, it is inserted at the end.</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>updatearray(
  ri!allQuestions,
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: index(
      wherecontains(
        ri!question.questionTemplateId,
        ri!allQuestions.questionTemplateId
      ),
      1,
      null
    ),
    replacementValue: length(ri!allQuestions) + 1
  ),
  ri!question
)</definition>
        <namedTypedValue>
            <name>allQuestions</name>
            <type>
                <name>AS_QNM_T_Question?list</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>question</name>
            <type>
                <name>AS_QNM_T_Question</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
