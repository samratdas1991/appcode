    <rule>
        <name>AS_QNM_UT_questionMetadata</name>
        <uuid>_a-0000e447-933d-8000-9baa-011c48011c48_31643</uuid>
        <description>Given a question, returns the appropriate metadata</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  isnull(
    ri!questionTypeCode
  ),
  #"_a-0000e3d1-f4dc-8000-9ba4-011c48011c48_143475"(
    type: 'type!{urn:com:appian:types:AS:QNM}AS_QNM_R_QuestionType'
  ),
  a!localVariables(
    local!metadata: a!refreshVariable(
      value: index(
        #"_a-0000e447-933d-8000-9baa-011c48011c48_36913"(
          internationalize: ri!internationalize
        ),
        wherecontains(
          ri!questionTypeCode,
          #"_a-0000e447-933d-8000-9baa-011c48011c48_36913"(
            internationalize: false
          ).questionTypeCode
        )
      ),
      refreshAlways: true
    ),
    if(
      isnull(
        local!metadata
      ),
      error(
        concat(
          "Question Type Code ",
          ri!questionTypeCode,
          " does not have metadata defined."
        )
      ),
      local!metadata[1]
    )
  )
)</definition>
        <namedTypedValue>
            <name>questionTypeCode</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>internationalize</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
