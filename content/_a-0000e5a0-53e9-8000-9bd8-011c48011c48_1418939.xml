        <definition>#"SYSTEM_SYSRULES_formLayout_v1"(
  label: ri!task.taskName,
  contents: #"SYSTEM_SYSRULES_columnsLayout"(
    columns: {
      #"SYSTEM_SYSRULES_columnLayout"(
        contents: {
          #"SYSTEM_SYSRULES_columnsLayout"(
            columns: {
              #"SYSTEM_SYSRULES_columnLayout"(contents: {}, width: "EXTRA_NARROW"),
              #"SYSTEM_SYSRULES_columnLayout"(
                contents: {
                  if(
                    and(
                      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!headerText),
                      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!bodyText)
                    ),
                    {},
                    #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_235251"(
                      align: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
                        nullableValue: ri!textAlignment,
                        replacementValue: "CENTER"
                      ),
                      prominentText: ri!headerText,
                      secondaryText: ri!bodyText,
                      componentBetweenText: ri!componentBetweenHeaderAndBodyText,
                      includeMarginAbove: true,
                      includeMarginBelow: true
                    )
                  )
                }
              ),
              #"SYSTEM_SYSRULES_columnLayout"(contents: {}, width: "EXTRA_NARROW")
            }
          ),
          #"SYSTEM_SYSRULES_columnsLayout"(
            columns: {
              #"SYSTEM_SYSRULES_columnLayout"(
                contents: {},
                width: "EXTRA_NARROW",
                showWhen: ri!showSpacerColumns
              ),
              #"SYSTEM_SYSRULES_columnLayout"(
                contents: { ri!taskContents },
                width: "AUTO"
              ),
              #"SYSTEM_SYSRULES_columnLayout"(
                contents: {},
                width: "EXTRA_NARROW",
                showWhen: ri!showSpacerColumns
              )
            }
          )
        }
      ),
      #"SYSTEM_SYSRULES_columnLayout"(
        contents: {
          #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_172196"(task: ri!task, i18nData: ri!i18nData)
        },
        width: "MEDIUM"
      )
    }
  ),
  buttons: #"SYSTEM_SYSRULES_ButtonLayout"(
    primaryButtons: #"SYSTEM_SYSRULES_ButtonWidget"(
      label: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: "AS.CO.AllBundles.btn_Close"
      ),
      submit: true,
      confirmHeader: ri!confirmHeader,
      confirmMessage: ri!confirmMessage,
      saveInto: {
        a!save(
          ri!userAction,
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73305"
        ),
        ri!confirmSaveInto
      }
    )
  )
)</definition>
