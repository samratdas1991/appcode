    <rule>
        <name>AS_CO_UI_userImageAndDisplayNameGridColumns</name>
        <uuid>_a-0000e40a-de32-8000-9ba8-011c48011c48_282524</uuid>
        <description>Returns two grid columns, the first containing a user's image and the second containing their name.  Should only be used in gridFields with a borderStyle of LIGHT</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>{
  #"SYSTEM_SYSRULES_gridColumn"(
    label: ri!label,
    value: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_254166"(
      user: fv!row[ri!usernameField],
      size: ri!size
    ),
    align: "CENTER",
    width: "NARROW",
    sortField: if(
      ri!allowSorting = false,
      null,
      ri!usernameField
    )
  ),
  #"SYSTEM_SYSRULES_gridColumn"(
    value: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73660"(
      user: fv!row[ri!usernameField],
      showHyphenIfValueNull: ri!showHyphenIfValueNull,
      i18nData: ri!i18nData
    ),
    sortField: null
  )
}</definition>
        <namedTypedValue>
            <name>usernameField</name>
