    <rule>
        <name>AS_FS_VLD_customerAcronym</name>
        <uuid>_a-0000e471-623a-8000-9bc3-011c48011c48_703656</uuid>
        <description>Validation for uniqueness and length for a customer acronym</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
    ri!customer.customerAcronym
  ),
  "",
  {
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74104"(
      value: ri!customer.customerAcronym,
      minLength: #"_a-0000e447-91f4-8000-9bc0-011c48011c48_557589",
      maxLength: #"_a-0000e447-91f4-8000-9bc0-011c48011c48_557589"
    ),
    if(
      length(
        #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
          cdt: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_33079"(
            customerAcronym: ri!customer.customerAcronym,
            returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290163"
          ),
          field: "customerId",
          value: ri!customer.customerId
        )
      ) &gt; 0,
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: "AS.FS.AllBundles.vld_DuplicateCustomerAcronym"
      ),
      ""
    )
  }
)</definition>
