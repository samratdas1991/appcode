    <rule>
        <name>AS_CO_UT_filterCdtByFieldMathOperation</name>
        <uuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2571943</uuid>
        <description>used to filter a cdt by where a value is greater than/less than/etc a value
executes as cdt value (&gt;/&lt;/&gt;=/&lt;=/=/&lt;&gt;/between) value
options available: "&lt;","&gt;","&lt;=", "&gt;=", "=", "&lt;&gt;", "between"
Note that "=" and "&lt;&gt;" can be better served by other filter rules in most situations.
Note that between is inclusive
DO NOT USE FOR NESTED LISTS</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/*Pass in a CDT to pull back all rows that fit the comparision*/
if(
  or(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!cdt),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!field),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!operator),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!value)
  ),
  null,
  filter(
    #"_a-0000e70f-2824-8000-9c01-011c48011c48_2571984"(
      cdt: _,
      field: ri!field,
      operator: ri!operator,
      value: ri!value
    ),
    ri!cdt
  )
)</definition>
