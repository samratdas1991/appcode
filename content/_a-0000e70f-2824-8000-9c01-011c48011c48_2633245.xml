        <definition>#"_a-0000e40a-de32-8000-9ba8-011c48011c48_269170"(
  cdt: ri!alertConfiguration,
  /*There should always be at least one trigger, so this code is likely never used now,*/
  /*but I am keeping it here because in the case that we nest triggers for more complex behavior*/
  /*we do not want to forget to do this*/
  fieldValuePairs: {
    a!map(
      field: "triggerSet",
      value: if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
            cdt: ri!alertConfiguration,
            field: "triggerSet.triggerConditions",
            indexDefault: {}
          )
        ),
        null,
        ri!alertConfiguration.triggerSet
      )
    ),
    a!map(
      field: "filterSet",
      value: if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
            cdt: ri!alertConfiguration,
            field: "filterSet.filterConditions",
            indexDefault: {}
          )
        ),
        null,
        ri!alertConfiguration.filterSet
      )
    )
  }
)</definition>
