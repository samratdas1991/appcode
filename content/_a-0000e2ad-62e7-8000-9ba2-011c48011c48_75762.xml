    <rule>
        <name>AS_CO_UT_userLinkDisplay</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75762</uuid>
        <description>Displays a read-only link to a user page with customize-able label and label position</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_richTextItem_v1"(
  text: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!username),
    ri!valueIfNull,
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73726"(groupOrUser: ri!username)
  ),
  linkStyle: "STANDALONE",
  link: if(
    and(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!username),
      isusernametaken(ri!username)
    ),
    #"SYSTEM_SYSRULES_userRecordLink"(
      label: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73726"(groupOrUser: ri!username),
      user: touser(ri!username)
    ),
    null
  )
)</definition>
        <namedTypedValue>
            <name>username</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>valueIfNull</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
