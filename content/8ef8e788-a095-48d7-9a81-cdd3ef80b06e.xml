    items: ri!taskCategories,
    expression: a!localVariables(
      local!taskDataByCategory: #"04f7bf4a-965d-4935-8fab-e1b3947212a3"(
        requestId: ri!requestId,
        taskCategory: fv!item,
        triggerRefresh: ri!triggerRefresh
      ),
      #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
        cdt: local!taskDataByCategory,
        fields: { "taskCategory", "isCompleted" },
        newValues: {
          fv!item,
          local!taskDataByCategory.totalCompletedTasks = local!taskDataByCategory.totalRequiredTasks
        }
      )
    )
  ),
  field: "isCompleted",
  ascending: true
)</definition>
