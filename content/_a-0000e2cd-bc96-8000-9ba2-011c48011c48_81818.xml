    lbl_Example: "Label 1"
  }
}</a:value>
                    <a:nameRef>i18nData</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">"SampleBundle.lbl_Example"</a:value>
                    <a:nameRef>bundleKey</a:nameRef>
