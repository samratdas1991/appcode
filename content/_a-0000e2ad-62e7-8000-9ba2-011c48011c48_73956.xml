            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!i18nData: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81720"(
    bundleFolder: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_104931",
    bundleNames: "AS.CO.CommonObjects"
  ),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!value),
    if(
      ri!required,
      /* Number field requiredness can be bypassed with a " ", but this prevents that */
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!requiredMessage,
        replacementValue: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: local!i18nData,
          bundleKey: "AS.CO.CommonObjects.vld_AValueIsRequired"
        )
      ),
      {}
    ),
    if(
      and(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!minSize),
        ri!value &lt; ri!minSize
      ),
      if(
        isnull(ri!minSizeMessage),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: local!i18nData,
          bundleKey: "AS.CO.CommonObjects.vld_MinValue",
          arguments: ri!minSize
        ),
        ri!minSizeMessage
      ),
      if(
        and(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!maxSize),
          ri!value &gt; ri!maxSize
        ),
        if(
          isnull(ri!maxSizeMessage),
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: local!i18nData,
            bundleKey: "AS.CO.CommonObjects.vld_MaxValue",
            arguments: ri!maxSize
          ),
          ri!maxSizeMessage
        ),
        if(
          isinfinite(ri!value),
          if(
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!infiniteMessage),
            #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
              i18nData: local!i18nData,
              bundleKey: "AS.CO.CommonObjects.vld_ValueTooLarge"
            ),
            ri!infiniteMessage
          ),
          null
        )
      ),
      
    )
  )
)</definition>
