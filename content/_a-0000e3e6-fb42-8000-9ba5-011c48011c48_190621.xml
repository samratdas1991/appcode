    <rule>
        <name>AS_IO_BL_reopenTasksVisibility</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_190621</uuid>
        <description>return true if user can reopen tasks</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>and(
  #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_114114"(
    user: loggedInUser(),
    groups: {#"_a-0000e438-fd8a-8000-9bbe-011c48011c48_411694"}
  ),
  /*Request status is not complete or cancelled*/
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(
    contains(
      {
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_49494",
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_49488"
      },
      ri!requestStatus
    )
  ),
  /*Task configuration is not system*/
  ri!taskBehaviorType.configurationLevelCode &lt;&gt; #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_99892",
  /*Task is not an integration task*/
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(
    contains(
      #"_a-0000e673-a4a7-8000-9bef-011c48011c48_1974789"(),
      ri!taskBehaviorType.behaviorSubtypeCode
    )
  ),
  /*Task status is complete or not needed*/
  or(
    ri!taskStatus = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603248",
    ri!taskStatus = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603254"
  )
)</definition>
