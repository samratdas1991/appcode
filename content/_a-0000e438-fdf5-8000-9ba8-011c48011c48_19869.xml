<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_857375</versionUuid>
    <rule>
        <name>AS_CO_I18N_UT_parseBundleKey</name>
        <uuid>_a-0000e438-fdf5-8000-9ba8-011c48011c48_19869</uuid>
        <description>Parses a bundle key and returns a dictionary of the `bundleName` &amp; `labelKey`</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Dictionary',
  with(
    local!split: split(
      ri!bundleKey,
      "."
    ),
    local!bundleName: joinarray(
      remove(
        local!split,
        count(
          local!split
        )
      ),
      "."
    ),
    local!labelKey: local!split[count(
      local!split
    )],
    {
      bundleName: local!bundleName,
      labelKey: local!labelKey
    }
  )
)</definition>
        <namedTypedValue>
            <name>bundleKey</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
