    <rule>
        <name>AS_CO_UT_zInternal_queryEntity_determineSort</name>
        <uuid>_a-0000e52f-5316-8000-9bac-011c48011c48_303936</uuid>
        <description>Used by rule `AS_CO_UT_queryEntity `</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!sortToUse: if(
    /*SINGLE_OBJECT*/
    ri!returnType = #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
    ri!sort,

    /*AGGREGATION*/
    ri!returnType = #"_a-0000e415-3035-8000-9bb2-011c48011c48_290169",
    ri!sort,

    /*TOTAL_COUNT*/
    ri!returnType = #"_a-0000e415-3035-8000-9bb2-011c48011c48_290175",
    null,

    /*DATA_SUBSET or OBJECT_ARRAY*/
    a!localVariables(
      /*Guess what the primary key is by using the first primitive field in the CDT*/
      local!primaryKeyField_guess: index(#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75660"(
        type: ri!entityType,
        onlyPrimitiveFields: true
      ), 1, null),

      /*Grab the appropriate input sort by the returnType*/
      local!inputSort: if(
        ri!returnType = #"_a-0000e415-3035-8000-9bb2-011c48011c48_290131",
        ri!pagingInfo.sort,

        ri!returnType = #"_a-0000e415-3035-8000-9bb2-011c48011c48_290163",
        ri!sort,

        null
      ),

      /*If the primaryKeyField_guess is not null, build a secondary sort by the PK field*/
      local!secondaryPkSortToAppend: if(
        or(isnull(local!primaryKeyField_guess), ri!isAggregation),
        {},
        #"SYSTEM_SYSRULES_sortInfo"(field: local!primaryKeyField_guess, ascending: true)
      ),

      /*Append the secondary PK sort to the sort*/
      local!sortWithSecondaryPkSortApplied: if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!inputSort),
        local!secondaryPkSortToAppend,
        append(local!inputSort, local!secondaryPkSortToAppend)
      ),

      local!sortWithSecondaryPkSortApplied
    )
  ),

  local!sortToUse
)</definition>
