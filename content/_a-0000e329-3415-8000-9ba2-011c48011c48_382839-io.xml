    <rule>
        <name>AS_GM_UT_addUserValidations</name>
        <uuid>_a-0000e329-3415-8000-9ba2-011c48011c48_382839-io</uuid>
        <description>User validations when adding members using the picker fields</description>
        <parentUuid>_a-0000e471-623a-8000-9bc3-011c48011c48_750565</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!addUser),
  {},
  {
    if(
      and(
        contains(
          touser(
            index(
              index(
                ri!indirectUsers,
                wherecontains(
                  false,
                  toboolean(index(ri!indirectUsers, "isGroup", {}))
                ),
                {}
              ),
              "name",
              {}
            )
          ),
          touser(ri!addUser)
        ),
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(touser(ri!addUser))
      ),
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: "AS.GM.GroupManagement.lbl_UserAlreadyPartGroup"
      ),
      if(
        and(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(togroup(ri!addUser)),
          contains(
            index(ri!indirectUsers, "groupId", {}),
            group(ri!addUser, "id")
          )
        ),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.GM.GroupManagement.lbl_GroupAlreadyExists"
        ),
        if(
          and(
            contains(
              ri!parentGroupsToValidate,
              ri!selectedParentGroup
            ),
            contains(
              touser(
                a!groupMembers(
                  pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(
                    startIndex: 1, 
                    /*current max for this function*/
                    batchSize:10000
                  ),
                  group: ri!selectedParentGroup,
                  direct: false,
                  memberType: "USER"
                ).data
              ),
              touser(ri!addUser)
            )
          ),
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: ri!i18nData,
            bundleKey: "AS.GM.GroupManagement.vld_UserAlreadyPartOfOtherGroup"
          ),
          ""
        )
      )
    ),
    
  }
)</definition>
        <namedTypedValue>
            <name>addUser</name>
            <type>
                <name>UserOrGroup?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
