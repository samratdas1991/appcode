    <rule>
        <name>AS_IO_UT_buildI18nLabel</name>
        <uuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1215811</uuid>
        <description>given a text value returns an i18n label</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!iteration: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!iteration,
    replacementValue: 1
  ),
  local!label: if(
    and(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!label),
      local!iteration = 1
    ),
    concat(
      "lbl_",
      stripwith(
        proper(
          #"_a-0000e447-9390-8000-9baa-011c48011c48_41575"(
            folderName: #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_1021111"(string: ri!text, character: " ")
          )
        ),
        " "
      )
    ),
    ri!label
  ),
  if(
    ri!appendIterationValue,
    a!localVariables(
      local!labelToTest: if(
        local!iteration = 1,
        local!label,
        concat(
          local!label,
          tostring(local!iteration - 1)
        )
      ),
      local!initialLabelExists: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
        #"_a-0000e430-f270-8000-9bbc-011c48011c48_355384"(
          labelList: concat(ri!bundleName, ".", local!labelToTest),
          returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157"
        )
      ),
      if(
        local!initialLabelExists,
        #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1215811"(
          iteration: local!iteration + 1,
          label: local!label,
          bundleName: ri!bundleName,
          appendIterationValue: true
        ),
        local!labelToTest
      )
    ),
    local!label
  )
)</definition>
