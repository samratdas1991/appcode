    <versionUuid>_a-0000e774-a792-8000-9c04-011c48011c48_2821535</versionUuid>
    <rule>
        <name>AS_CO_UT_captureMetadata</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_28158</uuid>
        <description>Automatically sets the "auditing" fields on a CDT, including createdDatetime/createdBy/modifiedDatetime/modifiedBy/isActive/isDeleted</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  with(
    local!modifiedBy: a!defaultValue(
      value: ri!modifiedBy,
      default: loggedInUser()
    ),
    local!cdtFields: touniformstring(
      if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!cdt),
        {},
        #"_a-0000e774-a792-8000-9c04-011c48011c48_2821463"(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(cdt: ri!cdt, field: 1, indexDefault: ri!cdt)
        )
      )
    ),
    local!possibleMetaDataFields: #"_a-0000e673-a4a7-8000-9bef-011c48011c48_2003886"(),
    local!capturedMetadataFields: intersection(
      local!cdtFields,
      local!possibleMetaDataFields
    ),
    a!flatten(
      #"SYSTEM_SYSRULES_forEach"(
        items: ri!cdt,
        expression: {
          #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
            cdt: fv!item,
            fields: local!capturedMetadataFields,
            newValues: #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_41232"(
              values: local!capturedMetadataFields,
              inArray: local!possibleMetaDataFields,
              replacement: {
                now(),
                local!modifiedBy,
                a!defaultValue(
                  value: index(fv!item, "createdDatetime", null),
                  default: now()
                ),
                a!defaultValue(
                  value: index(fv!item, "createdBy", null),
                  default: local!modifiedBy
                ),
                #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(
                  boolean: index(fv!item, "isDeleted", null)
                ),
                #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(boolean: index(fv!item, "isActive", null))
              }
            )
          )
        }
      )
    )
  )
)</definition>
