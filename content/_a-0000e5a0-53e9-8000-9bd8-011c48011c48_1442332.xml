    <rule>
        <name>AS_IO_UT_returnDocumentDefaultStatus</name>
        <uuid>_a-0000e5a0-53e9-8000-9bd8-011c48011c48_1442332</uuid>
        <description>Returns Doc default status based on upload type - New/ Existing</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/*If attach later is selected, status is blank */
if(
  ri!uploadType = #"_a-0000e438-fd8a-8000-9bbe-011c48011c48_407338",
  null,
  /*Onboarding Documents */
  /*if IDP is enabled and upload type = New Document, status is "Accepted", otherwise "Awaiting Classified"*/
  /*if IDP is disabled status is "Accepted" if user uploads a required document otherwise "Submitted" */
  if(
    #"_a-0000e5d6-d648-8000-9bde-011c48011c48_1501904"(
      settingsCode: {
        #"_a-0000e5fd-0ed3-8000-9be6-011c48011c48_1579702"
      }
    ).userSettingValue,
    if(
      ri!uploadType = #"_a-0000e438-fd8a-8000-9bbe-011c48011c48_377879",
      #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1290786",
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_118222"
    ),
    if(
      #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(ri!isRequiredDocument),
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_118222",
      #"_a-0000e329-3415-8000-9ba2-011c48011c48_322205"
    )
  )
)</definition>
        <namedTypedValue>
            <name>uploadType</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>isRequiredDocument</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
