    <rule>
        <name>AS_FS_ALT_GenerateAlertAuditMappings</name>
        <uuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2674470</uuid>
        <description>Given a list of alertIds and a list of auditParams, creates a list of alertAuditMappings</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!flatten(
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!alerts,
    expression: {
      if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!customerAuditParameters.auditObject),
        {},
        'type!{urn:com:appian:types:AS:FS}AS_FS_ALT_AlertAuditMapping'(
          alertId: fv!item.alertId,
          auditTypeCode: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2677100",
          auditId: ri!customerAuditParameters.auditObject.customerAuditId
        )
      ),
      if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
          ri!beneficialOwnersAuditParameters.auditObject
        ),
        {},
        /*Ideally we would only link the actual audit entry that triggered the event, but this overlinking is sufficient for now */
        apply(
          'type!{urn:com:appian:types:AS:FS}AS_FS_ALT_AlertAuditMapping'(
            alertId: fv!item.alertId,
            auditTypeCode: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2677106",
            auditId: _
          ),
          ri!beneficialOwnersAuditParameters.auditObject.beneficialOwnerAuditId
        )
      ),
      if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
          ri!entityRelationsAuditParameters.auditObject
        ),
        {},
        /*Ideally we would only link the actual audit entry that triggered the event, but this overlinking is sufficient for now */
        apply(
          'type!{urn:com:appian:types:AS:FS}AS_FS_ALT_AlertAuditMapping'(
            alertId: fv!item.alertId,
            auditTypeCode: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2677113",
            auditId: _
          ),
          ri!entityRelationsAuditParameters.auditObject.customerRelationAuditId
        )
      )
    }
  )
)</definition>
