    <rule>
        <name>AS_FS_DependentDocumentsForCustomerDocumentToDelete</name>
        <uuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2799654</uuid>
        <description>Returns entity data for related documents to be deleted from all apps and updates the data to isDeleted:true</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!srqDocuments: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2794164"(
    customerDocumentId: ri!customerDocumentId
  ),
  local!ioDocuments: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2794031"(
    customerDocumentId: ri!customerDocumentId
  ),
  local!kycDocuments: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2799533"(
    customerDocumentId: ri!customerDocumentId
  ),
  a!flatten(
    #"SYSTEM_SYSRULES_forEach"(
      items: {
        local!ioDocuments,
        local!srqDocuments,
        local!kycDocuments
      },
      expression: if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(fv!item.data),
        {},
        #"urn:appian:function:v1:a:update"(
          fv!item,
          "data",
          #"urn:appian:function:v1:a:update"(
            a!flatten(
              #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_28158"(
                cdt: fv!item.data,
                modifiedBy: loggedInUser()
              )
            ),
            "isDeleted",
            true
          )
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>customerDocumentId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
