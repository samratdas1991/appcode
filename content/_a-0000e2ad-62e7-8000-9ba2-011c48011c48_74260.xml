    <rule>
        <name>AS_CO_UT_formatFieldEntryPlaceHolder</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74260</uuid>
        <description>Creates a formatted placeholder label for dropdowns. </description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!placeholderTrim: trim(ri!placeholder),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!placeholderTrim),
    "",
    ri!wrapInMarkers,
    concat("--- ", local!placeholderTrim, " ---"),
    ri!appendEllipsis,
    concat(local!placeholderTrim, "..."),
    local!placeholderTrim
  )
)</definition>
        <namedTypedValue>
            <name>placeholder</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>wrapInMarkers</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
