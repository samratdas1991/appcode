    <versionUuid>_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2419988</versionUuid>
    <rule>
        <name>AS_CO_UT_mergeSort</name>
        <uuid>_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2374937</uuid>
        <description>Sorting rule used to sort records.  For dictionaries, maps, and CDTs, use AS_CO_UT_sortCdt.

Note: this rule will sort CDTs as long as the field to compare is not of type `Any Type`.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!list),
    length(ri!list) = 1
  ),
  ri!list,
  a!localVariables(
    local!nullPositions: where(
      apply(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(_),
        ri!list[ri!fieldToCompare]
      )
    ),
    local!nullElements: index(ri!list, local!nullPositions, {}),
    local!sortedElements: #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2374846"(
      list: remove(ri!list, local!nullPositions),
      fieldToCompare: ri!fieldToCompare,
      ascending: ri!ascending
    ),
    if(
      ri!nullsFirst,
      append(
        local!nullElements,
        local!sortedElements
      ),
      append(
        local!sortedElements,
        local!nullElements
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>list</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fieldToCompare</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>nullsFirst</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
