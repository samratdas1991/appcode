    <rule>
        <name>AS_CO_UT_zINTERNAL_updateCdtFieldsByPath_nestHandler</name>
        <uuid>_a-0000e40a-de32-8000-9ba8-011c48011c48_268524</uuid>
        <description>Used by rule `AS_CO_UT_updateCdtFieldsByPath`</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!nestedCdt: if(
    ri!index = 1,
    ri!cdt,
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268729"(
      cdt: ri!cdt,
      fields: ri!fieldsAndIndices[enumerate(ri!index - 1) + 1]
    )
  ),
  
  /*Have to handle differently for a dictionary/map or list of dictionary/map, or if the field itself is an array*/
  local!nestedCdtUpdate: if(
    and(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!nestedCdt),
      or(
        #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
          value1: ri!cdt,
          value2: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_101148"()
        ),
        #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
          value1: ri!cdt,
          value2: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_101176"()
        ),
        #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
          value1: ri!cdt,
          value2: a!map()
        ),
        #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
          value1: ri!cdt,
          value2: {a!map()}
        )
      )
    ),
    if(
      /*Have to handle differently if passing indices through from AS_CO_UT_updateCdtFields*/
      and(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!indices),
        ri!index = 1
      ),
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_101176"(),
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_101148"()
    ),
    local!nestedCdt
  ),
  
  #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268432"(
    cdt: local!nestedCdtUpdate,
    field: ri!fieldsAndIndices[ri!index],
/*Indices is only used when passing through from AS_CO_UT_updateCdtFields, never AS_CO_UT_updateCdtFieldsByPath*/
    indices: if(ri!index = 1, ri!indices, {}),
    newValue: ri!previous
  )
)</definition>
