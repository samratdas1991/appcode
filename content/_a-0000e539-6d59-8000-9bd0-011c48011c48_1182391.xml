    <rule>
        <name>AS_FS_BL_searchRefTypesInAdminSettings</name>
        <uuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1182391</uuid>
        <description>Rule to search ref types in admin settings</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!searchText),
  ri!refTypes,
  #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
    cdt: ri!refTypes,
    field: "rDataId",
    value: {
      #"SYSTEM_SYSRULES_forEach"(
        items: ri!internationalizedRefTypes,
        expression: if(
          search(ri!searchText, fv!item.label) &gt; 0,
          {},
          fv!item.rDataId
        )
      )
    }
  )
)</definition>
        <namedTypedValue>
            <name>refTypes</name>
            <type>
                <name>AS_FS_R_Data?list</name>
                <namespace>urn:com:appian:types:AS:FS</namespace>
