    <rule>
        <name>AS_REF_UT_buildRefDataCode</name>
        <uuid>_a-0000e528-bc07-8000-9bcf-011c48011c48_1044244</uuid>
        <description>given a text value and ref data type returns the appropriate Ref data code: &lt;REF_DATA_TYPE&gt;_&lt;TEXT_VALUE&gt;</description>
        <parentUuid>_a-0000e50d-bf1d-8000-9bcd-011c48011c48_1019846</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  /*stack overflow error if iteration is not passed in, so set a default*/
  local!iteration: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!iteration,
    replacementValue: 1
  ),
  local!code: if(
    local!iteration = 1,
    upper(
      concat(
        substitute(ri!refDataType, " ", "_"),
        "_",
        substitute(
          trim(
            #"_a-0000e447-9390-8000-9baa-011c48011c48_41575"(folderName: ri!text)
          ),
          " ",
          "_"
        )
      )
    ),
    ri!code
  ),
  local!codeToTest: if(
    local!iteration = 1,
    local!code,
    concat(
      local!code,
      "_",
      tostring(local!iteration - 1)
    )
  ),
  local!initialCodeExists: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
    #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_1020299"(
      codeList: local!codeToTest,
      returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
      triggerRefresh:ri!triggerRefresh
    )
  ),
  if(
    local!initialCodeExists,
    #"_a-0000e528-bc07-8000-9bcf-011c48011c48_1044244"(
      iteration: local!iteration + 1,
      code: local!code
    ),
    local!codeToTest
  )
)</definition>
