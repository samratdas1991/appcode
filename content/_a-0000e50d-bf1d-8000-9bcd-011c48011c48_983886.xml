        <definition>/*
Audit config for 
'type!{urn:com:appian:types:AS:FS}AS_FS_Customer'
*/
{
  idField: "customerId",
  simpleFields: {
    /*"customerId", excluded since it is the primary key*/
    "name",
    "customerAcronym",
    "isInternational",
    "domicile.countryCode",
    "customerTypeCode",
    "riskScore",
    "gmei",
    "taxId",
    "isQIB",
    "isERISA",
    "industryClassificationCode",
    "customerNumber"
    /* folderId not tracked as it is constant */
    /*no need to track metadata fields*/
  },
  complexFields: {},
  recordIdField: "customerId"
}</definition>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
