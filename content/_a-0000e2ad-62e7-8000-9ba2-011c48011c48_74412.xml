    <interface>
        <name>AS_CO_CP_phoneTextField</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74412</uuid>
        <description>Generic text field to collect a US phone number. Validates that the phone number is the correct length and formats the number properly.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73562"(
  label: ri!label,
  instructions: ri!instructions,
  required: ri!required,
  readOnly: ri!readOnly,
  disabled: ri!disabled,
  value: if(
    isnull(ri!phoneNumber),
    ri!phoneNumber,
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73902"(phoneNumber: ri!phoneNumber)
  ),
  validations: if(
    ri!readOnly,
    {},
    {
      if(
        len(ri!phoneNumber) = 7,
        {},
        if(
          len(ri!phoneNumber) = 10,
          {},
          ri!validationMessage
        )
      ),
      ri!additionalValidation
    }
  ),
  saveInto: {
    a!save(
      ri!phoneNumber,
      cleanwith(save!value, "1234567890")
    ),
    ri!additionalSaveInto
  },
  refreshAfter: ri!refreshAfter,
  validationGroup: ri!validationGroup,
  requiredMessage: ri!requiredMessage,
  labelPosition: ri!labelPosition,
  placeholder: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74260"(placeholder: ri!placeholder),
  helpTooltip: ri!helpTooltip,
  showWhen: if(isnull(ri!showWhen), true, ri!showWhen),
  accessibilityText: ri!accessibilityText
)</definition>
