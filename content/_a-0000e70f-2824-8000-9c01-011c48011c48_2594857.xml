        </visibility>
        <definition>if(
  ri!riskScoreStyle = "INTEGER",
  a!localVariables(
    local!operator: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2539235"(
      json: ri!configurationTriggerSettings,
      field: "operator",
      indexDefault: null
    ),
    local!riskScoreUpperBound: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2539235"(
      json: ri!configurationTriggerSettings,
      field: "riskScoreUpperBound",
      indexDefault: null
    ),
    local!riskScoreLowerBound: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2539235"(
      json: ri!configurationTriggerSettings,
      field: "riskScoreLowerBound",
      indexDefault: null
    ),
    local!customer: if(
      ri!isOriginalObjectEvaluation,
      ri!auditParameters.originalObjects,
      ri!auditParameters.newObjects
    ),
    #"_a-0000e70f-2824-8000-9c01-011c48011c48_2571984"(
      cdt: local!customer,
      field: "riskScore",
      operator: local!operator,
      value: choose(
        #"_a-0000e40a-de32-8000-9ba8-011c48011c48_280802"(
          value: local!operator,
          inArray: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2571673"()
        ),
        error("Invalid risk score operator in triggerConditionId: " &amp; ri!triggerConditionId),
        local!riskScoreLowerBound,
        local!riskScoreLowerBound,
        local!riskScoreUpperBound,
        local!riskScoreUpperBound,
        local!riskScoreLowerBound,
        local!riskScoreLowerBound,
        {
          local!riskScoreLowerBound,
          local!riskScoreUpperBound
        }
      )
    )
  ),
  ri!riskScoreStyle = "TEXT",
  a!localVariables(
    local!operator: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2539235"(
      json: ri!configurationTriggerSettings,
      field: "operator",
      indexDefault: null
    ),
    local!riskScoreLevels: split(
      #"_a-0000e70f-2824-8000-9c01-011c48011c48_2539235"(
        json: ri!configurationTriggerSettings,
        field: "riskScoreLevels",
        indexDefault: null
      ),
      ","
    ),
    local!customer: if(
      ri!isOriginalObjectEvaluation,
      ri!auditParameters.originalObjects,
      ri!auditParameters.newObjects
    ),
    /*extract the level from the untranslated text label returned by AS_FS_BL_displayRiskScore*/
    local!customerRiskScoreLevel: #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_979015"(
      i18nData: null,
      riskScore: local!customer.riskScore
    ).code,
    local!isCustomerRiskScoreInListOfRiskScores: contains(
      local!riskScoreLevels,
      local!customerRiskScoreLevel
    ),
    if(
      local!operator = #"AS_FS_ALT_OPERATOR_CODE_IN",
      local!isCustomerRiskScoreInListOfRiskScores,
      local!operator = #"AS_FS_ALT_OPERATOR_CODE_NOT_IN",
      not(
        local!isCustomerRiskScoreInListOfRiskScores
      ),
      error("Invalid risk score operator in triggerConditionId: " &amp; ri!triggerConditionId)
    )
  ),
  error("Invalid Risk Score Style in triggerConditionId: " &amp; ri!triggerConditionId)
)</definition>
