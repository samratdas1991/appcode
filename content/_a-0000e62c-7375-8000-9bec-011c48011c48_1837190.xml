    <rule>
        <name>AS_CO_VD_alphanumericCharactersValidation</name>
        <uuid>_a-0000e62c-7375-8000-9bec-011c48011c48_1837190</uuid>
        <description>Validation for special characters. If the value contains a characters that is not alphanumeric ( abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890), will return a validation</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  cleanwith(
    ri!value,
    " abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
  ) &lt;&gt; ri!value,
  #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
    i18nData: ri!i18nData,
    bundleKey: "AS.CO.CommonObjects.vld_SpecialCharacterExists",
    arguments: ri!value
  ),
  ""
)</definition>
        <namedTypedValue>
            <name>i18nData</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>value</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
