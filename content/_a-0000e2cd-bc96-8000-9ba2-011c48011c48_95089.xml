    <versionUuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_2967939</versionUuid>
    <rule>
        <name>AS_CO_UT_safeLink</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_95089</uuid>
        <description>Generic safe link component that gracefully handles a null URI</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!link: #"SYSTEM_SYSRULES_safeLink"(
    label: ri!label,
    uri: trim(ri!uri),
    showWhen: and(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!uri),
      ri!showWhen &lt;&gt; false
    )
  ),
  if(
    ri!includeRichTextIcon,
    #"SYSTEM_SYSRULES_richTextItem_v1"(
      text: {
        ri!label,
        " ",
        #"SYSTEM_SYSRULES_richTextIcon"(
          icon: "external-link",
          altText: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: ri!i18nData,
            bundleKey: "AS.CO.CommonObjects.txt_OpensANewWindow"
          )
        )
      },
      style: ri!style,
      link: local!link,
      linkStyle: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!linkStyle,
        replacementValue: "STANDALONE"
      ),
      color: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!color,
        replacementValue: "STANDARD"
      )
    ),
    local!link
  )
)</definition>
