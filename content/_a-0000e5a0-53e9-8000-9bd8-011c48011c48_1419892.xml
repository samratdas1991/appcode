    <rule>
        <name>AS_CO_UT_formatDayAndMonth</name>
        <uuid>_a-0000e5a0-53e9-8000-9bd8-011c48011c48_1419892</uuid>
        <description>displays days and month text</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!day),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!month)
  ),
  "-",
  concat(
    ri!day,
    " ",
    text(date(2020, ri!month, ri!day), "mmmm")
  )
)</definition>
        <namedTypedValue>
            <name>day</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>month</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
