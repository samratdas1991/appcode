    <interface>
        <name>AS_CO_CP_pickerFieldUserGroup</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74476</uuid>
        <description>Generic user group picker with optionally displayed column browser view to help with user search. 

Returns a User or Group type array.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>load(
 local!browserFlag: false,
 local!path,
 local!selectionInBrowser,
 local!value,
 {
  #"SYSTEM_SYSRULES_pickerFieldUsersAndGroups"(
    label: ri!label,
    instructions: ri!instructions,
    labelPosition: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73568"(
      label: ri!label,
      labelPosition: ri!labelPosition,
      readOnly: ri!readOnly
    ),
    validations: ri!validations,
    validationGroup: ri!validationGroup,
    disabled: ri!disabled,
    readOnly: ri!readOnly,
    maxSelections: ri!maxSelections,
    groupFilter: ri!groupFilter,
    value: ri!saveInto,
    placeholder: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74260"(placeholder: ri!placeholder),
    saveInto: a!save(ri!saveInto, #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73758"(value: save!value)),
    required: ri!required,
    requiredMessage:ri!requiredMessage,
    helpTooltip: ri!helpTooltip,
    showWhen: if(isnull(ri!showWhen), true, ri!showWhen),
    accessibilityText: ri!accessibilityText
  ),
  #"SYSTEM_SYSRULES_userAndGroupBrowserFieldColumns"(
    labelPosition: "COLLAPSED",
    rootGroup: ri!groupFilter,
    pathValue:  local!path,
    pathSaveInto:  local!path,
    selectionValue:  local!selectionInBrowser,
    selectionSaveInto:  local!selectionInBrowser,
    height: "SHORT",
    showWhen: local!browserFlag
  ),
  if(or(ri!readOnly, ri!disabled), {},
      #"SYSTEM_SYSRULES_richTextDisplayField"(
        labelPosition: "COLLAPSED",
        value: {
          if(local!browserFlag,
            {#"SYSTEM_SYSRULES_richTextItem_v1"(
              text: "Add Selection",
              link: #"SYSTEM_SYSRULES_dynamicLink"(
                value: local!selectionInBrowser,
                saveInto: if(isnull(local!selectionInBrowser), 
                {}, a!save(ri!saveInto, #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73758"(reject(fn!isnull, {ri!saveInto, local!selectionInBrowser}))))
              )
            ), "   |   "},
            {}
          ),
          #"SYSTEM_SYSRULES_richTextItem_v1"(
            text: if(local!browserFlag, "Hide Browser", "Show Browser"),
            link: #"SYSTEM_SYSRULES_dynamicLink"(
              label: "Toggle Browser",
              value: not(local!browserFlag),
              saveInto: {local!browserFlag, 
              a!save({local!path, local!selectionInBrowser}, null)
              }
            )
          )
        },
        showWhen: ri!showBrowser
      ) 
    )
  }
)</definition>
