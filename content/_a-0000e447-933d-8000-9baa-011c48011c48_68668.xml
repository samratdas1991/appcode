    <rule>
        <name>AS_CO_UT_cardsToGridStyleLayout_handler</name>
        <uuid>_a-0000e447-933d-8000-9baa-011c48011c48_68668</uuid>
        <description>Used by rules `AS_CO_CPS_cardsAsSelection` and `AS_CO_CPS_cardsAsNavigation`</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!maxPerRow: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!maxPerRow,
    replacementValue: count(ri!cardLayouts)
  ),
  local!minPerRow: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!minPerRow,
    replacementValue: 0
  ),
  #"SYSTEM_SYSRULES_forEach"(
    items: enumerate(
      ceiling(
        count(ri!cardLayouts) / local!maxPerRow,
        1
      )
    ),
    expression: a!localVariables(
      local!rowIndex: fv!index,
      #"SYSTEM_SYSRULES_columnsLayout"(
        marginBelow: ri!marginBelow,
        columns: {
          #"SYSTEM_SYSRULES_columnLayout"(
            showWhen: contains({ "RIGHT", "CENTER" }, ri!align)
          ),
          #"SYSTEM_SYSRULES_forEach"(
            items: enumerate(local!maxPerRow),
            expression: a!localVariables(
              local!adjustedIndex: (local!rowIndex - 1) * local!maxPerRow + fv!index,
              local!cardLayout: a!refreshVariable(
                value: index(ri!cardLayouts, local!adjustedIndex, {}),
                refreshAlways: true
              ),
              #"SYSTEM_SYSRULES_columnLayout"(
                width: ri!columnWidth,
                contents: local!cardLayout,
                showWhen: if(
                  and(
                    local!rowIndex = 1,
                    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!cardLayout),
                    fv!index &gt; local!minPerRow
                  ),
                  false,
                  true
                )
              )
            )
          ),
          #"SYSTEM_SYSRULES_columnLayout"(
            showWhen: contains({ "LEFT", "CENTER" }, ri!align)
          )
        },
        stackWhen: ri!stackWhen
      )
    )
  )
)</definition>
