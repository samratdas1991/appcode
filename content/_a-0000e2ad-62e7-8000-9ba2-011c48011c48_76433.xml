            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!fieldsAndIndices: if(
    runtimetypeof(ri!field) = #"_a-0000e40a-de32-8000-9ba8-011c48011c48_279359"(),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75913"(field: ri!field),
    ri!field
  ),

  reduce(
    index(_, _, ri!indexDefault),
    ri!cdt,
    a!flatten(local!fieldsAndIndices)
  )
)</definition>
