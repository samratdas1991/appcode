        <definition>{
  #"SYSTEM_SYSRULES_sectionLayout_v1"(
    label: ri!questionCategory.questionCategoryName,
    divider: "ABOVE",
    marginBelow: "NONE"
  ),
  #"SYSTEM_SYSRULES_forEach"(
    items: #"_a-0000e447-933d-8000-9baa-011c48011c48_42922"(
      array: ri!questionsOnPage.groupNumber,
      ascending: true
    ),
    expression: a!localVariables(
      local!questionInGroupIndices: wherecontains(
        fv!item,
        ri!questionsOnPage.groupNumber
      ),
      local!questionMetadataInGroupIndices: wherecontains(
        ri!questionsOnPage.questionId[local!questionInGroupIndices],
        ri!calculatedQuestionPageMetadata.questionId
      ),
      #"_a-0000e447-933d-8000-9baa-011c48011c48_37453"(
        questionsInGroup: ri!questionsOnPage[local!questionInGroupIndices],
        calculatedQuestionGroupMetadata: ri!calculatedQuestionPageMetadata[local!questionMetadataInGroupIndices],
        readOnly: ri!readOnly,
        i18nData: ri!i18nData,
        divider: if(
          or(
            not(
              fv!isLast
            ),
            ri!spacerBelow
          ),
          "BELOW",
          "NONE"
        ),
        allUploadedDocs: ri!allUploadedDocs
      )
    )
  ),
  if(
    ri!spacerBelow,
    {
      #"SYSTEM_SYSRULES_sideBySideLayout"(),
      #"SYSTEM_SYSRULES_sideBySideLayout"()
    },
    {}
  )
}</definition>
