    <rule>
        <name>AS_CO_UT_wrapTextInQuotes</name>
        <uuid>_a-0000e329-3415-8000-9ba2-011c48011c48_386641</uuid>
        <description>Wraps text in quotes, allows for a custom quote type</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Text',
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!text),
    "",
    concat(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!quote,
        replacementValue: """"
      ),
      ri!text,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!quote,
        replacementValue: """"
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>text</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>quote</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
