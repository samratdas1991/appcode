        <name>AS_IO_BL_chooseStatusIconForRequest</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_138931</uuid>
        <description>Choose the status icon based on est. funding date/actual funding date/status</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!requestStatusRefData: #"_a-0000e41f-788a-8000-9ba9-011c48011c48_54484"(
    refData: ri!refdata,
    refDataCodes: ri!status
  ),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!status),
    a!map(
      icon: "minus-circle",
      color: "NEGATIVE",
      caption: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: "AS.CO.AllBundles.lbl_ErrorPleaseContact"
      )
    ),
    a!map(
      icon: local!requestStatusRefData.icon,
      color: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(
        brandingKey: "OnboardingRequestStatusIcon"
      ),
      label: local!requestStatusRefData.label,
      caption: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: "AS.CO.AllBundles.lbl_Onboarding_With_Param",
        arguments: lower(local!requestStatusRefData.label)
      )
    )
  )
)</definition>
