<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e471-623a-8000-9bc3-011c48011c48_702371</versionUuid>
    <rule>
        <name>AS_CO_UT_urlForRecord</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_91843</uuid>
        <description>Returns the url for a record for use in a safe link</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Text',
  if(
    or(
      isnull(ri!recordType),
      isnull(ri!recordId)
    ),
    "",
    with(
      local!tempoUrl: concat(
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_100254"(),
        "tempo/records/item/",
        #"SYSTEM_SYSRULES_recordLink"(
          recordType: ri!recordType,
          identifier: ri!recordId
        ).'@attributes'.'@anyAttribute'.'_recordRef',
        "/view/summary"
      ),
      if(
        isnull(ri!siteWebAddressIdentifier),
        /*tempo safe link*/
        local!tempoUrl,
        if(
          isnull(ri!recordId),
          /* Link to the record list view. Navigates user to tempo */
          local!tempoUrl,
          /* Link to the record dashboard */
          substitute(
            local!tempoUrl,
            "/tempo/records/item",
            concat(
              "/sites/",
              ri!siteWebAddressIdentifier,
              if(
                isnull(ri!sitePage),
                "",
                concat("/page/", ri!sitePage)
              ),
              "/record"
            )
          )
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>recordType</name>
            <type>
                <name>RecordType</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>recordId</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>siteWebAddressIdentifier</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>sitePage</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
