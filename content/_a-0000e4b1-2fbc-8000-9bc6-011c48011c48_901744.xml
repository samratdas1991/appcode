    <versionUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_901755</versionUuid>
    <rule>
        <name>AS_QNM_UT_splitTextIfNotNull</name>
        <uuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_901744</uuid>
        <description>Rule to split the value if it is not null</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  and(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
      ri!textToSplit
    ),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
      ri!separator
    )
  ),
  split(
    ri!textToSplit,
    ri!separator
  ),
  null
)</definition>
        <namedTypedValue>
            <name>textToSplit</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>separator</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
