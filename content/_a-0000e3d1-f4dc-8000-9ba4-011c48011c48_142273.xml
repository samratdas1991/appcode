        <definition>'type!{urn:com:appian:types:AS:IO}AS_IO_Task'(
  taskId: null,
  request: null,
  taskRefId: ri!templateTask.taskRef.taskRefId,
  taskName: ri!templateTask.taskName,
  taskDesc: ri!templateTask.taskDesc,
  taskBehaviorType: ri!templateTask.taskRef.taskBehaviorType,
  taskCategory: ri!templateTask.taskRef.taskCategory,
  groupAssignee: if(
    and(
      ri!templateTask.groupAssignee = group(#"_a-0000e539-6d59-8000-9bd0-011c48011c48_1296974", "id"),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(
        input: #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(ri!skipCustomerGroupSwap)
      )
    ),
    ri!request.customer.groupId,
    ri!templateTask.groupAssignee
  ),
  userAssignee: null,
  taskStatus: if(
    ri!templateTask.taskRef.taskBehaviorType.behaviorSubtypeCode = #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_99647",
    #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3006672"(
      returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
      taskStatusIds: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236"
    ),
    if(
      ri!templateTask.taskRef.taskBehaviorType.behaviorSubtypeCode = #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3040883",
      #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3006672"(
        returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
        taskStatusIds: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603230"
      ),
      if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!templateTask.templateTaskPrecedents),
        #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3006672"(
          returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
          taskStatusIds: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236"
        ),
        #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3006672"(
          returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
          taskStatusIds: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603230"
        ),
        
      ),
      
    )
  ),
  docUploadContext: ri!templateTask.taskRef.docUploadContext,
  docSignatureContext: ri!templateTask.taskRef.docSignatureContext,
  taskPrecedents: #"SYSTEM_SYSRULES_forEach"(
    items: ri!templateTask.templateTaskPrecedents,
    expression: 'type!{urn:com:appian:types:AS:IO}AS_IO_Task_Precedent'(
      taskPrecedentId: null,
      taskId: null,
      /* Here we are storing the index of the precedent within the larger list of tasks */
      /* This is because we do not have actual IDs before writing to the database */
      /* Within process AS IO Process Setup, within rule AS_IO_UT_setTaskPrecedentsOnTasksAfterInitialWrite, */
      /* we will update the IDs after writing the initial tasks without precedents */
      taskId_precedent: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_269546"(
        cdt: ri!template.templateTasks,
        field: "templateTaskUUID",
        value: fv!item.templateTaskUUIDPrecedent
      )
    )
  ),
  availableDatetime: null,
  orderNumber: ri!templateTask.orderNumber,
  completedBy: null,
  completedDatetime: null,
  createdBy: null,
  createdDatetime: null,
  modifiedBy: null,
  modifiedDatetime: null,
  dueDate: #"_a-0000e471-623a-8000-9bc3-011c48011c48_655029"(
    slaDays: ri!templateTask.slaDays,
    slaRule: ri!templateTask.slaRule,
    fundingDate: ri!request.fundingDetails.estFundingDate,
    slaDayType: ri!template.slaDayType
  ),
  slaDays: ri!templateTask.slaDays,
  slaRule: ri!templateTask.slaRule
)</definition>
