    <rule>
        <name>AS_IO_CDT_mapTemplateTaskToRuntimeTask</name>
        <uuid>_a-0000e3d1-f4dc-8000-9ba4-011c48011c48_142273</uuid>
        <description>Converts an AS_IO_R_TemplateTask to an AS_IO_Task object</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
