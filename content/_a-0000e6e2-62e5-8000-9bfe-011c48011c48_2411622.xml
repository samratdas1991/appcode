    <rule>
        <name>AS_FS_UT_sortCustomerRelationsForDisplayByCustomerName</name>
        <uuid>_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2411622</uuid>
        <description>will return sorted Customer relations by Name</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75720"(
  cdt: #"SYSTEM_SYSRULES_forEach"(
    /*cast to map in order to add customerNameToDisplay field*/
    items: #"_a-0000e586-af6f-8000-9bd2-011c48011c48_1348231"(ri!customerRelationsForDisplay),
    expression: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_269170"(
      cdt: fv!item,
      fieldValuePairs: {
        {
          field: "relatedCustomer",
          /*relatedCustomer = customer which is not currentCustomer*/
          value: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
            cdt: ri!relatedCustomers,
            field: "customerId",
            value: if(
              ri!currentCustomer.customerId = fv!item.objectForDisplay.fromCustomerId,
              fv!item.objectForDisplay.toCustomerId,
              fv!item.objectForDisplay.fromCustomerId
            )
          )[1]
        }
      }
    )
  ),
  field: "relatedCustomer.name",
  ascending: true()
)</definition>
        <namedTypedValue>
            <name>customerRelationsForDisplay</name>
            <type>
                <name>AS_FS_UNMAPPED_PendingObjectDisplay?list</name>
                <namespace>urn:com:appian:types:FS</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>relatedCustomers</name>
            <type>
                <name>AS_FS_Customer?list</name>
                <namespace>urn:com:appian:types:AS:FS</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>currentCustomer</name>
            <type>
                <name>AS_FS_Customer</name>
                <namespace>urn:com:appian:types:AS:FS</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
