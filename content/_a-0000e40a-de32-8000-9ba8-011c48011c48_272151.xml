<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e723-f4e4-8000-9bc6-011c48011c48_537044</versionUuid>
    <rule>
        <name>AS_CO_UT_whereContains</name>
        <uuid>_a-0000e40a-de32-8000-9ba8-011c48011c48_272151</uuid>
        <description>Enhanced wherecontains() which avoids type-casting issues and properly handles for document and group types</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Integer?list',
  with(
    local!array: a!flatten(ri!array),
  
    if(
    
      /*Have to handle separately for document types due to how it casts to string*/
      #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
        value1: local!array,
        value2: {todocument(null)}
      ),
      wherecontains(
        todocument(ri!values),
        local!array
      ),
    
      /*Have to handle separately for group types due to how it casts to string*/
      #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162769"(
        value1: local!array,
        value2: {togroup(null)}
      ),
      wherecontains(
        togroup(ri!values),
        local!array
      ),
    
      /*Casting to string accounts for any primitive type (integer, boolean, ect.)*/
      wherecontains(
        if(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!values),
          touniformstring(ri!values),
          touniformstring(a!flatten(ri!values))
        ),
        touniformstring(local!array)
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>values</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>array</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
