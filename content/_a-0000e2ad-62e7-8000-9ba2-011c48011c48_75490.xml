    <rule>
        <name>AS_CO_UT_checkIfDuplicateExistsInArrayByIndex</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75490</uuid>
        <description>Checks if an array contains duplicate values, but only returns true if the index entered is duplicated (used for grid validations)</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Boolean',
  if(
    or(
      
      /*If index is less than 0*/
      tointeger(ri!index) &lt;= 0,
      
      /*If index is greater than array length*/
      tointeger(ri!index) &gt; count(ri!array),
      
      /*If we are skipping nulls, and the current index is null*/
      and(
        #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(ri!skipNulls),
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!array[ri!index])
      )
    ),
    
    /*Then return false*/
    false,
  
    /*Otherwise, remove the current index from the list*/
    /*And check if the remaining list contains the current index*/
    with(
      local!array: remove(
        ri!array,
        tointeger(ri!index)
      ),
      local!valueAtIndex: ri!array[ri!index],
      if(
        ri!caseSensitive,
        contains(
          local!array,
          local!valueAtIndex
        ),
        contains(
          lower(local!array),
          lower(local!valueAtIndex)
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>array</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>skipNulls</name>
            <type>
                <name>boolean</name>
