        <definition>a!localVariables(
  local!responsesForOtherFields: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
    cdt: ri!question.responses,
    field: "fieldKey",
    value: ri!fieldKey
  ),
  #"_a-0000e40a-de32-8000-9ba8-011c48011c48_269170"(
    cdt: ri!question,
    fieldValuePairs: {
      field: "responses",
      value: append(
        local!responsesForOtherFields,
        ri!currentFieldResponses
      )
    }
  )
)</definition>
