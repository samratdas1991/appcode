        <definition>a!localVariables(
  local!currencies: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157870"(
    entity: #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1135811",
    filters: {
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "isCountryActive",
        operator: "=",
        value: true
      ),
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "countryCode",
        operator: "in",
        value: ri!countryCodes
      )
    },
    executeWhen: ri!executeWhen,
    entityType: 'type!{urn:com:appian:types:AS:FS}AS_FS_V_Currency',
    returnType: ri!returnType
  ),
  #"SYSTEM_SYSRULES_forEach"(
    items: local!currencies,
    expression: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268479"(
      cdt: fv!item,
      fields: "countryName",
      newValues: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: fv!item.countryName
      )
    )
  )
)</definition>
