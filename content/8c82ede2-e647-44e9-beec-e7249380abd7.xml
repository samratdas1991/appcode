<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e5d6-d648-8000-9bde-011c48011c48_1493189</versionUuid>
    <rule>
        <name>AS_CO_UT_convertToUnixTime</name>
        <uuid>8c82ede2-e647-44e9-beec-e7249380abd7</uuid>
        <description>Given a date and time, converts to unix time</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  isnull(ri!dateTime),
  null,
  with(
    local!unixEpoch: fn!datetime(1970, 1, 1, 0, 0, 0, 0),
    local!intervalDiff: ri!dateTime - local!unixEpoch,
    local!stringintervalDiff: fn!tostring(local!intervalDiff),
    local!days: fn!todecimal(local!intervalDiff),
    local!stringPartExcludingDays: fn!split(local!stringintervalDiff, "::")[2],
    local!splitPartExcludingDays: fn!split(local!stringPartExcludingDays, ":"),
    local!hours: fn!todecimal(local!splitPartExcludingDays[1]),
    local!minutes: fn!todecimal(local!splitPartExcludingDays[2]),
    local!splitSecondsAndMilliseconds: split(local!splitPartExcludingDays[3], "."),
    local!seconds: fn!todecimal(local!splitSecondsAndMilliseconds[1]),
    local!milliseconds: fn!todecimal(local!splitSecondsAndMilliseconds[2]),
    product(
      local!days * 24 * 60 * 60 + local!hours * 60 * 60 + local!minutes * 60 + local!seconds + local!milliseconds,
      if(ri!inMilliseconds, 1000, 1)
    )
  )
)</definition>
        <namedTypedValue>
            <name>dateTime</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>inMilliseconds</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
