    <rule>
        <name>AS_ADT_UT_buildSimpleFieldChangesDictionary</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_162005-adt</uuid>
        <description>Given the fieldName, original object, and the new object, determines if a simple field change was made, returning {} if not, and returning a dictionary representation of the simple field audit if so.</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-adt</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!oldValue: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
    cdt: ri!originalObject,
    field: ri!simpleFieldName
  ),
  local!newValue: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
    cdt: ri!newObject,
    field: ri!simpleFieldName
  ),
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73624"(
      value1: local!oldValue,
      value2: local!newValue
    ),
    {},
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73856"(
      fieldValuePairs: {
        {
          field: "fieldName",
          value: ri!simpleFieldName
        },
        {
          field: "oldValue",
          value: #"_a-0000e447-933d-8000-9baa-011c48011c48_112825"(local!oldValue)
        },
        {
          field: "newValue",
          value: #"_a-0000e447-933d-8000-9baa-011c48011c48_112825"(local!newValue)
        }
      }
    )
  )
)</definition>
        <namedTypedValue>
            <name>simpleFieldName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>originalObject</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>newObject</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
