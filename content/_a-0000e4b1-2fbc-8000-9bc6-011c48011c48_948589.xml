    <rule>
        <name>AS_CO_UT_swapFieldInList</name>
        <uuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_948589</uuid>
        <description>Given a list, a field, and two indices to update, swaps the value of the field at index one with that of index two.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    max(
      ri!indexOne,
      ri!indexTwo
    ) &gt; count(
      ri!list
    ),
    min(
      ri!indexOne,
      ri!indexTwo
    ) &lt; 1
  ),
  error(
    "Both indices must be within the bounds of the list"
  ),
  a!localVariables(
    local!valueOne: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
      cdt: ri!list[ri!indexOne],
      field: ri!field
    ),
    local!valueTwo: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
      cdt: ri!list[ri!indexTwo],
      field: ri!field
    ),
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
      cdt: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
        cdt: ri!list,
        fields: ri!field,
        indices: ri!indexOne,
        newValues: local!valueTwo
      ),
      fields: ri!field,
      indices: ri!indexTwo,
      newValues: local!valueOne
    )
  )
)</definition>
        <namedTypedValue>
            <name>list</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
