    <rule>
        <name>AS_QNM_UI_documentDownloadLink</name>
        <uuid>_a-0000e447-91f4-8000-9bc0-011c48011c48_585818</uuid>
        <description>Generic document download link component that handles read-only logic dynamically. </description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/* TOBEDONE Can this be easily replaced with AS_CO_LNK_documentDownloadLink?  */
#"SYSTEM_SYSRULES_richTextItem_v1"(
  text: {
    if(
      ri!displayDocTypeIcon,
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_145598"(
        extension: fn!document(
          ri!document,
          "extension"
        ),
        size: "MEDIUM",
        link: #"SYSTEM_SYSRULES_documentDownloadLink"(
          document: ri!document,
          label: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73556"(
            label: ri!label
          ),
          showWhen: and(
            #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(
              ri!showWhen
            ),
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
              ri!document
            )
          )
        )
      ),
      ""
    ),
    " ",
    ri!label,
    ".",
    fn!document(
      ri!document,
      "extension"
    ),
    " ",
    #"SYSTEM_SYSRULES_richTextIcon"(
      icon: "download",
      link: #"SYSTEM_SYSRULES_documentDownloadLink"(
        document: ri!document,
        label: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73556"(
          label: ri!label
        ),
        showWhen: and(
          #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(
            ri!showWhen
          ),
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
            ri!document
          )
        )
      ),
      linkStyle: "STANDALONE",
      showWhen: ri!displayDownloadIcon
    )
  },
  link: #"SYSTEM_SYSRULES_documentDownloadLink"(
    document: ri!document,
    label: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73556"(
      label: ri!label
    ),
    showWhen: and(
      #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(
        ri!showWhen
      ),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(
        ri!document
      )
    )
  ),
  linkStyle: "STANDALONE",
  style: "STRONG"
)</definition>
