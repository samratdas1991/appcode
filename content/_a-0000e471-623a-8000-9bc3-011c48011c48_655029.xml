<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e471-623a-8000-9bc3-011c48011c48_712785</versionUuid>
    <rule>
        <name>AS_IO_BL_calculateTaskDueDateFromSlaDays</name>
        <uuid>_a-0000e471-623a-8000-9bc3-011c48011c48_655029</uuid>
        <description>Business logic to calculate task due date from sla days</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
      ri!slaDays
    ),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
      ri!fundingDate
    )
  ),
  null,
  if(
    ri!slaRule = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620337",
    if(
      ri!slaDayType = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620378",
      workday(
        ri!fundingDate,
        - ri!slaDays
      ),
      /*default is calendar days*/
      ri!fundingDate - ri!slaDays,
      
    ),
    if(
      ri!slaDayType = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620378",
      workday(
        ri!fundingDate,
        + ri!slaDays
      ),
      /*default is calendar days*/
      ri!fundingDate + ri!slaDays,
      
    )
  )
)</definition>
        <namedTypedValue>
            <name>slaRule</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>slaDays</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fundingDate</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>slaDayType</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
