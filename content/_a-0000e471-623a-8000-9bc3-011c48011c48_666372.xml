        <definition>'type!{urn:com:appian:types:AS:QNM}AS_QNM_QuestionMetadata'(
  questionId: ri!question.questionId,
  showWhen: ri!questionMetadata.showWhen,
  fieldMetadata: append(
    ri!questionMetadata.fieldMetadata,
    'type!{urn:com:appian:types:AS:QNM}AS_QNM_FieldMetadata'(
      fieldKey: ri!fieldKey,
      showWhen: if(
        ri!questionMetadata.showWhen,
        #"_a-0000e447-933d-8000-9baa-011c48011c48_31049"(
          question: ri!question,
          applicableQuestions: ri!applicableQuestions,
          applicableQuestionsMetadata: ri!applicableQuestionsMetadata,
          fieldKey: ri!fieldKey
