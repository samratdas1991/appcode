  entityType: 'type!{urn:com:appian:types:AS:EI:NORTHROW}AS_EI_NORTHROW_V_AutomatedCompanyCheckSchedule',
  returnType: ri!returnType,
  filters: {
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "companyId",
      operator: "=",
      value: ri!companyId
    ),
    if(
      ri!isDue,
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "upcomingAutomatedCheckDate",
        operator: "&lt;=",
        value: today()
      ),
      {}
    )
  }
)</definition>
