    <interface>
        <name>AS_FS_TAG_displayDueTagForTask</name>
        <uuid>_a-0000e586-af6f-8000-9bd2-011c48011c48_1345877</uuid>
        <description>Used to display the tag field for displaying the due date tag for onboarding task</description>
        <parentUuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1261631</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_tagField"(
  label: "Tag Field",
  labelPosition: "COLLAPSED",
  tags: {
    if(
      ri!dueDate &lt; ri!currentTime,
      #"SYSTEM_SYSRULES_tagItem"(
        text: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.FS.AllBundles.txt_Overdue"
        ),
        backgroundColor: "NEGATIVE"
      ),
      ri!dueDate &lt; ri!currentTime + 5,
      #"SYSTEM_SYSRULES_tagItem"(
        text: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.FS.AllBundles.txt_Duesoon"
        ),
        backgroundColor: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(
          brandingKey: "WarningColor"
        ),
        textColor: "STANDARD"
      ),
      {}
    )
  },
  align: "CENTER",
  size: "SMALL",
  showWhen: ri!dueDate &lt; ri!currentTime + 5
)</definition>
        <namedTypedValue>
