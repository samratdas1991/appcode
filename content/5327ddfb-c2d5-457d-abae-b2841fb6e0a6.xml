    <rule>
        <name>AS_QNM_UT_retrieveDirectFollowUpQuestions</name>
        <uuid>5327ddfb-c2d5-457d-abae-b2841fb6e0a6</uuid>
        <description>rule to retrieve the followup questions</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{urn:com:appian:types:AS:QNM}AS_QNM_T_Question?list',
  a!localVariables(
    /* Filter first for efficiency */
    local!potentialFollowupQuestions: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
      cdt: ri!allQuestions,
      field: "questionCategoryTemplateId",
      value: ri!question.questionCategoryTemplateId
    ),
    #"SYSTEM_SYSRULES_forEach"(
      items: local!potentialFollowupQuestions,
      expression: if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
            cdt: #"SYSTEM_SYSRULES_forEach"(
              items: fv!item.isDisplayedPrecedentSets,
              expression: fv!item.questionPrecedents
            ),
            field: "questionTemplateId_precedent",
            value: ri!question.questionTemplateId
          )
        ),
        {},
        fv!item
      )
    )
  )
)</definition>
