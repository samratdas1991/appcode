    <rule>
        <name>AS_FS_UT_displayCountryFlag</name>
        <uuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2598124</uuid>
        <description>Displays Country Flag for a given list of country.</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/* NOTE: Windows 10 does not support flag emojis, so this rule will only display the two country code unicode characters on that operating system */
#"SYSTEM_SYSRULES_forEach"(
  items: ri!country.flagUnicodeJson,
  expression: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(fv!item),
    {},
    concat(char(#"SYSTEM_SYSRULES_fromJson_v1"(fv!item)))
  )
)</definition>
        <namedTypedValue>
            <name>country</name>
            <type>
                <name>AS_FS_R_Country?list</name>
                <namespace>urn:com:appian:types:AS:FS</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
