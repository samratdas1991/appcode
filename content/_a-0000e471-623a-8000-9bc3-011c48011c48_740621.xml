    <versionUuid>_a-0000e825-b425-8000-9c1c-011c48011c48_3620488</versionUuid>
    <rule>
        <name>AS_IO_BL_isEstFundingDateAtRisk</name>
        <uuid>_a-0000e471-623a-8000-9bc3-011c48011c48_740621</uuid>
        <description>Given an onboarding request Id returns whether the onboarding is at risk</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/*  check status first and possible early exit*/
if(
  /*  The onboarding is not at risk if its status is not  "active"*/
  ri!status &lt;&gt; #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_48769",
  false,
  /* Get tasks to determine if there are any overdue tasks*/
  a!localVariables(
    local!outstandingTasks: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_104148"(
      requestId: ri!requestId,
      taskStatusIds: {
        #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236",
        #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603230"
      },
      returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290163",
      triggerRefresh: ri!triggerRefresh
    ),
    or(
      #"SYSTEM_SYSRULES_forEach"(
        items: local!outstandingTasks,
        expression: #"_a-0000e471-623a-8000-9bc3-011c48011c48_698768"(task: fv!item)
      ),
      /* If no overdue tasks, then the onboarding is at risk only if estFundingDate is in the past*/
      todate(ri!estFundingDate) &lt; today()
    )
  )
)</definition>
        <namedTypedValue>
            <name>requestId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>status</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>estFundingDate</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>triggerRefresh</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
