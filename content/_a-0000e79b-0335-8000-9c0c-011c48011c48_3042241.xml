    <rule>
        <name>AS_IO_UT_listOfSavesforRemoveDocumentRequirementActions</name>
        <uuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_3042241</uuid>
        <description>This rule is a list of saves when document requirement is removed</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>{
  a!localVariables(
    local!newRequirements: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
      cdt: ri!allrequiredDocuments,
      field: "requiredOnboardingDocumentId",
      value: null
    ),
    /*determine which tasks correspond to any selected new requirements*/
    local!taskIndexToBeRemoved: wherecontains(
      ri!selectedDocRequirements.requirementName,
      touniformstring(
        index(
          index(
            ri!docUploadTasks,
            "requiredDocument",
            null
          ),
          "requirementName",
          null
        )
      )
    ),
    if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!newRequirements),
      /*tasks associated with existing requirements will be removed later in process*/
      {},
      {
        a!save(
          ri!docUploadTasks,
          remove(
            ri!docUploadTasks,
            local!taskIndexToBeRemoved
          )
        ),
        a!save(
          ri!reviewTasks,
          remove(
            ri!reviewTasks,
            local!taskIndexToBeRemoved
          )
        )
      }
    )
  ),
  /*remove/update requirements*/
  a!save(
    ri!allrequiredDocuments,
    #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3042466"(
      selectedRequiredDocList: ri!selectedDocRequirements,
      requiredDocuments: ri!allrequiredDocuments
    )
  ),
  
  #"_a-0000e3c8-6861-8000-9ba2-011c48011c48_124304"(ri!selectedDocRequirements),
  #"_a-0000e3c8-6861-8000-9ba2-011c48011c48_124304"(ri!selectedValue)
}</definition>
