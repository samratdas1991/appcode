          ),
          if(
            or(
              #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(fv!item.documentStatusCode),
              #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(fv!item.customerDocument.docTypeCode)
            ),
            #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
              i18nData: ri!i18nData,
              bundleKey: "AS.CO.AllBundles.lbl_DocumentWithArugument",
              arguments: fv!index
            ),
            null
          )
        )
      ),
      
    ),
    ascending: true
  ),
  " , "
)</definition>
