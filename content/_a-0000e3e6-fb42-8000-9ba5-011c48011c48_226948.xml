        <definition>a!localVariables(
  local!i18nData: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81720"(
    bundleFolder: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_104931",
    bundleNames: "AS.CO.CommonObjects"
  ),
  /*if pagingInfo is passed, only use subset of choice values*/
  local!choiceValues: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(value: ri!pagingInfo),
    ri!choiceValues,
    todatasubset(
      arrayToPage: ri!choiceValues,
      pagingConfiguration: ri!pagingInfo
    ).data
  ),
  local!selectedItemLabelStyle: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!selectedItemLabelStyle,
    replacementValue: "PLAIN"
  ),
  local!iconSize: a!defaultValue(ri!iconSize, "MEDIUM"),
  local!labelSize: a!defaultValue(ri!labelSize, "MEDIUM"),
  {
    #"SYSTEM_SYSRULES_forEach"(
      items: local!choiceValues,
      expression: a!localVariables(
        local!isSelected: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73624"(
          value1: ri!value,
          value2: local!choiceValues[fv!index]
        ),
        local!hasLabel: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(index(ri!choiceLabels, fv!index, null)),
        local!itemIcon: index(ri!choiceIcons, fv!index, null),
        local!itemLink: index(ri!choiceLinks, fv!index, null),
        local!itemHovertext: index(ri!choiceHovertexts, fv!index, null),
        local!iconToShow: if(
          local!isSelected,
          a!defaultValue(
            value: ri!selectedIcon,
            default: local!itemIcon
          ),
          local!itemIcon
        ),
        #"SYSTEM_SYSRULES_cardLayout"(
          tooltip: local!itemHovertext,
          contents: {
            #"SYSTEM_SYSRULES_richTextDisplayField"(
              align: ri!alignLabels,
              labelPosition: "COLLAPSED",
              value: {
                #"SYSTEM_SYSRULES_richTextItem_v1"(
                  text: {
                    #"SYSTEM_SYSRULES_richTextIcon"(
                      icon: local!iconToShow,
                      size: local!iconSize
                    ),
                    if(local!hasLabel, " ", "")
                  },
                  showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(local!iconToShow)
                ),
                #"SYSTEM_SYSRULES_richTextItem_v1"(
                  text: ri!choiceLabels[a!defaultValue(
                    value: ri!pagingInfo.startIndex, default: 1
                  ) + fv!index - 1],
                  size: local!labelSize,
                  color: if(
                    local!isSelected,
                    ri!selectedLabelColor,
                    ri!labelColor
                  ),
                  showWhen: local!hasLabel,
                  style: if(
                    local!isSelected,
                    local!selectedItemLabelStyle,
                    "PLAIN"
                  )
                )
              }
            )
          },
          link: if(
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!itemLink),
            #"SYSTEM_SYSRULES_dynamicLink"(
              label: ri!choiceLabels[fv!index],
              value: local!choiceValues[fv!index],
              saveInto: ri!saveInto,
              showWhen: and(
                #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(ri!disabled),
                #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(local!isSelected)
              )
            ),
            local!itemLink
          ),
          accessibilityText: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: local!i18nData,
            bundleKey: "AS.CO.CommonObjects.acs_CardAsNavigation",
            arguments: ri!choiceLabels[fv!index]
          ) &amp; ri!additionalAccessibilityText,
          style: if(
            not(local!isSelected),
            /*not selected*/
            ri!cardStyle,
            ri!disabled,
            /*selected but disabled*/
            "STANDARD",
            /*selected*/
            a!defaultValue(
              ri!selectedCardStyle,
              #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(brandingKey: "AccentColor")
            )
          ),
          showBorder: false,
          height: "AUTO",
          padding: "LESS",
          marginBelow: "NONE"
        )
      )
    ),
    /*allow for user to adjust paging if applicable*/
    if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(value: ri!pagingInfo),
      {},
      #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1207543"(
        pagingInfo: ri!pagingInfo,
        totalCount: #"_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_880586"(ri!choiceValues),
        additionalSaveInto: {
          /*save initial value of new page as selected item*/
          a!save(
            ri!value,
            ri!choiceValues[ri!pagingInfo.startIndex]
          )
        },
        align: "RIGHT"
      )
    )
  }
)</definition>
