    <interface>
        <name>AS_CO_UT_displayComponentsInRows</name>
        <uuid>_a-0000e62c-7375-8000-9bec-011c48011c48_1752283</uuid>
        <description>Used to dynamically array multiple rows of columnar components</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_forEach"(
  items: enumerate(ceiling(length(ri!components) / ri!numberOfColumns)),
  expression: a!localVariables(
    /* 0 based */
    local!row: fv!item,
    #"SYSTEM_SYSRULES_columnsLayout"(
      columns: #"SYSTEM_SYSRULES_forEach"(
        items: enumerate(ri!numberOfColumns),
        expression: a!localVariables(
          /* 1 based */
          local!column: fv!item + 1,
          #"SYSTEM_SYSRULES_columnLayout"(
            contents: index(
              ri!components,
              local!row * ri!numberOfColumns + local!column,
              null
            )
          )
        )
      ),
      stackWhen: a!defaultValue(
        ri!stackWhen,
        #"_a-0000e528-bc07-8000-9bcf-011c48011c48_1050221"
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>numberOfColumns</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
