<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e673-a4a7-8000-9bef-011c48011c48_1962587</versionUuid>
    <rule>
        <name>AS_CO_I18N_UT_replaceArgument</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_91550</uuid>
        <description>Replaces a single argument symbol with an argument value in a display label</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Text',
  if(
    isnull(ri!input),
    ri!input,
    substitute(
      ri!input,
      /*The standard argument format is `[%x]` where `x` is the argument position*/
      concat("[%", ri!index, "]"),
      fn!index(ri!arguments, ri!index, "")
    )
  )
)</definition>
        <namedTypedValue>
            <name>input</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>arguments</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
