    <rule>
        <name>AS_CO_UT_createDateTimeQueryFilterValueFromDateRange</name>
        <uuid>_a-0000e40a-961e-8000-9ba7-011c48011c48_257043</uuid>
        <description>Creates a date/time filter value based on a date range, for use with a "between" operator</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Datetime?list',
  if(
    or(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!startDate),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!endDate)
    ),
    {},
    with(
      local!endDateToUse: ri!endDate + 1, 
      {
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75563"(date: ri!startDate, userHour: 0, userMinute: 0, userSecond: 0),
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75563"(date: local!endDateToUse, userHour: 0, userMinute: 0, userSecond: 0)
      }
    )
  )
)</definition>
        <namedTypedValue>
            <name>startDate</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>endDate</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
