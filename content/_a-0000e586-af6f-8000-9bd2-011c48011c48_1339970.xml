    <rule>
        <name>AS_IO_QE_getOnboardingTasksForCustomer</name>
        <uuid>_a-0000e586-af6f-8000-9bd2-011c48011c48_1339970</uuid>
        <description>AS IO GETDATA entry-point to get onboarding tasks for a customer</description>
        <parentUuid>_a-0000e586-af6f-8000-9bd2-011c48011c48_1321233</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e393-fbf1-8000-9ba2-011c48011c48_104148"(
  triggerRefresh: ri!triggerRefresh,
  taskStatusIds: if(
    ri!onlyOpenTasks,
    #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236",
    {
      #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236",
      #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603248"
    }
  ),
  requestId: ri!requestId,
  customerName: ri!customer.name,
  customerId: ri!customer.customerId,
  additionalFilters: {
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "taskBehaviorType.behaviorTypeCode",
      operator: "&lt;&gt;",
      value: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_99629"
    )
  },
  additionalLogicalExpressions: {
    #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "OR",
      filters: {
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "userAssignee",
          operator: "=",
          value: loggedInUser()
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "groupAssignee",
          operator: "=",
          value: ri!customer.groupId
        )
      }
    ),
    /*  If required document task and document has been uploaded then get non internal documents only  */
    #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "OR",
      filters: {
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "requiredDocument.onboardingDocument.customerDocument.isInternal",
          operator: "=",
          value: false()
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "requiredDocument.onboardingDocument.onboardingDocumentId",
          operator: "is null"
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "requiredDocument.requiredOnboardingDocumentId",
          operator: "is null"
        )
      }
    )
  },
  sort: { ri!pagingInfo.sort },
  returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290163"
)</definition>
