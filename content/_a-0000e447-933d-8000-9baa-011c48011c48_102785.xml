        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!alertDataSubset.data),
  /*empty state*/
  if(
    ri!alertStyle = #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2387746",
    #"_a-0000e777-b9f8-8000-9c06-011c48011c48_2828600"(
      icon: "bell-slash-o",
      label: "AS.FS.AllBundles.lbl_NoAlerts",
      size: "EXTRA_LARGE",
      color: "SECONDARY",
      i18nData: ri!i18nData
    ),
    #"_a-0000e777-b9f8-8000-9c06-011c48011c48_2828600"(
      icon: "thumbs-o-up",
      label: "AS.ALT.Alerts.lbl_NoOutstandingAlerts",
      size: "SMALL",
      color: "SECONDARY",
      i18nData: ri!i18nData
    )
  ),
  /*display alerts*/
  {
    #"SYSTEM_SYSRULES_cardLayout"(
      contents: {
        #"SYSTEM_SYSRULES_forEach"(
          items: ri!alertDataSubset.data,
          expression: if(
            ri!alertWrapperComponentStyle = "CARD_IN_SECTION",
            #"_a-0000e77d-004e-8000-9c08-011c48011c48_2837892"(
              contents: #"_a-0000e447-933d-8000-9baa-011c48011c48_103563"(
                i18nData: ri!i18nData,
                alert: fv!item,
                alertStyle: ri!alertStyle,
                allowDismissals: #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(ri!allowDismissals),
                shortenAlertMessagesToLength: ri!shortenAlertMessagesToLength,
                integrationCodes: ri!integrationCodes,
                showCustomer: ri!showCustomer,
                showCreatedBy: ri!showCreatedBy,
                showCreatedOn: ri!showCreatedOn
              )
            ),
            #"_a-0000e447-933d-8000-9baa-011c48011c48_103563"(
              i18nData: ri!i18nData,
              alert: fv!item,
              alertStyle: ri!alertStyle,
              allowDismissals: #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(ri!allowDismissals),
              integrationCodes: ri!integrationCodes,
              shortenAlertMessagesToLength: ri!shortenAlertMessagesToLength,
              showCustomer: ri!showCustomer,
              showCreatedBy: ri!showCreatedBy,
              showCreatedOn: ri!showCreatedOn
            )
          )
        ),
        #"SYSTEM_SYSRULES_cardLayout"(
          contents: #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1207543"(
            labelPosition: "COLLAPSED",
            align: "RIGHT",
            pagingInfo: ri!pagingInfo,
            totalCount: ri!alertDataSubset.totalCount
          ),
          marginBelow: "NONE",
          showBorder: false
        )
      },
      height: "AUTO",
      style: "NONE",
      padding: "NONE",
      marginBelow: "STANDARD",
      showBorder: false(),
      showShadow: true()
    )
  }
)</definition>
