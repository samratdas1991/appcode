    <rule>
        <name>AS_CO_UT_friendlyDateTime</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_76341</uuid>
        <description></description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!i18nData: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81720"(
    bundleFolder: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_104931",
    bundleNames: "AS.CO.CommonObjects"
  ),
  if(
    isnull(
      ri!value
    ),
    "",
    fn!with(
      local!diffInterval: now() - ri!value,
      if(
        local!diffInterval &lt; intervalds(
          0,
          0,
          0
        ),
        /* Time is in the future */
        if(
          ri!value &gt; (
            today() + 7
          ),
          /* a week or more in the future */
          text(
            ri!value,
            "mmm dd"
          ),
          ri!value &gt; (
            today() + 1
          ),
          /* after midnight today */
          text(
            ri!value,
            "dddd h:mm am/pm"
          ),
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: local!i18nData,
            bundleKey: "AS.CO.CommonObjects.lbl_TodayWithTime",
            arguments: text(
              ri!value,
              "h:mm am/pm"
            )
          )
        ),
        /* Else time is in the past */
        local!diffInterval &lt; intervalds(
          0,
          1,
          0
        ),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: local!i18nData,
          bundleKey: "AS.CO.CommonObjects.lbl_AMomentAgo"
        ),
        /* Else if */
        local!diffInterval &lt; intervalds(
          0,
          2,
          0
        ),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: local!i18nData,
          bundleKey: "AS.CO.CommonObjects.lbl_AMinuteAgo"
        ),
        /* Else if */
        local!diffInterval &lt; intervalds(
          1,
          0,
          0
        ),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: local!i18nData,
          bundleKey: "AS.CO.CommonObjects.lbl_MinutesAgo",
          arguments: minute(
            local!diffInterval
          )
        ),
        /* Else if */
        local!diffInterval &lt; intervalds(
          2,
          0,
          0
        ),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: local!i18nData,
          bundleKey: "AS.CO.CommonObjects.lbl_AnHourAgo"
        ),
        /* Else if */
        ri!value &gt; today(),
        /* after midnight today */
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: local!i18nData,
          bundleKey: "AS.CO.CommonObjects.lbl_HoursAgo",
          arguments: hour(
            local!diffInterval
          )
        ),
        /* Else if */
        ri!value &gt; (
          today() - 1
        ),
        /* after yesterday at midnight */
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: local!i18nData,
          bundleKey: "AS.CO.CommonObjects.lbl_YesterdayWithTime",
          arguments: text(
            ri!value,
            "h:mm am/pm"
          )
        ),
        /* Else if */
        ri!value &gt; (
          today() - 6
        ),
        /* fewer than 7 days ago */
        text(
          ri!value,
          "dddd h:mm am/pm"
        ),
        /* Else if */
        year(
          now()
        ) = year(
          ri!value
        ),
        /* this year */
        text(
          ri!value,
          "mmm dd"
        ),
        /* Else */
        text(
          ri!value,
          "mmm dd, yyyy"
        )
      )
    )
  )
)</definition>
