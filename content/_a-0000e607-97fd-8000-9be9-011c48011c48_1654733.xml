<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e607-97fd-8000-9be9-011c48011c48_1667988</versionUuid>
    <rule>
        <name>AS_CO_UT_filterCDTByGivenDateRange</name>
        <uuid>_a-0000e607-97fd-8000-9be9-011c48011c48_1654733</uuid>
        <description>Filter CDT by given date range</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  a!flatten(
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!cdt,
    expression: a!localVariables(
      local!date: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
        cdt: fv!item,
        field: ri!dateField,
        indexDefault: null
      ),
      if(
        and(
          local!date &gt;= ri!startDate,
          local!date &lt;= ri!endDate
        ),
        fv!item,
        {}
      )
    )
  )
))</definition>
        <namedTypedValue>
            <name>startDate</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>endDate</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>cdt</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>dateField</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
