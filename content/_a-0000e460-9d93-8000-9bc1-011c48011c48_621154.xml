  color: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(
    brandingKey: "WarningColor"
  ),
  size: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!size,
    replacementValue: "STANDARD"
  ),
  showWhen: or(
    #"_a-0000e471-623a-8000-9bc3-011c48011c48_740621"(
      triggerRefresh: ri!triggerRefresh,
      requestId: ri!requestId,
      status: ri!status,
      estFundingDate: ri!estFundingDate
    ),
    #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(ri!additionalShowWhen)
  )
)</definition>
