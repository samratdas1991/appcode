    <rule>
        <name>AS_QNM_UT_buildQuestions_RuntimeFromTemplate</name>
        <uuid>_a-0000e447-933d-8000-9baa-011c48011c48_44246</uuid>
        <description>Given a template question, builds a question by mapping the fields</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_forEach"(
  items: ri!questions_Template,
  expression: 'type!{urn:com:appian:types:AS:QNM}AS_QNM_Question'(
    questionId: if(ri!isPreview, fv!index, null),
    questionReferenceId: fv!item.questionReferenceId,
    questionTemplateId: fv!item.questionTemplateId,
    label: fv!item.label,
    instructions: fv!item.instructions,
    pageNumber: fv!item.pageNumber,
    groupNumber: fv!item.groupNumber,
    orderNumber: fv!item.orderNumber,
    responseOptions: #"_a-0000e447-933d-8000-9baa-011c48011c48_44252"(
      responses_Template: fv!item.responseOptions
    ),
    responseRequirements: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_597567"(
      responseRequirements_Template: fv!item.responseRequirements
    ),
    responses: #"_a-0000e447-933d-8000-9baa-011c48011c48_44252"(
      responses_Template: fv!item.defaultResponses
    ),
    isExternalUserFacing: fv!item.isExternalUserFacing,
    isDisplayedPrecedentSets: #"_a-0000e447-933d-8000-9baa-011c48011c48_44330"(
      questions_Template: ri!questions_Template,
      questionPrecedentSets_Template: fv!item.isDisplayedPrecedentSets
    ),
    isRequiredPrecedentSets: #"_a-0000e447-933d-8000-9baa-011c48011c48_44330"(
      questions_Template: ri!questions_Template,
      questionPrecedentSets_Template: fv!item.isRequiredPrecedentSets
    ),
    questionCategoryId: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
      nullableValue: wherecontains(
        fv!item.questionCategoryTemplateId,
        ri!questionCategories_Template.questionCategoryTemplateId
      ),
      replacementValue: 1
    ),
    questionTypeCode: fv!item.questionTypeCode
  )
)</definition>
        <namedTypedValue>
            <name>questions_Template</name>
            <type>
                <name>AS_QNM_T_Question?list</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>questionCategories_Template</name>
            <type>
                <name>AS_QNM_T_QuestionCategory?list</name>
