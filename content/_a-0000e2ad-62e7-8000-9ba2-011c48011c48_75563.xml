<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e62c-7375-8000-9bec-011c48011c48_1815941</versionUuid>
    <rule>
        <name>AS_CO_UT_convertDateToDateTime</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75563</uuid>
        <description>Converts a date to the dateTime, maintaining the date value, and allows for optional hour, minute, second parameters</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Datetime',
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!date),
    null,
    userdatetime(
      year(ri!date),
      month(ri!date),
      day(ri!date),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!userHour,
        replacementValue: 0
      ),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!userMinute,
        replacementValue: 0
      ),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!userSecond,
        replacementValue: 0
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>date</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>userHour</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>userMinute</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>userSecond</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
