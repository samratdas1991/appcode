    <typedValue>
        <type>
            <name>RuleTestConfig?list</name>
            <namespace>http://www.appian.com/ae/types/2009</namespace>
        </type>
        <value>
            <el>
                <a:name>Question Response Generation</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">"key1"</a:value>
                    <a:nameRef>fieldKey</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">{label: "lab1", value: "val1"}</a:value>
                    <a:nameRef>labelValuePairs</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
                    <a:expectedOutput xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nil="true"/>
                    <a:resultAssertions>#"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157603"(type: 'type!{urn:com:appian:types:AS:QNM}AS_QNM_Response', value: test!output) = 'type!{urn:com:appian:types:AS:QNM}AS_QNM_Response'(fieldKey: "key1", label: "lab1", value: "val1")</a:resultAssertions>
                </a:assertions>
            </el>
            <el>
                <a:name>Multiple Response Generation</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">"key1"</a:value>
                    <a:nameRef>fieldKey</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">{{label: "lab1", value: "val1"},{label: "lab2", value: "val2"}}</a:value>
                    <a:nameRef>labelValuePairs</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
                    <a:expectedOutput xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nil="true"/>
                    <a:resultAssertions>and(
  length(
    test!output
  ) = 2,
  contains(
    test!output,
    {
      'type!{urn:com:appian:types:AS:QNM}AS_QNM_Response'(
        fieldKey: "key1",
        label: "lab1",
        value: "val1"
      ),
      'type!{urn:com:appian:types:AS:QNM}AS_QNM_Response'(
        fieldKey: "key1",
        label: "lab2",
        value: "val2"
      )
    }
  )
)</a:resultAssertions>
                </a:assertions>
            </el>
        </value>
