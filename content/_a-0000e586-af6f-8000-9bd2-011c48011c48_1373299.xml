    <rule>
        <name>AS_IO_UT_filterToCustomerTasks</name>
        <uuid>_a-0000e586-af6f-8000-9bd2-011c48011c48_1373299</uuid>
        <description></description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!flatten(
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!allTasks,
    expression: if(
      or(
        and(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(fv!item.userAssignee),
          isusernametaken(fv!item.userAssignee),
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_114114"(
            user: tostring(fv!item.userAssignee),
            groups: tointeger(#"_a-0000e539-6d59-8000-9bd0-011c48011c48_1296974")
          )
        ),
        and(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(fv!item.groupAssignee),
          /* TOBEDONE Should include a group existence check for completeness */
          group(fv!item.groupAssignee, "parentId") = tointeger(#"_a-0000e539-6d59-8000-9bd0-011c48011c48_1296974")
        )
      ),
      fv!item,
      {}
    )
  )
)</definition>
        <namedTypedValue>
            <name>allTasks</name>
            <type>
                <name>AS_IO_Task?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
