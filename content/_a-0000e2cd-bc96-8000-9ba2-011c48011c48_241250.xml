        <definition>a!localVariables(
  local!nextTaskRef: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_113793"(
    taskRefId: #"_a-0000e3c8-6861-8000-9ba2-011c48011c48_123596",
    returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157"
  ),
  local!updatedResultsTask: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
    cdt: #"8c731acd-3941-41e1-b06f-452fd89ab48b"(
      tasks: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_193601"(taskRef: local!nextTaskRef)
    ),
    fields: {
      "dueDate",
      "slaDays",
      "slaRule",
      "request"
    },
    newValues: {
      ri!consolidatedScreeningTask.dueDate,
      ri!consolidatedScreeningTask.slaDays,
      ri!consolidatedScreeningTask.slaRule,
      ri!consolidatedScreeningTask.request
    }
  ),
  /*Above update cdt rule doesnt work for taskPrecedents as they are multiple so used below*/
  #"_a-0000e40a-de32-8000-9ba8-011c48011c48_269170"(
    cdt: cast(
      'type!{urn:com:appian:types:AS:IO}AS_IO_Task',
      local!updatedResultsTask
    ),
    fieldValuePairs: {
      field: "taskPrecedents",
      value: 'type!{urn:com:appian:types:AS:IO}AS_IO_Task_Precedent'(
        taskId_precedent: ri!consolidatedScreeningTask.taskId
      )
    }
  )
)</definition>
