    <interface>
        <name>AS_CO_CP_multipleDropdownField_Internal</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74386</uuid>
        <description>Generic multi select dropdown field that handles read-only logic dynamically. 

For instances where a custom value should be displayed when the component is read-only, pass it into the "readOnlyValue" input.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!displayValues: joinarray(
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!value,
      expression: displayvalue(
        fv!item,
        ri!choiceValues,
        ri!choiceLabels,
        null
      )
    ),
    ", "
  ),
  local!i18nData: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81720"(
    bundleFolder: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_104931",
    bundleNames: "AS.CO.CommonObjects"
  ),
  local!choiceLabelValueDict: a!flatten(
    {
      #"SYSTEM_SYSRULES_forEach"(
        items: merge({ ri!choiceLabels }, { ri!choiceValues }),
        expression: {
          choiceLabels: tostring(fv!item[1]),
          choiceValues: fv!item[2]
        }
      )
    }
  ),
  local!choiceLabelValueDictSorted: if(
    ri!skipSorting = true(),
    local!choiceLabelValueDict,
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75720"(
      cdt: local!choiceLabelValueDict,
      field: "choiceLabels",
      ascending: true
    )
  ),
  local!choiceLabels: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76367"(
    array: {
      a!flatten(
        index(local!choiceLabelValueDictSorted, "choiceLabels", {})
      )
    }
  ),
  local!choiceValues: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76367"(
    array: {
      a!flatten(
        index(local!choiceLabelValueDictSorted, "choiceValues", {})
      )
    }
  ),
  if(
    ri!readOnly,
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74131"(
      label: ri!label,
      instructions: ri!instructions,
      value: if(
        isnull(ri!readOnlyValue),
        local!displayValues,
        ri!readOnlyValue
      ),
      labelPosition: ri!labelPosition,
      helpTooltip: ri!helpTooltip,
      showWhen: ri!showWhen,
      accessibilityText: ri!accessibilityText
    ),
    #"SYSTEM_SYSRULES_multipleDropdownField_v1"(
      label: ri!label,
      labelPosition: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73568"(
        label: ri!label,
        labelPosition: ri!labelPosition,
        readOnly: ri!readOnly
      ),
      instructions: ri!instructions,
      helpTooltip: ri!helpTooltip,
      placeholder: if(
        ri!noPlaceholderLabel,
        null,
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74260"(
          wrapInMarkers: #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(boolean: ri!wrapInMarkers),
          placeholder: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
            nullableValue: ri!placeholderLabel,
            replacementValue: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
              i18nData: local!i18nData,
              bundleKey: "AS.CO.CommonObjects.lbl_SelectValue"
            )
          )
        )
      ),
      choiceLabels: local!choiceLabels,
      choiceValues: local!choiceValues,
      value: ri!value,
      saveInto: ri!saveInto,
      showWhen: ri!showWhen,
      required: ri!required,
      requiredMessage: ri!requiredMessage,
      disabled: ri!disabled,
      validations: ri!validations,
      validationGroup: ri!validationGroup,
      accessibilityText: ri!accessibilityText
    )
  )
)</definition>
