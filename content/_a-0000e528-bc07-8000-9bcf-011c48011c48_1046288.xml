          null,
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: ri!i18nData,
            bundleKey: "AS.CO.AllBundles.vld_PleaseCorrectFieldValidationErrors",
            arguments: joinarray(local!invalidSectionNames, ", ")
          )
        ),
        if(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75483"(array: ri!products.name, skipNulls: true),
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: ri!i18nData,
