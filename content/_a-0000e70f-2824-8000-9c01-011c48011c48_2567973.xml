<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2605527</versionUuid>
    <rule>
        <name>AS_FS_ALT_BL_EvaluateDomicileTrigger</name>
        <uuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2567973</uuid>
        <description>Expression rule that evaluates whether a domicile trigger condition has been met</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!operator: #"_a-0000e70f-2824-8000-9c01-011c48011c48_2539235"(
    json: ri!configurationTriggerSettings,
    field: "operator",
    indexDefault: 1
  ),
  local!countryCodes: split(
    #"_a-0000e70f-2824-8000-9c01-011c48011c48_2539235"(
      json: ri!configurationTriggerSettings,
      field: "countryCodes",
      indexDefault: 1
    ),
    ","
  ),
  local!customer: if(
    ri!isOriginalObjectEvaluation,
    ri!auditParameters.originalObjects,
    ri!auditParameters.newObjects
  ),
  local!isCustomerDomicileInListOfCountryCodes: contains(
    local!countryCodes,
    local!customer.domicile.countryCode
  ),
  if(
    local!operator = #"AS_FS_ALT_OPERATOR_CODE_IN",
    local!isCustomerDomicileInListOfCountryCodes,
    local!operator = #"AS_FS_ALT_OPERATOR_CODE_NOT_IN",
    not(local!isCustomerDomicileInListOfCountryCodes),
    error("Invalid domicile operator in triggerConditionId: " &amp; ri!triggerConditionId)
  )
)</definition>
        <namedTypedValue>
            <name>configurationTriggerSettings</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>isOriginalObjectEvaluation</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>triggerConditionId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>auditParameters</name>
            <type>
                <name>AS_ADT_P_AuditProcessParameters</name>
                <namespace>urn:com:appian:types:AS:ADT</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
