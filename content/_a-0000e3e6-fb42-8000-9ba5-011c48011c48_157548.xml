    <rule>
        <name>AS_CO_UT_checkIfSingleType</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157548</uuid>
        <description>Returns true if the `type` or `typeOf` inputs represent a singular, non-array type</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Boolean',
  with(
    local!typeNumber: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157503"(
      type: ri!type,
      typeOf: ri!typeOf
    ),
  
    and(
      /*The type of the input equals the type of the input returned as single type*/
      local!typeNumber = #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157503"(
        typeOf: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75842"(typeOf: cast(local!typeNumber, null))
      ),
      
      /*And the type of the input is not list of variant*/
      local!typeNumber &lt;&gt; #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157503"(
        typeOf: {}
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>type</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
