    <rule>
        <name>AS_IO_UT_constructDocRequirementName</name>
        <uuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_3039557</uuid>
        <description>Rule to construct document requirement name if requirement name is null</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!currentDocRequirement.docTypeCode),
  "",
  a!localVariables(
    local!countOfDuplicateDocs: count(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
        cdt: ri!activeDocumentRequirements,
        field: "docTypeCode",
        value: ri!currentDocRequirement.docTypeCode
      )
    ) + 1,
    concat(
      #"_a-0000e438-fdf5-8000-9ba8-011c48011c48_21856"(
        i18nData: ri!i18nData,
        labelField: "label",
        singleData: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
          cdt: ri!documentTypes,
          field: "code",
          value: ri!currentDocRequirement.docTypeCode
        )
      ),
      if(
        local!countOfDuplicateDocs = 0,
        "",
        /*   local!countOfDuplicateDocs + 1 -&gt; Because we dont want to append 1 when it is a second requirement of a  type that already exists. It should be 2,3,etc     */
        concat(char(32), local!countOfDuplicateDocs)
      )
    )
  )
)</definition>
