            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  filter(
    #"_a-0000e69f-3688-8000-9bf2-011c48011c48_2133205"(value: ri!value, field: ri!field, cdt: _),
    a!flatten(ri!cdt)
  )
)</definition>
