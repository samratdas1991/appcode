        <name>AS_CO_UT_filterCdtByFieldContains</name>
        <uuid>_a-0000e69f-3688-8000-9bf2-011c48011c48_2132891</uuid>
        <description>Filters a CDT where a value is included in a nested list within a list of cdts</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
