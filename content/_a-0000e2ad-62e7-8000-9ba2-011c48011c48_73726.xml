        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73726</uuid>
        <description>Returns a formatted user name or group name.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Text',
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!groupOrUser),
    "",
    if(
      runtimetypeof(ri!groupOrUser) = 'type!{http://www.appian.com/ae/types/2009}Group',
      group(togroup(ri!groupOrUser), "groupName"),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73660"(
        i18nData: ri!i18nData,
        user: ri!groupOrUser
      )
    )
  )
)</definition>
