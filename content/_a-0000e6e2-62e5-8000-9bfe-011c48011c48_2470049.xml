    <interface>
        <name>AS_FS_UT_DisplayOpenEventsTag</name>
        <uuid>_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2470049</uuid>
        <description>Displays a tag with the number of open events in effect for a field/object, along with hovertext showing the associated event unique identifiers</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  /*Multiple in case this applied to a list of objects*/
  local!uniqueIdentifiers: a!flatten(
    apply(
      #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2469934"(
        pendingObjectDisplay: _,
        field: ri!field,
        displayType: #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2469960",
        applyFieldFilter: ri!applyFieldFilter
      ),
      ri!pendingObjectDisplays
    )
  ),
  local!isPendingDeletion: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
    ri!pendingObjectDisplays.objectForDisplay
  ),
  #"_a-0000e607-97fd-8000-9be9-011c48011c48_1713562"(
    showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(local!uniqueIdentifiers),
    tooltip: joinarray(local!uniqueIdentifiers, ", "),
    color: if(
      local!isPendingDeletion,
      "NEGATIVE",
      ""/*default*/
      
    ),
    tagText: upper(
      if(
        local!isPendingDeletion,
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.FS.AllBundles.txt_PendingDeletion"
        ),
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2407133"(
            singularLabel: "AS.FS.AllBundles.txt_OpenEvent",
            pluralLabel: "AS.FS.AllBundles.txt_OpenEvents",
            value: length(local!uniqueIdentifiers)
          ),
          arguments: length(local!uniqueIdentifiers)
        )
      )
    )
  )
)</definition>
