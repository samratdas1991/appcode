    <rule>
        <name>AS_CO_UT_displayUser</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73660</uuid>
        <description>Returns a display name for a user in the format of "firstName lastName". Null-safe.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!integration: a!refreshVariable(
    value: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
      cdt: #"_a-0000e6ca-7e19-8000-9bfb-011c48011c48_2234155"(i18nData: ri!i18nData),
      field: "integrationCode",
      value: ri!user
    ),
    refreshAlways: true
  ),
  cast(
    'type!{http://www.appian.com/ae/types/2009}Text',
    if(
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81749"(user: ri!user),
      /* TOBEDONE enhance to support other naming display styles, e.g. Korean? */
      #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_985828"(
        firstName: fn!user(ri!user, "firstName"),
        lastName: fn!user(ri!user, "lastName")
      ),
      if(
        and(
          ri!showHyphenIfValueNull = true,
          isnull(ri!user)
        ),
        "-",
        a!defaultValue(
          value: local!integration.integrationLabel,
          default: if(
            ri!user = #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366",
            /*Old code before i18n was included in this rule normally, should be refactored and passed in over time*/
            if(
              #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!i18nData),
              #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_84411"(
                bundleFolder: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_104931",
                bundleKey: "AS.CO.CommonObjects.lbl_SystemUser"
              ),
              #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
                i18nData: ri!i18nData,
                bundleKey: "AS.CO.CommonObjects.lbl_SystemUser"
              )
            ),
            ri!user
          )
        )
      )
    )
  )
)</definition>
