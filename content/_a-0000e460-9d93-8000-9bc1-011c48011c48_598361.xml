    <rule>
        <name>AS_QNM_VD_evaluateResponseRequirement_MAX_CHAR_LENGTH</name>
        <uuid>_a-0000e460-9d93-8000-9bc1-011c48011c48_598361</uuid>
        <description>Used to evaluate response requirements of type MAX_CHAR_LENGTH</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  ri!returnValidationMessage,
  /* Max character length requirements are handled directly in text based CPS objects, so we return no validation here */
  {""},
  len(ri!responses.value) &lt;= ri!responseRequirement.requirementValue 
)</definition>
