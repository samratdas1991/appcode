    <rule>
        <name>AS_IO_VLD_requiredFieldsForRequiredDocuments</name>
        <uuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_3041041</uuid>
        <description>Rule to get if all the requiredFields are added for document requirement</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76367"(
  array: #"SYSTEM_SYSRULES_forEach"(
    items: ri!newDocumentRequirements,
    expression: if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
        index(
          ri!documentRequirements,
          wherecontains(
            touniformstring(fv!item.requirementName),
            touniformstring(
              index(
                ri!documentRequirements,
                "requirementName",
                null
              )
            )
          ),
          null
        ).onboardingDocument
      ),
      a!localVariables(
        local!relatedDocUploadTask: index(ri!docUploadTasks, fv!index, null),
        local!relatedReviewTask: index(ri!reviewTasks, fv!index, null),
        local!validation: a!refreshVariable(
          value: or(
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!relatedDocUploadTask.dueDate),
            local!relatedDocUploadTask.dueDate &lt; today(),
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
              local!relatedDocUploadTask.groupAssignee
            ),
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(local!relatedReviewTask.groupAssignee)
          ),
          refreshOnVarChange: ri!triggerRefresh
        ),
        if(
          local!validation,
          fv!item.requirementName,
          null
        )
      ),
      if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
          index(
            ri!documentRequirements,
            wherecontains(
              touniformstring(fv!item.requirementName),
              touniformstring(
                index(
                  ri!documentRequirements,
                  "requirementName",
                  null
                )
              )
            ),
            null
          ).onboardingDocument.customerDocument
        ),
        fv!item.requirementName,
        null
      )
    )
  )
)</definition>
