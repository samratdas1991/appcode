    <rule>
        <name>AS_CO_UT_groupsByName</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_253723</uuid>
        <description>Executes the `a!groupsByName()` function with error handling for a null input</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Group?list',
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
      ri!groupName
    ),
    {},
    a!groupsByName(
      ri!groupName
    )
  )
)</definition>
        <namedTypedValue>
            <name>groupName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
