        <definition>#"SYSTEM_SYSRULES_richTextDisplayField"(
  value: {
    #"SYSTEM_SYSRULES_richTextItem_v1"(
      text: {
        #"SYSTEM_SYSRULES_richTextItem_v1"(
          #"SYSTEM_SYSRULES_richTextIcon"(icon: ri!icon, color: ri!color)
        ),
        #"SYSTEM_SYSRULES_richTextItem_v1"(
          text: if(
            and(
              #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!icon),
              #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!label)
            ),
            " ",
            null
          ) &amp; ri!label,
          color: ri!color
        )
      },
      link: #"SYSTEM_SYSRULES_dynamicLink"(
        saveInto: ri!saveinto,
        showWhen: ri!showLink
      ),
      linkStyle: "STANDALONE"
    )
  },
  showWhen: ri!showWhen
)</definition>
