    <rule>
        <name>AS_IO_UI_taskRefTaskTypeComponents</name>
        <uuid>_a-0000e586-af6f-8000-9bd2-011c48011c48_1355279</uuid>
        <description>Returns a map of lists of components to be array in columns while creating/modifying a reference task</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!isExistingTaskRef: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!taskRef.taskRefId),
  a!map(
    taskTypeDropdown: {
      #"_a-0000e528-bc07-8000-9bcf-011c48011c48_1027802"(
        label: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.lbl_TaskType"
        ),
        labelPosition: "ABOVE",
        required: true,
        placeholderLabel: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.lbl_Type"
        ),
        /*Users should not be able to edit the type of system tasks*/
        readOnly: local!isExistingTaskRef,
        choiceLabels: #"SYSTEM_SYSRULES_forEach"(
          items: ri!taskBehaviorTypes,
          expression: #"_a-0000e438-fdf5-8000-9ba8-011c48011c48_21856"(
            singleData: fv!item,
            labelField: "behaviorDisplayName",
            i18nData: ri!i18nData
          )
        ),
        choiceValues: ri!taskBehaviorTypes,
        value: ri!taskRef.taskBehaviorType,
        validations: {},
        saveInto: {
          ri!taskRef.taskBehaviorType,
          a!save(
            target: ri!taskRef.docUploadContext,
            value: null
          ),
          a!save(
            target: ri!taskRef.docSignatureContext,
            value: null
          )
        }
      )
    },
    taskTypeSpecificContents: {
      #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_81756"(
        i18nData: ri!i18nData,
        refData: ri!refData,
        showWhen: contains(
          {
            #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_99647",
            #"_a-0000e586-af6f-8000-9bd2-011c48011c48_1347796"
          },
          ri!taskRef.taskBehaviorType.behaviorSubtypeCode
        ),
        labelPosition: "ABOVE",
        required: true,
        selectedTypeCode: if(
          #"_a-0000e586-af6f-8000-9bd2-011c48011c48_1347796" = ri!taskRef.taskBehaviorType.behaviorSubtypeCode,
          ri!taskRef.docSignatureContext.documentTypeCode,
          ri!taskRef.docUploadContext.docTypeCode
        )
      ),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74302"(
        label: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.lbl_DocumentToBeSigned"
        ),
        showWhen: #"_a-0000e586-af6f-8000-9bd2-011c48011c48_1347796" = ri!taskRef.taskBehaviorType.behaviorSubtypeCode,
        value: ri!taskRef.docSignatureContext.unsignedDocumentId,
        saveInto: ri!taskRef.docSignatureContext.unsignedDocumentId,
        target: #"_a-0000e586-af6f-8000-9bd2-011c48011c48_1354903",
        maxSelections: 1,
        maxDocSize: "25 MB",
        required: true,
        allUploadedDocuments: ri!allUploadedDocuments,
        allowedFileExtensions: #"_a-0000e52f-5316-8000-9bac-011c48011c48_290777"()
      )
    }
  )
)</definition>
