    <rule>
        <name>AS_FS_UT_SearchGroupName</name>
        <uuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2659014</uuid>
        <description>Given a group:groupName pair and searchText, returns whether the searchText exists in the groupName</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e70f-2824-8000-9c01-011c48011c48_2658889"(
    searchText: ri!searchText,
    withinText: ri!groupName
  ),
  ri!group,
  {}
)</definition>
        <namedTypedValue>
            <name>group</name>
