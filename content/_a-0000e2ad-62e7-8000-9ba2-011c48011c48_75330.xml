        </type>
        <value>
            <el>
                <a:name>Example</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">{
  {a: 1, b: 1},
  {a: 2, b: 2},
  {a: 2, b: 3}
}</a:value>
                    <a:nameRef>cdt</a:nameRef>
