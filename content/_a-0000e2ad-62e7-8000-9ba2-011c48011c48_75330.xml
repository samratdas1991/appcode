<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e430-f270-8000-9bbc-011c48011c48_372510</versionUuid>
    <rule>
        <name>AS_CO_UT_cdtDistinctOnField</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75330</uuid>
        <description>Returns only distinct CDT indices on a single field, returning the first index where there are multiples of the same value.

i.e. {{a: 1, b: 2}, {a: 1, b: 4}, {a: 5, b: 4}}, distinct on field "a" would return {{a: 1, b: 2}, {a: 5, b: 4}}</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!cdt),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!field)
  ),
  ri!cdt,
  with(
    
    local!distinctIndices: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73758"(
      reduce(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75287"(
          cdtIndices: _,
          cdt: ri!cdt,
          singleCdt: _,
          fields: ri!field
        ),
        {},
        ri!cdt
      )
    ),
    
    index(ri!cdt, local!distinctIndices, {})
    
  )
)</definition>
        <namedTypedValue>
            <name>cdt</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>field</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
