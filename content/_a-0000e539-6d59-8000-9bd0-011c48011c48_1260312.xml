    <rule>
        <name>AS_FS_IDP_isUploadedDocUnsupported</name>
        <uuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1260312</uuid>
        <description>checks if the doc is over the size limit for training or understanding and that the files are pdf files</description>
        <parentUuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1261631</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>or(
  if(
    ri!isTraining,
    #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1260285"(
      sizeInBytes: fn!document(ri!document, "size")
    ),
    #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1260305"(
      sizeInBytes: fn!document(ri!document, "size")
    )
  ),
  #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1266967"(document: ri!document),
  like(fn!document(ri!document, "name"), "._*")
)</definition>
        <namedTypedValue>
            <name>document</name>
            <type>
                <name>CollaborationDocument</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>isTraining</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
