<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e430-f270-8000-9bbc-011c48011c48_371778</versionUuid>
    <rule>
        <name>AS_CO_UT_formatDocumentAndExtension</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_170600</uuid>
        <description>Formats a document with its extension, leaving out the . if the extension is null</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Text',
  concat(
    ri!documentName,
    if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!extension),
      "",
      concat(
        ".",
        ri!extension
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>documentName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>extension</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
