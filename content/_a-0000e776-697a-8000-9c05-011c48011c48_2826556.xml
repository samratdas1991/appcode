    <interface>
        <name>AS_CO_GRD_gridViewForCharts</name>
        <uuid>_a-0000e776-697a-8000-9c05-011c48011c48_2826556</uuid>
        <description>grid that displays charts information - use AS_CO_GRD_gridViewForPieCharts for pie charts</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(ri!showWhen),
  a!localVariables(
    local!data: a!refreshVariable(
      value: #"SYSTEM_SYSRULES_forEach"(
        items: ri!categories,
        expression: a!localVariables(
          local!categoryIndex: fv!index,
          a!map(
            category: fv!item,
            data: #"SYSTEM_SYSRULES_forEach"(
              items: ri!chartSeries,
              expression: a!map(
                label: fv!item.label,
                data: a!defaultValue(
                  value: fv!item.data[local!categoryIndex],
                  default: "-"
                ),
                links: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76367"(
                  array: index(fv!item.links, local!categoryIndex, null)
                )
              )
            )
          )
        )
      ),
      refreshAlways: true
    ),
    #"SYSTEM_SYSRULES_gridField_v1"(
      label: ri!gridLabel,
      labelPosition: "COLLAPSED",
      data: local!data,
      columns: {
        #"SYSTEM_SYSRULES_gridColumn"(
          label: ri!columnHeading,
          value: fv!row.category
        ),
        #"SYSTEM_SYSRULES_forEach"(
          items: ri!chartSeries,
          expression: #"SYSTEM_SYSRULES_gridColumn"(
            label: fv!item.label,
            value: a!localVariables(
              local!value: a!refreshVariable(
                value: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
                  cdt: fv!row.data,
                  field: "label",
                  value: fv!item.label
                ),
                refreshAlways: true
              ),
              #"SYSTEM_SYSRULES_richTextDisplayField"(
                value: #"SYSTEM_SYSRULES_richTextItem_v1"(
                  text: local!value.data,
                  link: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
                    nullableValue: local!value.links,
                    replacementValue: null
                  ),
                  linkStyle: "STANDALONE"
                )
              )
            )
          )
        )
      }
    )
  ),
  {}
)</definition>
