            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!displayOptions: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_187421"(
    value: ri!removalType,
    inArray: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_186261"(),
    replacement: {
      /* CLEAR */
      {
        icon: "times",
        color: "NEGATIVE"
      },
      /* DELETE */
      {
        icon: "trash",
        color: "ACCENT"
      }
    },
    /* Default to CLEAR behavior */
    firstIsDefault: true
  ),
  #"SYSTEM_SYSRULES_richTextDisplayField"(
    labelPosition: "COLLAPSED",
    value: #"SYSTEM_SYSRULES_richTextIcon"(
      icon: local!displayOptions.icon,
      size: ri!size,
      caption: ri!caption,
      link: #"SYSTEM_SYSRULES_dynamicLink"(
        saveInto: {
          ri!saveInto
        },
        showWhen: ri!disable &lt;&gt; true
      ),
      linkStyle: "STANDALONE",
      color: if(
        ri!disable,
        "SECONDARY",
        local!displayOptions.color
      )
    ),
    align: "CENTER",
    showWhen: ri!showWhen
  )
)</definition>
