    <versionUuid>_a-0000e438-fd8a-8000-9bbe-011c48011c48_422933</versionUuid>
    <rule>
        <name>AS_IO_UT_createTaskAssigneeFieldLogicalExpression</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_236006</uuid>
        <description>Returns a logical expression for the filter against the assignee fields of a task (userAssignee &amp; groupAssignee)</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!username: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!username,
    replacementValue: loggedInUser()
  ),
  local!isUserManager: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_236057"(
    username: local!username
  ),
  #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_187421"(
    value: ri!assigneeTypeFilter,
    inArray: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_217594"().value,
    replacement: {
      /* All -- only filter if we are applying some type of group filter */
      if(
        ri!applyGroupFilter,
        #"SYSTEM_SYSRULES_queryLogicalExpression"(
          operator: "OR",
          logicalExpressions: {
            #"SYSTEM_SYSRULES_queryLogicalExpression"(
              operator: "AND",
              filters: {
                #"SYSTEM_SYSRULES_queryFilter"(
                  field: "groupAssignee",
                  operator: "in",
                  value: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_249947"(
                    username: local!username
                  )
                ),
                /*Only restrict to unassigned if they are not a manager*/
                #"SYSTEM_SYSRULES_queryFilter"(
                  field: "userAssignee",
                  operator: "=",
                  value: " ",
                  applyWhen: not(local!isUserManager)
                )
              }
            ),
            #"SYSTEM_SYSRULES_queryLogicalExpression"(
              operator: "AND",
              filters: #"SYSTEM_SYSRULES_queryFilter"(
                field: "userAssignee",
                operator: "=",
                value: local!username
              )
            )
          }
        ),
        null
      ),
      /* Assigned to me */
      #"SYSTEM_SYSRULES_queryLogicalExpression"(
        operator: "AND",
        filters: {
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "userAssignee",
            operator: "=",
            value: local!username
          )
        }
      ),
      /* Unassigned */
      #"SYSTEM_SYSRULES_queryLogicalExpression"(
        operator: "AND",
        filters: {
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "groupAssignee",
            operator: "in",
            value: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_249947"(
              username: local!username
            ),
            applyWhen: ri!applyGroupFilter
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "userAssignee",
            operator: "=",
            value: " "
          )          
        }
      )      
    },
    customDefault: {}
  )
)</definition>
        <namedTypedValue>
            <name>assigneeTypeFilter</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>applyGroupFilter</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>username</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
