    <rule>
        <name>AS_QNM_VD_evaluateResponseRequirement</name>
        <uuid>_a-0000e460-9d93-8000-9bc1-011c48011c48_596441</uuid>
        <description>Used to generically evaluate all types of response requirements</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  isnull(
    ri!responseRequirement
  ),
  "",
  with(
    local!evaluationRule: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_597333"(
      responseRequirement: ri!responseRequirement
    ).evaluationRule,
    local!displayedResponses: if(
      index(
        ri!questionMetadata.fieldMetadata.showWhen,
        wherecontains(
          ri!responseRequirement.fieldKey,
          ri!questionMetadata.fieldMetadata.fieldKey
        )[1],
        false
      ),
      #"_a-0000e447-933d-8000-9baa-011c48011c48_31196"(
        question: ri!question,
        fieldKey: ri!responseRequirement.fieldKey
      ),
      {}
    ),
    local!evaluationRule(
      responses: local!displayedResponses,
      responseRequirement: ri!responseRequirement,
      returnValidationMessage: ri!returnValidationMessage
    )
  )
)</definition>
        <namedTypedValue>
            <name>question</name>
            <type>
