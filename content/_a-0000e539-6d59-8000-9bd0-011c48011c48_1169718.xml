      returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290169",
      type: ri!onboardingTypeCodeFilter,
      modifiedDate: ri!dateFilter,
      modifiedDateFilterOperator: "&gt;=",
      aggregation: #"SYSTEM_SYSRULES_queryAggregation"(
        aggregationColumns: {
          #"SYSTEM_SYSRULES_queryAggregationColumn"(
            field: "fundingDateStatus",
            isGrouping: true
          ),
          #"SYSTEM_SYSRULES_queryAggregationColumn"(
            field: "requestId",
            alias: "count",
            aggregationFunction: "COUNT"
          )
        }
      )
    ),
    field: "count",
    ascending: false,
    nullsFirst: true
  ),
  local!fundingStatusRefData: #"_a-0000e471-623a-8000-9bc3-011c48011c48_791888"(i18nData: ri!i18nData),
  local!chartColorScheme: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1924695"(brandingKey: "ChartColorScheme"),
  local!chartSeries: a!refreshVariable(
    value: #"SYSTEM_SYSRULES_forEach"(
      items: local!pieChartData,
      expression: a!localVariables(
        local!fundingDateStatus: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
          cdt: local!fundingStatusRefData,
          field: "code",
          value: fv!item.fundingDateStatus
        ),
        #"SYSTEM_SYSRULES_chartSeries"(
          label: local!fundingDateStatus.label,
          data: fv!item.count,
          links: #"SYSTEM_SYSRULES_dynamicLink"(
            value: true,
            saveInto: {
              a!save(
                ri!selectedOnboardingFundingDateStatus,
                local!fundingDateStatus
              ),
              a!save(
                ri!drilldownColor,
                #"_a-0000e65f-acc5-8000-9bb8-011c48011c48_412542"(
                  colorSchemeValue: local!chartColorScheme,
                  colorIndex: fv!index,
                  totalIndexes: fv!itemCount,
                  isPieChart: true()
                )
              ),
              ri!hideOtherCharts,
              ri!showCurrentChartGrid
            }
          )
        )
      )
    ),
    refreshAlways: true()
  ),
  {
    #"_a-0000e7e5-dad4-8000-9c14-011c48011c48_3312448"(
      "AS_IO_CHT_pieChartForCompletedOnboardings"
    ),
    if(
      ri!showGrid,
      #"_a-0000e79b-0335-8000-9c0c-011c48011c48_2983833"(
        gridLabel: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.lbl_OnboardingsByStatus"
        ),
        chartSeries: local!chartSeries,
        columnHeading: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.lbl_Status"
        ),
        showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(ri!hideOtherCharts),
        countColumnLabel: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.lbl_Count"
        )
      ),
      #"SYSTEM_SYSRULES_pieChartField"(
        label: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.lbl_OnboardingsByStatus"
        ),
        labelPosition: "COLLAPSED",
        colorScheme: #"_a-0000e65f-acc5-8000-9bb8-011c48011c48_409039"(
          colorSchemeValue: local!chartColorScheme,
          returnRawHexCodes: false
        ),
        series: local!chartSeries,
        seriesLabelStyle: "LEGEND",
        showDataLabels: false,
        showtooltips: true,
        style: "DONUT",
        height: "SHORT",
        showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(ri!hideOtherCharts)
      )
    )
  }
)</definition>
