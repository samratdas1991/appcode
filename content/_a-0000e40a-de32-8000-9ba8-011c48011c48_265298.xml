    <rule>
        <name>AS_CO_UT_filterRefDataForSelection</name>
        <uuid>_a-0000e40a-de32-8000-9ba8-011c48011c48_265298</uuid>
        <description>Rule to filter out ref data</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!filteredRefData: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81482"(
    cdt: ri!data,
    fields: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76367"(
      array: { "isActive", ri!additionalFilterField }
    ),
    values: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76367"(array: { true, ri!additionalFilterValue })
  ),
  local!selectedData: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81482"(
    cdt: ri!data,
    fields: if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!additionalFilterField),
      { ri!valueField },
      { ri!valueField, ri!additionalFilterField }
    ),
    values: if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!additionalFilterField),
      { ri!currentValue },
      {
        ri!currentValue,
        ri!additionalFilterValue
      }
    )
  ),
  /*Does not refresh to ensure that previously-selected values do not disappear while on a form*/
  local!initiallySelectedValue: a!refreshVariable(
    value: local!selectedData,
    refreshOnReferencedVarChange: false
  ),
  local!selectableReferenceData: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75330"(
    cdt: {
      local!filteredRefData,
      /* In case the options are filtered while something is selected */
      local!selectedData,
      /* So that the user can always return to the initial value */
      local!initiallySelectedValue
    },
    field: ri!valueField
  ),
  local!selectableReferenceDataLabelUpdate: #"SYSTEM_SYSRULES_forEach"(
    items: local!selectableReferenceData,
    expression: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
      cdt: fv!item,
      fields: ri!labelField,
      newValues: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_238805"(
        value: fv!item[ri!valueField],
        data: local!selectableReferenceData,
        valueField: ri!valueField,
        labelField: ri!labelField
      )
    )
  ),
  local!selectableReferenceDataLabelUpdate
)</definition>
