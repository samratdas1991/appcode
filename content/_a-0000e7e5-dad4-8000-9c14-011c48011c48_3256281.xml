        <uuid>_a-0000e7e5-dad4-8000-9c14-011c48011c48_3256281</uuid>
        <description>Rule to clone the tasks ref in a template</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!taskPrecedentsToClone),
  {},
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!taskPrecedentsToClone,
    expression: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
      cdt: fv!item,
      fields: {
        "templateTaskPrecedentId",
        "templateTaskId",
        "templateTaskUUIDPrecedent"
      },
      newValues: {
        null,
        null,
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
          cdt: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
            cdt: ri!clonedTaskMaps,
            field: "originalTask.templateTaskUUID",
            value: fv!item.templateTaskUUIDPrecedent
          ),
          field: "[1]newTask.templateTaskUUID"
        )
      }
    )
  )
)</definition>
        <namedTypedValue>
            <name>taskPrecedentsToClone</name>
            <type>
