    <rule>
        <name>AS_QNM_VD_evaluateResponseRequirement_SELECTION_EXCLUDES</name>
        <uuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_855705</uuid>
        <description>Used to evaluate response requirements of type EXCLUDES</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/* It is nonsensical to display a validation telling someone that a selection response must "exclude" something
especially when the value is unlikely to match the label,
so we do not bother with a return validation for this type of requirement.
This type of requirement should really only be used in question precedents */
not(
  #"_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_855594"(
    responseRequirement: ri!responseRequirement,
    responses: ri!responses
  )
)</definition>
        <namedTypedValue>
            <name>responses</name>
            <type>
                <name>AS_QNM_Response?list</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>responseRequirement</name>
            <type>
                <name>AS_QNM_ResponseRequirement</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
