            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
    ri!responseRequirement.requirementTypeCode
  ),
  error(
    concat(
      "Response requirement (ID: ",
      ri!responseRequirement.responseRequirementId,
      ") does not have a response requirement type code defined."
    )
  ),
  with(
    local!metadata: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
      cdt: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_597237"(),
      field: "requirementTypeCode",
      value: ri!responseRequirement.requirementTypeCode
    ),
    if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
        local!metadata
      ),
      error(
        concat(
          "Response requirement (ID: ",
          ri!responseRequirement.responseRequirementId,
          ") Code: ",
          ri!responseRequirement.requirementTypeCode,
          ") does not have metadata defined."
        )
      ),
      local!metadata[1]
    )
  )
)</definition>
        <namedTypedValue>
