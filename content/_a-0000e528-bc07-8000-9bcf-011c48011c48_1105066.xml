    <rule>
        <name>AS_IO_BL_invalidSectionsForAddProducts</name>
        <uuid>_a-0000e528-bc07-8000-9bcf-011c48011c48_1105066</uuid>
        <description>Rule to find out the invalid section names while adding the products</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76367"(
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!products,
    expression: if(
      or(
        #"_a-0000e528-bc07-8000-9bcf-011c48011c48_1039358"(product: fv!item),
        #"SYSTEM_SYSRULES_forEach"(
          items: #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_1021012"(product: fv!item),
          expression: not(
            #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(fv!item.isValid)
          )
        ),
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75483"(
          array: index(fv!item.accounts, "accountNumber", {}),
          skipNulls: true
        ),
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75483"(
          array: index(fv!item.accounts, "name", {}),
          skipNulls: true
        ),
        and(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(fv!item.accounts),
          contains(fv!item.accounts.name, null)
        )
      ),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: fv!item.name,
        replacementValue: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: "AS.CO.AllBundles.lbl_UnnamedProductPlaceholder"
        )
      ),
      null
    )
  )
)</definition>
        <namedTypedValue>
