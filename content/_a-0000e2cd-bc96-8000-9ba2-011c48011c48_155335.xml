            <users/>
            <groups/>
        </role>
    </roleMap>
    <typedValue>
        <type>
            <name>RuleTestConfig?list</name>
            <namespace>http://www.appian.com/ae/types/2009</namespace>
        </type>
        <value>
            <el>
                <a:name>Null Search Term</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xsd:string"/>
                    <a:nameRef>searchTerm</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nil="true"/>
                    <a:nameRef>fields</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
                    <a:expectedOutput>
                        <a:type a:uuid="{http://www.appian.com/ae/types/2009}Variant"/>
                        <a:runtimeType a:uuid="{http://www.appian.com/ae/types/2009}Variant"/>
                        <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">cast(
  'type!{http://www.appian.com/ae/types/2009}LogicalExpression',
  null
)</a:value>
                    </a:expectedOutput>
                </a:assertions>
            </el>
            <el>
                <a:name>Complex Search Term</a:name>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">"wild card"</a:value>
                    <a:nameRef>searchTerm</a:nameRef>
                    <a:id>1</a:id>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">{"field1", "field2"}</a:value>
                    <a:nameRef>fields</a:nameRef>
                    <a:id>2</a:id>
                </a:ruleInputTestConfigs>
                <a:assertions>
                    <a:expectedOutput>
                        <a:type a:uuid="{http://www.appian.com/ae/types/2009}Variant"/>
                        <a:runtimeType a:uuid="{http://www.appian.com/ae/types/2009}Variant"/>
                        <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">#"SYSTEM_SYSRULES_queryLogicalExpression"(
  operator: "AND",
  logicalExpressions: {
    #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "OR",
      filters: {
        #"SYSTEM_SYSRULES_queryFilter"(field: "field1", operator: "includes", value: "wild"),
        #"SYSTEM_SYSRULES_queryFilter"(field: "field2", operator: "includes", value: "wild"),
      }
    ),
    #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "OR",
      filters: {
        #"SYSTEM_SYSRULES_queryFilter"(field: "field1", operator: "includes", value: "card"),
        #"SYSTEM_SYSRULES_queryFilter"(field: "field2", operator: "includes", value: "card"),
      }
    )
  }
)</a:value>
                    </a:expectedOutput>
                </a:assertions>
            </el>
        </value>
