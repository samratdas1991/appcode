<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e62c-7375-8000-9bec-011c48011c48_1753602</versionUuid>
    <rule>
        <name>AS_CO_UT_createSearchLogicalExpression</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_155335</uuid>
        <description>Creates a wild-card search logical expression across multiple fields</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}LogicalExpression',
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!searchTerm),
    null,
    #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "AND",
      logicalExpressions: #"SYSTEM_SYSRULES_forEach"(
        items: split(trim(ri!searchTerm), " "),
        expression: a!localVariables(
          local!searchTerm: fv!item,
          #"SYSTEM_SYSRULES_queryLogicalExpression"(
            operator: "OR",
            ignoreFiltersWithEmptyValues: true,
            filters: #"SYSTEM_SYSRULES_forEach"(
              items: ri!fields,
              expression: #"SYSTEM_SYSRULES_queryFilter"(
                field: fv!item,
                operator: "includes",
                value: local!searchTerm
              )
            )
          )
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>searchTerm</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>fields</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
