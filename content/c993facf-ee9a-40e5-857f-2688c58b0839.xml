    <rule>
        <name>AS_IO_UT_createTaskActionAuditCdts</name>
        <uuid>c993facf-ee9a-40e5-857f-2688c58b0839</uuid>
        <description>Builds the audit CDTs for use in process</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{urn:com:appian:types:AS:IO}AS_IO_TaskActionAudit?list',
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!tasks,
    expression: {
      if(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!startTime),
        {},
        'type!{urn:com:appian:types:AS:IO}AS_IO_TaskActionAudit'(
          task: fv!item,
          review: fv!item.review,
          username: ri!initiator,
          timestamp: ri!startTime,
          taskAction: 'type!{urn:com:appian:types:AS:IO}AS_IO_R_TaskAction'(
            taskActionId: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_602167"
          ),
          assignedTo: null,
          
        )
      ),
      'type!{urn:com:appian:types:AS:IO}AS_IO_TaskActionAudit'(
        task: fv!item,
        review: fv!item.review,
        username: ri!initiator,
        timestamp: now(),
        taskAction: 'type!{urn:com:appian:types:AS:IO}AS_IO_R_TaskAction'(taskActionId: ri!taskActionId),
        assignedTo: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
          nullableValue: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73660"(user: fv!item.userAssignee),
          replacementValue: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73726"(groupOrUser: fv!item.groupAssignee)
        ),
        comment: if(
         count(ri!comments) = 1,
          ri!comments,
          index(ri!comments, fv!index, null)
        ),
        oldDueDate: ri!oldDueDate
      )
    }
  )
)</definition>
