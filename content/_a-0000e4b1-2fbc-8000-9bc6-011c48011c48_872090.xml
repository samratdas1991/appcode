    <rule>
        <name>AS_QNM_getPossibleActivePrecedentQuestions</name>
        <uuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_872090</uuid>
        <description>Returns all the possible active questions for an questionnnaire</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/* To be a precedent for question A, question B must...
be in the same category
have a selection based field
not already be in the follow up question chain of question A */
cast(
  'type!{urn:com:appian:types:AS:QNM}AS_QNM_T_Question?list',
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
    cdt: filter(
      #"_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_929670"(
        question: _
      ),
      #"_a-0000e40a-de32-8000-9ba8-011c48011c48_271202"(
        cdt: ri!allQuestions,
        field: "questionTemplateId",
        value: append(
          ri!question.questionTemplateId,
          #"_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_873728"(
            question: ri!question,
            allQuestions: ri!allQuestions
          ).questionTemplateId
        )
      )
    ),
    field: "questionCategoryTemplateId",
    value: ri!question.questionCategoryTemplateId
  )
)</definition>
        <namedTypedValue>
            <name>question</name>
            <type>
                <name>AS_QNM_T_Question</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>allQuestions</name>
            <type>
                <name>AS_QNM_T_Question?list</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
