  local!comments: a!flatten(
    #"SYSTEM_SYSRULES_forEach"(
      items: local!onboardingRequestDetails.requestId,
      expression: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157603"(
        type: 'type!{urn:com:appian:types:AS:IO:RC}AS_IO_RC_Comment?list',
