<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2676142</versionUuid>
    <rule>
        <name>AS_CO_I18N_UT_loadBundleFromMultiFolder</name>
        <uuid>_a-0000e673-a4a7-8000-9bef-011c48011c48_2065315</uuid>
        <description>Loads a property bundle file by name from multiple folders, returns a dictionary of the key-value pairs</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Dictionary',
  a!localVariables(
    local!locale: if(
      ri!localeAgnostic,
      null,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!localeOverride,
        replacementValue: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76004"(user: loggedInUser())
      )
    ),
    if(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!bundleFolders),
      null,
      a!localVariables(
        /*Wrapping this in a local variable to ensure caching -- it will only re-execute if inputs change*/
        local!bundleData: a!refreshVariable(
          value: loadbundlepropertiesfilemultifolder(
            ri!bundleFolders,
            local!locale,
            a!flatten(ri!bundleNames)
          ),
          refreshOnVarChange: { ri!triggerRefresh }
        ),
        local!bundleData
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>bundleFolders</name>
            <type>
                <name>CollaborationFolder?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>bundleNames</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>localeAgnostic</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>triggerRefresh</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>localeOverride</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
