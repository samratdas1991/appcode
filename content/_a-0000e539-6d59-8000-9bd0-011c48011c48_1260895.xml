    <rule>
        <name>AS_EI_NORTHROW_filterToNoteworthyAlerts</name>
        <uuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1260895</uuid>
        <description>Rule to check for any new Northrow alerts in the check summary.</description>
        <parentUuid>_a-0000e539-6d59-8000-9bd0-011c48011c48_1236798</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{urn:com:appian:types:AS:EI}AS_EI_NORTHROW_Alerts?list',
  a!localVariables(
    local!existingAlertsForCrids: #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1302590"(
      returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290163",
      crid: index(ri!alerts, "crid", {}),
      companyId: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
        cdt: ri!alerts,
        field: "companyId[1]",
        indexDefault: null
      ),
      executeWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!alerts)
    ),
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!alerts,
      expression: a!localVariables(
        local!previousAlertForCrid: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
          cdt: local!existingAlertsForCrids,
          field: "crid",
          value: fv!item.crid
        ),
        if(
          or(
            fv!item.score &lt;&gt; 0,
            and(
              #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(local!previousAlertForCrid),
              local!previousAlertForCrid.message &lt;&gt; fv!item.message
            )
          ),
          fv!item,
          {}
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>alerts</name>
            <type>
                <name>AS_EI_NORTHROW_Alerts?list</name>
                <namespace>urn:com:appian:types:AS:EI</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
