        <name>AS_FS_QE_getCustomerRelations</name>
        <uuid>_a-0000e50d-bf1d-8000-9bcd-011c48011c48_975253</uuid>
        <description>Queries AS_FS_CustomerRelations.  Use "customerId" to query for relations where the customer is either the owning or owned entity.</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
