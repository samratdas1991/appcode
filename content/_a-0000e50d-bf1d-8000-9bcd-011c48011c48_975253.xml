            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157870"(
  entity: #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_975246",
  entityType: 'type!{urn:com:appian:types:AS:FS}AS_FS_CustomerRelation',
  returnType: ri!returnType,
  executeWhen: ri!executeWhen,
  logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
    operator: "AND",
    ignoreFiltersWithEmptyValues: true,
    logicalExpressions: {
      #"SYSTEM_SYSRULES_queryLogicalExpression"(
        operator: "AND",
        ignoreFiltersWithEmptyValues: true,
        filters: {
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "customerRelationId",
            operator: "in",
            value: ri!customerRelationId
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "isActive",
            operator: "=",
            value: true
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "toCustomerId",
            operator: "in",
            value: ri!toCustomerId
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "fromCustomerId",
            operator: "in",
            value: ri!fromCustomerId
          )
        }
      ),
      /* Filters to all relations related to a customer */
      #"SYSTEM_SYSRULES_queryLogicalExpression"(
        operator: "OR",
        ignoreFiltersWithEmptyValues: true,
        filters: {
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "toCustomerId",
            operator: "in",
            value: ri!customerId
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "fromCustomerId",
            operator: "in",
            value: ri!customerId
          )
        }
      )
    }
  ),
  sort: ri!sort,
  pagingInfo: ri!pagingInfo,
  triggerRefresh: ri!triggerRefresh
)</definition>
