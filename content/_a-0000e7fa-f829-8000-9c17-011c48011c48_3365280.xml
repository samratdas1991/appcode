    <rule>
        <name>AS_IO_UI_modificationCommentForTemplateDocumentAudit</name>
        <uuid>_a-0000e7fa-f829-8000-9c17-011c48011c48_3365280</uuid>
        <description>Returns the nested comment for Template Document changes audit</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!getFieldName: wherecontains(
    "docTypeCode",
    ri!templateDocumentAuditEntry.simpleFieldChanges.fieldName
  ),
  local!getDocType: a!flatten(
    ri!templateDocumentAuditEntry.simpleFieldChanges[local!getFieldName].oldValue
  ),
  if(
    ri!templateDocumentAuditEntry.auditActionCode = #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_181584-adt",
    #"_a-0000e471-623a-8000-9bc3-011c48011c48_702641"(
      i18nData: ri!i18nData,
      bundleKey: "AS.CO.AllBundles.lbl_ActionPerformedTheTemplateDocumentName",
      richTextArguments: {
        #"_a-0000e40a-de32-8000-9ba8-011c48011c48_279925-adt"(
          actionCode: ri!templateDocumentAuditEntry.auditActionCode,
          displayCodesInColor: true
        ),
        #"SYSTEM_SYSRULES_richTextItem_v1"(text: ri!documentType, style: "STRONG")
      }
    ),
    {
      #"_a-0000e471-623a-8000-9bc3-011c48011c48_702641"(
        i18nData: ri!i18nData,
        bundleKey: "AS.CO.AllBundles.lbl_ActionPerformedTheTemplateDocumentName",
        richTextArguments: {
          #"_a-0000e40a-de32-8000-9ba8-011c48011c48_279925-adt"(
            actionCode: ri!templateDocumentAuditEntry.auditActionCode,
            displayCodesInColor: true
          ),
          #"SYSTEM_SYSRULES_richTextItem_v1"(text: ri!documentType, style: "STRONG")
        }
      ),
      if(
        ri!templateDocumentAuditEntry.auditActionCode = #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_181590-adt",
        #"SYSTEM_SYSRULES_richTextItem_v1"(
          text: #"_a-0000e41f-788a-8000-9ba9-011c48011c48_54484"(
            refData: ri!refData,
            refDataCodes: local!getDocType
          ).label,
          style: "STRONG"
        ),
        {}
      )
    }
  )
)</definition>
