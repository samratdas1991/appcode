        <definition>#"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157870"(
  entity: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_100868",
  logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
    operator: "AND",
    ignoreFiltersWithEmptyValues: true,
    logicalExpressions: {
      #"SYSTEM_SYSRULES_queryLogicalExpression"(
        operator: "AND",
        ignoreFiltersWithEmptyValues: true,
        filters: {
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "taskBehaviorType.behaviorTypeCode",
            operator: "=",
            value: ri!behaviorTypeCode
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "taskBehaviorType.behaviorSubtypeCode",
            operator: "=",
            value: ri!behaviorSubtypeCode
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "request.requestId",
            operator: "in",
            value: ri!requestId,
            applyWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!requestId)
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "taskId",
            operator: "in",
            value: ri!taskId
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "taskStatus.taskStatusId",
            operator: "in",
            value: ri!taskStatusIds
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "taskCategory.categoryName",
            operator: "in",
            value: ri!categoryName
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "request.customer.name",
            operator: "=",
            value: ri!customerName
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "request.customer.customerId",
            operator: "=",
            value: ri!customerId
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "request.type",
            operator: "in",
            value: ri!onboardingType
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "docUploadContext.docTypeCode",
            operator: "in",
            value: ri!docTypeCode
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "docUploadContext.onboardingDocumentId",
            operator: "=",
            value: ri!onboardingDocumentId
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "dueDate",
            operator: "&lt;=",
            value: ri!toDueDate
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "northrowCheckUuid",
            operator: "=",
            value: ri!northrowCheckUuid
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "requiredDocument.requiredOnboardingDocumentId",
            operator: "in",
            value: ri!requiredDocIds
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "rpaExecutionId",
            operator: "=",
            value: ri!rpaExecutionId
          ),
          /*Due soon condition*/
          if(
            ri!dueSoon,
            {
              #"SYSTEM_SYSRULES_queryFilter"(
                field: "dueDate",
                operator: "&gt;=",
                value: today()
              ),
              #"SYSTEM_SYSRULES_queryFilter"(
                field: "dueDate",
                operator: "&lt;",
                value: today() + 5
              ),
              /*open task status*/
              #"SYSTEM_SYSRULES_queryFilter"(
                field: "taskStatus.taskStatusId",
                operator: "in",
                value: {
                  #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236",
                  #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603230",
                  #"_a-0000e673-a4a7-8000-9bef-011c48011c48_1952244",
                  #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603242"
                }
              )
            },
            {}
          ),
          /*Overdue*/
          if(
            ri!overdue,
            {
              #"SYSTEM_SYSRULES_queryFilter"(
                field: "dueDate",
                operator: "&lt;",
                value: today()
              ),
              /*open task status*/
              #"SYSTEM_SYSRULES_queryFilter"(
                field: "taskStatus.taskStatusId",
                operator: "in",
                value: {
                  #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603236",
                  #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603230",
                  #"_a-0000e673-a4a7-8000-9bef-011c48011c48_1952244",
                  #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603242"
                }
              )
            },
            {}
          ),
          ri!additionalFilters
        }
      ),
      ri!additionalLogicalExpressions
    },
    
  ),
  pagingInfo: ri!pagingInfo,
  executeWhen: ri!executeWhen,
  entityType: 'type!{urn:com:appian:types:AS:IO}AS_IO_Task',
  returnType: ri!returnType,
  triggerRefresh: ri!triggerRefresh,
  sort: ri!sort,
  aggregation: ri!aggregation
)</definition>
