    <rule>
        <name>AS_CO_UT_displayRefDataLabelSingle</name>
        <uuid>_a-0000e438-fdf5-8000-9ba8-011c48011c48_21856</uuid>
        <description>Rule to get the display value for a single ref data</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>tostring(
  if(
    or(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!singleData),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!singleData[ri!labelField])
    ),
    null,
    a!localVariables(
      
      local!isInactive: not(#"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082"(index(ri!singleData, "isActive", null))),
      
      local!rawLabel: ri!singleData[ri!labelField],
      
      /*If the label includes a ".", then it is likely an internationalized label, */
      /*in which case we want to run it through AS_CO_I18N_UT_displayLabel*/
      local!isInternationalizedLabel: count(split(local!rawLabel, ".")) &gt; 1,
      
      local!displayLabel: if(
        local!isInternationalizedLabel,
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
          i18nData: ri!i18nData,
          bundleKey: ri!singleData[ri!labelField]
        ),
        local!rawLabel
      ),
      
      if(
        and(local!isInactive, ri!skipInactiveAppend &lt;&gt; true),
        #"_a-0000e447-933d-8000-9baa-011c48011c48_33160"(
          bundleKey: "AS.CO.CommonObjects.lbl_InactiveRefData",
          arguments: local!displayLabel
        ),
        local!displayLabel
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>singleData</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>labelField</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>skipInactiveAppend</name>
            <type>
                <name>boolean</name>
