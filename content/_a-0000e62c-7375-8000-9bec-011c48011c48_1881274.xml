            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!parsed: #"ca3dc866-cbf6-4df4-95a9-60a67847a13c"(
    versionConstant: #"_a-0000e62c-7375-8000-9bec-011c48011c48_1854026"
  ),
  local!version: "CO " &amp; joinarray(
    {
      local!parsed.solutionMajor,
      local!parsed.solutionMinor,
      local!parsed.solutionHotfix
    },
    "."
  ),
  /*will return the version text by default, but can be overruled*/
  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
    nullableValue: ri!input,
    replacementValue: local!version
  )
)</definition>
