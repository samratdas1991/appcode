        <uuid>_a-0000e586-af6f-8000-9bd2-011c48011c48_1349393</uuid>
        <description>Returns the number of onboarding task completed</description>
        <parentUuid>_a-0000e586-af6f-8000-9bd2-011c48011c48_1321233</parentUuid>
        <visibility>
