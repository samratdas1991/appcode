        <definition>tointeger(
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!requestId),
    "",
    sum(
      {
        #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_104148"(
          taskStatusIds: #"_a-0000e460-9d93-8000-9bc1-011c48011c48_603248",
          requestId: ri!requestId,
          customerName: ri!customerName,
          additionalFilters: {
            #"SYSTEM_SYSRULES_queryFilter"(
              field: "groupAssignee",
              operator: "=",
              value: ri!customerGroupId
            ),
            #"SYSTEM_SYSRULES_queryFilter"(
              field: "taskBehaviorType.behaviorTypeCode",
              operator: "&lt;&gt;",
              value: #"_a-0000e393-fbf1-8000-9ba2-011c48011c48_99629"
            )
          },
          returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290169",
          executeWhen: true,
          triggerRefresh: ri!triggerRefresh,
          aggregation: #"SYSTEM_SYSRULES_queryAggregation"(
            aggregationColumns: {
              #"SYSTEM_SYSRULES_queryAggregationColumn"(
                field: "taskStatus.taskStatusId",
                isGrouping: true
              ),
              #"SYSTEM_SYSRULES_queryAggregationColumn"(
                field: "taskId",
                alias: "count",
                aggregationFunction: "COUNT"
              )
            }
          )
        ).count
      }
    )
  )
)</definition>
