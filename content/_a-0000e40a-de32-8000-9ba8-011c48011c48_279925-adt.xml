    <rule>
        <name>AS_ADT_DSP_displayAuditActionCodeText</name>
        <uuid>_a-0000e40a-de32-8000-9ba8-011c48011c48_279925-adt</uuid>
        <description>Given an audit action code, creates a richtext item of the correct color and text to describe the action</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-adt</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!auditActionCodeDisplayDictionary: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_248820-adt"(),
  #"SYSTEM_SYSRULES_richTextItem_v1"(
    text: displayvalue(
      ri!actionCode,
      local!auditActionCodeDisplayDictionary.code,
      local!auditActionCodeDisplayDictionary.label,
      ""
    ),
    color: if(
      ri!displayCodesInColor,
      displayvalue(
        ri!actionCode,
        local!auditActionCodeDisplayDictionary.code,
        local!auditActionCodeDisplayDictionary.color,
        ""
      ),
      ""
    ),
    style: "STRONG"
  )
)</definition>
        <namedTypedValue>
            <name>actionCode</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>displayCodesInColor</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
