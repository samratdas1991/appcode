    <rule>
        <name>AS_CO_UT_cast</name>
        <uuid>_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157603</uuid>
        <description>Casts a variable to the type of either the input `type`, or the typeof of the input `typeof`, and properly handles for null inputs</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(  
  local!typeNumber: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157503"(
    type: ri!type,
    typeOf: ri!typeOf
  ),

  /* If the object is empty, use null or {} depending on if it is array type or not */
  local!objectToCast: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!value),
    ri!value,
    #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157548"(
      type: ri!type,
      typeOf: ri!typeOf
    ),
    null,
    {}
  ),

  cast(
    local!typeNumber,
    local!objectToCast
  )
  
)</definition>
