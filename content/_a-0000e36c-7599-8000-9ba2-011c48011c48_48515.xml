    <versionUuid>_a-0000e69f-3688-8000-9bf2-011c48011c48_2125471</versionUuid>
    <rule>
        <name>AS_IO_BL_displayDocumentStatusIconAndLabel</name>
        <uuid>_a-0000e36c-7599-8000-9ba2-011c48011c48_48515</uuid>
        <description>Displays the status of the document </description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!documentStatus: a!refreshVariable(
    value: #"_a-0000e41f-788a-8000-9ba9-011c48011c48_54484"(
      refData: ri!refData,
      refDataCodes: ri!statusCode
    ),
    refreshAlways: true
  ),
  local!documentStatusColor: if(
    ri!statusCode = #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_118222",
    "POSITIVE",
    if(
      ri!statusCode = #"_a-0000e36c-7599-8000-9ba2-011c48011c48_48067",
      "NEGATIVE",
      "SECONDARY"          
    )
  ),
  #"SYSTEM_SYSRULES_richTextItem_v1"(
    showWhen: and(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!statusCode),
      ri!showWhen &lt;&gt; false
    ),
    size: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
      nullableValue: ri!size,
      replacementValue: "STANDARD"
    ),
    text: {
      #"SYSTEM_SYSRULES_richTextIcon"(
        icon: local!documentStatus.icon,
        color: local!documentStatusColor
      ),
      #"SYSTEM_SYSRULES_richTextItem_v1"(
        text: concat(" ", upper(local!documentStatus.label)),
        showWhen: ri!includeLabel,
        color: local!documentStatusColor
      )
    }
  )
)</definition>
        <namedTypedValue>
            <name>statusCode</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>refData</name>
            <type>
                <name>AS_IO_R_Data?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>size</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>includeLabel</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>showWhen</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
