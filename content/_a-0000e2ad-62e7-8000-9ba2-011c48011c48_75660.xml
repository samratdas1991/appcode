        local!cdtFields
      ),
      ri!onlyComplexFields,
      reject(
        #"_a-0000e36c-7599-8000-9ba2-011c48011c48_64438"(
          singleType: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75842"(
            type: ri!type,
            typeOf: ri!typeOf
          ),
          field: _
