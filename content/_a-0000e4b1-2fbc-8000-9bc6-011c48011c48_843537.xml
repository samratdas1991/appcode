    <rule>
        <name>AS_CO_UT_findCircularDependency</name>
        <uuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_843537</uuid>
        <description>Used to find a circular dependency within a list of objects.  Supports using the index as the precedentValueField using AS_CO_ENUM_INDEX_AS_PRECEDENT_POINTER.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>or(
  index(
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!allCdts,
      expression: reduce(
        #"_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_843434"(
          map: _,
          index: _,
          allCdts: ri!allCdts, 
          isPrecedentIndex: true, 
          precedentPointerField: ri!precedentPointerField, 
          precedentValueField: ri!precedentValueField
        ),
        a!map(texts: {tostring(fv!index)}),
        enumerate(count(ri!allCdts)+1)
      )
    ),
    "isCircular",
    {}
  )
)</definition>
