    <rule>
        <name>AS_EI_NORTHROW_QE_getCompanyCheckRefreshInterval_WithDefaulting</name>
        <uuid>_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2368585</uuid>
        <description>Wrapper rule, giving users the option to 1) Return the unadulterated customer specific northrow refresh setting or 2) Return the customer specific setting with defaulting behavior applied</description>
        <parentUuid>_a-0000e5a0-53e9-8000-9bd8-011c48011c48_1403824</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!customerSetting: #"_a-0000e539-6d59-8000-9bd0-011c48011c48_1293113"(
    returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
    companyId: ri!customerId,
    triggerRefresh: ri!triggerRefresh,
    executeWhen: ri!executeWhen
  ),
  if(
    ri!executeWhen,
    a!localVariables(
      local!defaultCustomerSettingIfBlank: a!defaultValue(
        value: local!customerSetting,
        default: 'type!{urn:com:appian:types:AS:EI}AS_EI_NORTHROW_COMPANY_CHECK_REFRESH_INTERVAL'(
          companyId: ri!customerId,
          isActive: false,
          refreshIntervalDays: null
        )
      ),
      if(
        ri!applyDefaulting,
        #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
          cdt: local!defaultCustomerSettingIfBlank,
          fields: "refreshIntervalDays",
          newValues: a!defaultValue(
            value: local!defaultCustomerSettingIfBlank.refreshIntervalDays,
            default: #"_a-0000e5d6-d648-8000-9bde-011c48011c48_1501904"(
              triggerRefresh: ri!triggerRefresh,
              settingsCode: #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2368612"
            ).userSettingValue
          )
        ),
        local!defaultCustomerSettingIfBlank
      )
    ),
    local!customerSetting
  )
)</definition>
