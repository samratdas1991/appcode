    <rule>
        <name>AS_IO_BL_visibilityRuleForActivateOrDeactivateOnboardingTemplate</name>
        <uuid>_a-0000e7e5-dad4-8000-9c14-011c48011c48_3316375</uuid>
        <description>Visibility Rule For Activate Onboarding Template Action</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  ri!isForActivateAction,
  /*For Activate Template action - Template should be inactive.*/
  and(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(ri!isActive),
    /*Checks for other active templates with same region and type code*/
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(
      #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_155202"(
        returnType: #"_a-0000e415-3035-8000-9bb2-011c48011c48_290157",
        regionCode: ri!regionCode,
        onboardingTypeCode: ri!onboardingTypeCode,
        isActive: true(),
        additionalFilters: {
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "templateId",
            operator: "&lt;&gt;",
            value: ri!templateId
          )
        }
      )
    )
  ),
  /*For Deactivate Template action -Template should be active and not defaault*/
  and(
    ri!isActive,
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(ri!isDefault)
  )
)</definition>
