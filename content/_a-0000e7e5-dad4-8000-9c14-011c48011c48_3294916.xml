    <rule>
        <name>AS_IO_BL_mapRequiredDocsWithOnboardingDocsInUploadOnboardingDocuments</name>
        <uuid>_a-0000e7e5-dad4-8000-9c14-011c48011c48_3294916</uuid>
        <description>Rule to map requirements with onboarding docs based on appianDocId</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_forEach"(
  items: ri!requiredDocuments,
  expression: a!localVariables(
    local!onboardingDocuments: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
      cdt: ri!onboardingDocuments,
      field: "customerDocument.appianDocId",
      value: fv!item.onboardingDocument.customerDocument.appianDocId
    ),
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
      cdt: fv!item,
      fields: "onboardingDocument",
      newValues: if(
        #"_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_880586"({ local!onboardingDocuments }) = 1,
        local!onboardingDocuments,
        local!onboardingDocuments[fv!index]
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>requiredDocuments</name>
            <type>
                <name>AS_IO_RequiredOnboardingDocument?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>onboardingDocuments</name>
            <type>
                <name>AS_IO_OnboardingDocument?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
