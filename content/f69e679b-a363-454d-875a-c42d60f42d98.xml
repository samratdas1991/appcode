            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>touniformstring(
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!jsonText),
    {},
    a!localVariables(
      local!dictionary: #"SYSTEM_SYSRULES_fromJson_v1"(jsonText: ri!jsonText),
      local!allKeys: #"_a-0000e774-a792-8000-9c04-011c48011c48_2821463"(local!dictionary),
      #"SYSTEM_SYSRULES_forEach"(
        items: local!allKeys,
        expression: if(
          typeof(
            #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
              cdt: local!dictionary,
              field: fv!item,
              indexDefault: {}
            )
          ) = #"_a-0000e40a-de32-8000-9ba8-011c48011c48_283314"(),
          a!localVariables(
            local!item: fv!item,
            #"SYSTEM_SYSRULES_forEach"(
              items: #"f69e679b-a363-454d-875a-c42d60f42d98"(
                jsonText: #"SYSTEM_SYSRULES_toJson_v1"(
                  #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_76433"(
                    cdt: local!dictionary,
                    field: local!item,
                    indexDefault: {}
                  )
                )
              ),
              expression: local!item &amp; "." &amp; fv!item
            )
          ),
          fv!item
        )
      )
    )
  )
)</definition>
