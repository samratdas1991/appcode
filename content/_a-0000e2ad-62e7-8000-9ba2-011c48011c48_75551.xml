    <rule>
        <name>AS_CO_UT_convertDateTimeToDate</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75551</uuid>
        <description>Converts a date time to a date, keeping the date part the same regardless of the user's timezone</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
   'type!{http://www.appian.com/ae/types/2009}Date',
  if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!dateTime),
    null,
    date(
      useryear(ri!dateTime),
      usermonth(ri!dateTime),
      userday(ri!dateTime)
    )
  )
)</definition>
        <namedTypedValue>
            <name>dateTime</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
