    <rule>
        <name>AS_FS_UT_generateAccountName</name>
        <uuid>_a-0000e528-bc07-8000-9bcf-011c48011c48_1114750</uuid>
        <description>Generate account name in format [Type]-[Account Number]</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!account.type),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!account.accountNumber),
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!account.name)
  ),
  ri!account.name,
  joinarray(
    {
      displayvalue(
        ri!account.type,
        ri!accountTypes.code,
        ri!accountTypes.label,
        null
      ),
      ri!account.accountNumber
    },
    "-"
  )
)</definition>
        <namedTypedValue>
            <name>account</name>
            <type>
                <name>AS_FS_Account</name>
                <namespace>urn:com:appian:types:AS:FS</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>accountTypes</name>
            <type>
                <name>AS_FS_R_Data?list</name>
                <namespace>urn:com:appian:types:AS:FS</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
