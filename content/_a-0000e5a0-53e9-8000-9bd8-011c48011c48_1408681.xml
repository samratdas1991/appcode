<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e70f-2824-8000-9c01-011c48011c48_2632888</versionUuid>
    <interface>
        <name>AS_CO_LNK_displayTextWithShowMoreLink</name>
        <uuid>_a-0000e5a0-53e9-8000-9bd8-011c48011c48_1408681</uuid>
        <description>Rule to display text with show more link</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!showMore: false,
  local!i18nData: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81720"(
    bundleFolder: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_104931",
    bundleNames: "AS.CO.CommonObjects"
  ),
  #"SYSTEM_SYSRULES_richTextDisplayField"(
    showWhen: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!textToDisplay),
    labelPosition: "COLLAPSED",
    value: {
      #"SYSTEM_SYSRULES_richTextItem_v1"(
        size: ri!textSize,
        color: ri!textColor,
        text: if(
          local!showMore,
          ri!textToDisplay,
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_147294"(
            text: ri!textToDisplay,
            shortenToLen: ri!shortenToLen
          )
        )
      ),
      #"SYSTEM_SYSRULES_richTextItem_v1"(
        text: char(10) &amp; if(
          local!showMore,
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: local!i18nData,
            bundleKey: "AS.CO.CommonObjects.txt_ShowLess"
          ),
          #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
            i18nData: local!i18nData,
            bundleKey: "AS.CO.CommonObjects.txt_ShowMore"
          )
        ),
        link: #"SYSTEM_SYSRULES_dynamicLink"(
          value: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75636"(local!showMore),
          saveInto: local!showMore
        ),
        size: "SMALL",
        linkStyle: "STANDALONE",
        showWhen: len(ri!textToDisplay) &gt; ri!shortenToLen
      )
    }
  )
)</definition>
        <namedTypedValue>
            <name>textToDisplay</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>shortenToLen</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>textColor</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>textSize</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
        <isCustom>false</isCustom>
    </interface>
