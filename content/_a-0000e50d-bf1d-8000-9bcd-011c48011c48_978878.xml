    <rule>
        <name>AS_FS_QE_getStates</name>
        <uuid>_a-0000e50d-bf1d-8000-9bcd-011c48011c48_978878</uuid>
        <description>get state reference data
Columns - list of text column names to be returned by the query
StatesOnly - if this is true, will only return the states. If false or null the query will return states and territories
language - will return based on the ISO Language, if null will use the logged in user's locale or default to english_us</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!states: #"_a-0000e3e6-fb42-8000-9ba5-011c48011c48_157870"(
    entity: #"_a-0000e50d-bf1d-8000-9bcd-011c48011c48_978884",
    filters: {
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "isTerritory",
        value: false,
        operator: "=",
        applyWhen: ri!statesOnly = true
      ),
      #"SYSTEM_SYSRULES_queryFilter"(
        field: "stateCode",
        operator: "in",
        value: ri!stateCodes
      )
    },
    sort: #"SYSTEM_SYSRULES_sortInfo"(field: "stateName", ascending: true),
    executeWhen: ri!executeWhen,
    returnType: ri!returnType,
    entityType: 'type!{urn:com:appian:types:AS:FS}AS_FS_R_State'
  ),
  #"SYSTEM_SYSRULES_forEach"(
    items: local!states,
    expression: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268479"(
      cdt: fv!item,
      fields: "stateName",
      newValues: #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(
        i18nData: ri!i18nData,
        bundleKey: fv!item.stateName
      )
    )
  )
)</definition>
