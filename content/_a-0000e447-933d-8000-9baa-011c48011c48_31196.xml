    <rule>
        <name>AS_QNM_UT_filterToResponsesForQuestionByFieldKey</name>
        <uuid>_a-0000e447-933d-8000-9baa-011c48011c48_31196</uuid>
        <description>Given a question and a field key, filters to the responses for that key</description>
        <parentUuid>_a-0000e42d-ffef-8000-9ba6-011c48011c48_7213-qnm</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{urn:com:appian:types:AS:QNM}AS_QNM_Response?list',
  if(
    isnull(
      ri!question.responses
    ),
    {},
    index(
      ri!question.responses,
      wherecontains(
        ri!fieldKey,
        ri!question.responses.fieldKey
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>question</name>
            <type>
                <name>AS_QNM_Question</name>
                <namespace>urn:com:appian:types:AS:QNM</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fieldKey</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
