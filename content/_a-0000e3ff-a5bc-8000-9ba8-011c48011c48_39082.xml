    <versionUuid>_a-0000e447-933d-8000-9baa-011c48011c48_96119</versionUuid>
    <rule>
        <name>AS_CO_UT_booleanDefaultTrue</name>
        <uuid>_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39082</uuid>
        <description>Default a null value to true, otherwise returns the boolean value</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Boolean',
  if(
    isnull(ri!boolean),
    true,
    ri!boolean
  )
)</definition>
        <namedTypedValue>
            <name>boolean</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
