    <rule>
        <name>AS_IO_UT_saveIntosOnSelectingDocUploadOption</name>
        <uuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_3044918</uuid>
        <description>List of saves</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  ri!selectedUploadField = #"_a-0000e79b-0335-8000-9c0c-011c48011c48_3043381",
  {
    /*NOW*/
    a!save(
      ri!docUploadTask.groupAssignee,
      #"_a-0000e438-fd8a-8000-9bbe-011c48011c48_411694"
    ),
    a!save(ri!docUploadTask.dueDate, today()),
    a!save(
      ri!docUploadTask.completedBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(
      ri!docUploadTask.completedDatetime,
      now()
    ),
    a!save(
      ri!docUploadTask.createdBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(ri!docUploadTask.createdDatetime, now()),
    a!save(
      ri!docUploadTask.modifiedBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(
      ri!docUploadTask.modifiedDatetime,
      now()
    ),
    /*Review Upload Task*/
    a!save(
      ri!reviewTask.groupAssignee,
      #"_a-0000e438-fd8a-8000-9bbe-011c48011c48_411694"
    ),
    a!save(
      /*7 days from now for review task for attach now*/
      ri!reviewTask.dueDate,
      if(
        ri!dueDateCalculationUnit = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620378",
        workday(today(), 7),
        today() + 7
      )
    ),
    a!save(
      ri!reviewTask.createdBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(ri!reviewTask.createdDatetime, now()),
    a!save(
      ri!reviewTask.modifiedBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(ri!reviewTask.modifiedDatetime, now())
  },
  {
    /*ATTACH LATER*/
    a!save(ri!docUploadTask.groupAssignee, null),
    a!save(ri!docUploadTask.dueDate, ri!dueDate),
    a!save(
      ri!docUploadTask.createdBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(ri!docUploadTask.createdDatetime, now()),
    a!save(
      ri!docUploadTask.modifiedBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(
      ri!docUploadTask.modifiedDatetime,
      now()
    ),
    /*Review Upload Task*/
    a!save(ri!reviewTask.groupAssignee, null),
    a!save(
      ri!reviewTask.dueDate,
      if(
        ri!dueDateCalculationUnit = #"_a-0000e460-9d93-8000-9bc1-011c48011c48_620378",
        workday(ri!docUploadTask.dueDate, 7),
        /*default is calendar days*/
        ri!docUploadTask.dueDate + 7
      )
    ),
    a!save(
      ri!reviewTask.createdBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(ri!reviewTask.createdDatetime, now()),
    a!save(
      ri!reviewTask.modifiedBy,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73366"
    ),
    a!save(ri!reviewTask.modifiedDatetime, now())
  }
)</definition>
