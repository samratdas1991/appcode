    <rule>
        <name>AS_CO_UT_updateCdtFields</name>
        <uuid>_a-0000e40a-de32-8000-9ba8-011c48011c48_260165</uuid>
        <description>Updates one or more fields of a CDT, with an optional input for indices</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!cdt),
  if(
    and(
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!cdt),
      #"_a-0000e415-3035-8000-9bb2-011c48011c48_291952"(
        typeOf: ri!cdt
      )
    ),
    ri!cdt,
    reduce(
      #"_a-0000e40a-de32-8000-9ba8-011c48011c48_268798"(
        cdt: _,
        indices: ri!indices,
        field: _,
        newValue: _
      ),
      ri!cdt,
      merge(
        a!flatten(ri!fields),
        a!flatten(ri!newValues)
      )
    )
  )
)</definition>
