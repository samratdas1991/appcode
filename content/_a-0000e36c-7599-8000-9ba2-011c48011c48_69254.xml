<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_823201</versionUuid>
    <rule>
        <name>AS_IO_UT_checkIfFlowGoesToProcessSetupTask</name>
        <uuid>_a-0000e36c-7599-8000-9ba2-011c48011c48_69254</uuid>
        <description>Rule to check if the create onboarding process flow goes to the process setup task</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  /*If this is a Web API, do not go to process setup task*/
  ri!isWebApi,
  false,
  
  /*Otherwise, only go to the process setup task if BOTH the constant is set to true AND the user has access to the request*/
  and(
    #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_33214", 
    #"_a-0000e36c-7599-8000-9ba2-011c48011c48_69207"(requestId: ri!requestId)
  ),
  true,
  
  /*Otherwise, do not go to process setup task*/
  false
)</definition>
        <namedTypedValue>
            <name>isWebApi</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>requestId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
