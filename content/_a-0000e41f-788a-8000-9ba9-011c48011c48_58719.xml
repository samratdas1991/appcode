        <uuid>_a-0000e41f-788a-8000-9ba9-011c48011c48_58719</uuid>
        <description>Update customer document attributes</description>
        <parentUuid>_a-0000e4b1-2fbc-8000-9bc6-011c48011c48_968428</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
