        <definition>#"SYSTEM_SYSRULES_forEach"(
  items: ri!customerDocuments,
  expression: #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
    cdt: fv!item,
    fields: {
      "customerId",
      "documentName",
      "fileType",
      "folderId"
    },
    newValues: {
      ri!customerId,
      document(
        fv!item.appianDocId,
        "name"
      ),
      document(
        fv!item.appianDocId,
        "extension"
      ),
      #"_a-0000e62c-7375-8000-9bec-011c48011c48_1835064"(
        customerDocument: fv!item,
        customerFolder: ri!customerFolderId
      )
    }
  )
)</definition>
