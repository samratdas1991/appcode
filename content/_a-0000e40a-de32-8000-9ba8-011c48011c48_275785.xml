    <rule>
        <name>AS_CO_UT_joinListsByField</name>
        <uuid>_a-0000e40a-de32-8000-9ba8-011c48011c48_275785</uuid>
        <description>Iterates over each item in list one. If a match on the input field is found in list two, it replaces the item in list one with the match from list two. Otherwise it retains the item in list one.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  typeof(ri!listOne),
  if(
    or(#"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!listOne), #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(ri!listTwo)),
    ri!listOne,
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!listOne,
      expression: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: index(
          #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75800"(
            cdt: ri!listTwo,
            field: ri!field,
            value: fv!item[ri!field]
          ),
          1,
          null
        ),
        replacementValue: fv!item
      )
    )
  )
)</definition>
