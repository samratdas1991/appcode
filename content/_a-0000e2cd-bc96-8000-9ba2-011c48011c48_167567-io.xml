    <versionUuid>_a-0000e5a0-53e9-8000-9bd8-011c48011c48_1455913</versionUuid>
    <rule>
        <name>AS_GM_getGroupListForIndirectUsers</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_167567-io</uuid>
        <description>Returns the group list for indirect users</description>
        <parentUuid>_a-0000e471-623a-8000-9bc3-011c48011c48_750565</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  runtimetypeof(
    ri!user
  ) = #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_166426-io",
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!groupList,
    expression: if(
      contains(
        togroup(
          a!groupMembers(
            group: fv!item,
            direct: true,
            memberType: "GROUP",
            pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(
              startIndex: 1,
              /*current max batchSize for this function*/
              batchSize: #"_a-0000e447-933d-8000-9baa-011c48011c48_43371"
            )
          ).data
        ),
        togroup(
          ri!user
        )
      ),
      fv!item,
      {}
    )
  ),
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!groupList,
    expression: if(
      contains(
        touser(
          a!groupMembers(
            group: fv!item,
            direct: true,
            memberType: "USER",
            pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(
              startIndex: 1,
              /*current max batchSize for this function*/
              batchSize: #"_a-0000e447-933d-8000-9baa-011c48011c48_43371"
            )
          ).data
        ),
        touser(
          ri!user
        )
      ),
      fv!item,
      {}
    )
  )
)</definition>
        <namedTypedValue>
            <name>groupList</name>
            <type>
                <name>Group?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>user</name>
            <type>
                <name>UserOrGroup</name>
