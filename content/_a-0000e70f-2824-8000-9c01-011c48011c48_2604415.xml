        <definition>a!localVariables(
  local!updateRule: a!refreshVariable(
    value: if(
      typeof(ri!field) = #"_a-0000e40a-de32-8000-9ba8-011c48011c48_279359"(),
      #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165",
      #"_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2375385"
    ),
    refreshAlways: true
  ),
  apply(
    local!updateRule(cdt: _, fields: ri!field, newValues: _),
    merge(
      a!flatten(ri!objects),
      apply(
        #"_a-0000e2cd-bc96-8000-9ba2-011c48011c48_81818"(i18nData: ri!i18nData, bundleKey: _),
        a!flatten(index(ri!objects, ri!field, {}))
      )
    )
  )
)</definition>
