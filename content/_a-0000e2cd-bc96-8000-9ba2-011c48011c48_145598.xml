    <versionUuid>_a-0000e6e2-62e5-8000-9bfe-011c48011c48_2446742</versionUuid>
    <rule>
        <name>AS_CO_UI_createDocumentIcon</name>
        <uuid>_a-0000e2cd-bc96-8000-9ba2-011c48011c48_145598</uuid>
        <description>Returns a rich text icon for a document</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!fileExtension: tostring(
    index(
      split(ri!extension, "."),
      count(split(ri!extension, ".")),
      null
    )
  ),
  local!isExcel: any(
    search(_, local!fileExtension),
    { "xls", "csv" }
  ),
  local!isWord: any(search(_, local!fileExtension), { "doc" }),
  local!isPowerpoint: any(search(_, local!fileExtension), { "ppt" }),
  local!isPdf: any(search(_, local!fileExtension), { "pdf" }),
  local!isZip: any(
    search(_, local!fileExtension),
    { "zip", "rar", "rar4" }
  ),
  local!isImage: any(
    search(_, local!fileExtension),
    { "jpg", "jpeg", "png", "gif" }
  ),
  local!isCode: any(
    search(_, local!fileExtension),
    {
      "xml",
      "html",
      "java",
      "xsd",
      "sql",
      "css",
      "js"
    }
  ),
  local!fileIcon: if(
    local!isExcel,
    "file-excel-o",
    local!isWord,
    "file-word-o",
    local!isPowerpoint,
    "file-powerpoint-o",
    local!isPdf,
    "file-pdf-o",
    local!isZip,
    "file-zip-o",
    local!isImage,
    "file-picture-o",
    local!isCode,
    "file-code-o",
    "file-text-o"
  ),
  if(
    #"_a-0000e3ff-a5bc-8000-9ba8-011c48011c48_39070"(ri!createStamp),
    #"SYSTEM_SYSRULES_stampField"(
      labelPosition: "COLLAPSED",
      icon: local!fileIcon,
      backgroundColor: ri!backgroundColor,
      contentColor: ri!color,
      size: ri!size,
      tooltip: ri!caption
    ),
    #"SYSTEM_SYSRULES_richTextIcon"(
      icon: local!fileIcon,
      color: ri!color,
      size: ri!size,
      link: ri!link,
      linkStyle: "STANDALONE",
      caption: ri!caption
    )
  )
)</definition>
