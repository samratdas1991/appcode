    <versionUuid>_a-0000e7c7-bc72-8000-9c0e-011c48011c48_3074490</versionUuid>
    <rule>
        <name>AS_IO_UT_constructOnboardingDocDetailsForDocRequirement</name>
        <uuid>_a-0000e79b-0335-8000-9c0c-011c48011c48_3044856</uuid>
        <description>Rule to construct  onboarding document details for document requirement</description>
        <parentUuid>_a-0000e2bb-4188-8000-9ba2-011c48011c48_25469</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/*Requirements with attach now are updated here or requirements with documents should be updated with respective fields*/
#"SYSTEM_SYSRULES_forEach"(
  items: ri!documentRequirements,
  expression: if(
    #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73536"(fv!item.onboardingDocument),
    fv!item,
    #"_a-0000e40a-de32-8000-9ba8-011c48011c48_260165"(
      cdt: fv!item,
      fields: {
        "onboardingDocument.requestId",
        "onboardingDocument.documentStatusCode",
        "onboardingDocument.isDeleted",
        "onboardingDocument.customerDocument.customerId",
        "onboardingDocument.customerDocument.docTypeCode",
        "onboardingDocument.customerDocument.isDeleted",
        "onboardingDocument.customerDocument.isInternal",
        "onboardingDocument.customerDocument.createdBy",
        "onboardingDocument.customerDocument.createdDatetime",
        "onboardingDocument.customerDocument.modifiedBy",
        "onboardingDocument.customerDocument.modifiedDatetime"
      },
      newValues: {
        ri!onboardingRequest.requestId,
        #"_a-0000e70f-2824-8000-9c01-011c48011c48_2774613",
        false,
        ri!onboardingRequest.customer.customerId,
        fv!item.docTypeCode,
        false,
        /*    Is Internal should be false if not true    */
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
          nullableValue: fv!item.onboardingDocument.customerDocument.isInternal,
          replacementValue: false
        ),
        loggedInUser(),
        now(),
        loggedInUser(),
        now()
      }
    )
  )
)</definition>
        <namedTypedValue>
            <name>documentRequirements</name>
            <type>
                <name>AS_IO_RequiredOnboardingDocument?list</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>onboardingRequest</name>
            <type>
                <name>AS_IO_OnboardingRequest</name>
                <namespace>urn:com:appian:types:AS:IO</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
