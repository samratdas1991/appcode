    <rule>
        <name>AS_CO_VD_textValidation</name>
        <uuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_74104</uuid>
        <description>Performs minimum and maximum length validation on a text value. Returns a validation message if any provided length constraints are violated. Allows for custom validation messages.</description>
        <parentUuid>_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73105</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{http://www.appian.com/ae/types/2009}Text',
  a!localVariables(
    local!length: len(ri!value),
    local!minLength: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
      nullableValue: ri!minLength,
      replacementValue: 0
    ),
    if(
      /* Does not meet exact length requirement */
      and(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!minLength),
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!maxLength),
        ri!minLength = ri!maxLength,
        local!length &lt;&gt; ri!minLength
      ),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
          nullableValue: ri!maxLengthMessage,
          replacementValue: ri!minLengthMessage
        ),
        replacementValue: #"_a-0000e447-933d-8000-9baa-011c48011c48_33160"(
          bundleKey: if(
            ri!minLength = 1,
            "AS.CO.CommonObjects.vld_EnterExactlySingle",
            "AS.CO.CommonObjects.vld_EnterExactlyMultiple"
          ),
          arguments: ri!minLength
        )
      ),
      /*Too short*/
      local!length &lt; ri!minLength,
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!minLengthMessage,
        replacementValue: #"_a-0000e447-933d-8000-9baa-011c48011c48_33160"(
          bundleKey: "AS.CO.CommonObjects.vld_EnterAtLeast",
          arguments: local!minLength
        )
      ),
      /*Too long*/
      and(
        #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_75782"(ri!maxLength),
        local!length &gt; ri!maxLength
      ),
      #"_a-0000e2ad-62e7-8000-9ba2-011c48011c48_73980"(
        nullableValue: ri!maxLengthMessage,
        replacementValue: #"_a-0000e447-933d-8000-9baa-011c48011c48_33160"(
          bundleKey: if(
            ri!maxLength = 1,
            "AS.CO.CommonObjects.vld_EnterNoMoreThanSingle",
            "AS.CO.CommonObjects.vld_EnterNoMoreThanMultiple"
          ),
          arguments: ri!maxLength
        )
      ),
      /*Valid*/
      ""
    )
  )
)</definition>
